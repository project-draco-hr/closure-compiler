{
  Preconditions.checkState(isInterface);
  Property p=protoProps.get(pname);
  if (p != null) {
    return p;
  }
  if (interfaces != null) {
    for (    NominalType interf : interfaces) {
      p=interf.getPropFromInterface(pname);
      if (p != null) {
        return p;
      }
    }
  }
  return null;
}
