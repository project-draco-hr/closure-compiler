{
  Preconditions.checkState(!isFinalized);
  if (type == null && isConstant) {
    type=JSType.UNKNOWN;
  }
  classProps=classProps.with(pname,isConstant ? Property.makeConstant(defSite,type,type) : Property.makeWithDefsite(defSite,type,type));
  if (protoProps.containsKey(pname)) {
    addProtoProperty(pname,defSite,type,isConstant);
  }
}
