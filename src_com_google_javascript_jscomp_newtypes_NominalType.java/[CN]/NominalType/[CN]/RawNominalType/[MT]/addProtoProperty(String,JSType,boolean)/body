{
  Preconditions.checkState(!isFinalized);
  if (type == null && isConstant) {
    type=JSType.UNKNOWN;
  }
  if (classProps.containsKey(pname) && classProps.get(pname).getDeclaredType() == null) {
    classProps=classProps.without(pname);
  }
  protoProps=protoProps.with(pname,isConstant ? Property.makeConstant(type,type) : Property.make(type,type));
}
