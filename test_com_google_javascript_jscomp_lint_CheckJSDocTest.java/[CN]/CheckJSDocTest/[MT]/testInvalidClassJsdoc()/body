{
  this.setAcceptedLanguage(LanguageMode.ECMASCRIPT6_STRICT);
  testSame(Joiner.on('\n').join("class Foo {","  /** @param {number} x */","  constructor(x) {}","}"));
  testSame(Joiner.on('\n').join("class Foo {","  /** @constructor */","  constructor() {}","}"),DISALLOWED_MEMBER_JSDOC);
  testSame(Joiner.on('\n').join("class Foo {","  /** @interface */","  constructor() {}","}"),DISALLOWED_MEMBER_JSDOC);
  testSame(Joiner.on('\n').join("class Foo {","  /** @extends {Foo} */","  constructor() {}","}"),DISALLOWED_MEMBER_JSDOC);
  testSame(Joiner.on('\n').join("class Foo {","  /** @implements {Foo} */","  constructor() {}","}"),DISALLOWED_MEMBER_JSDOC);
}
