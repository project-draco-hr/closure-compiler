{
  testSame(LINE_JOINER.join("/**"," * @param {number=} n"," */","function f(n) {}"),OPTIONAL_TYPE_NOT_USING_OPTIONAL_NAME);
  testSame(LINE_JOINER.join("/**"," * @param {number} opt_n"," */","function f(opt_n) {}"),OPTIONAL_NAME_NOT_MARKED_OPTIONAL);
  testSame(LINE_JOINER.join("/**"," * @param {number=} opt_n"," */","function f(opt_n) {}"));
}
