{
switch (n.getType()) {
case Token.ASSIGN:
case Token.VAR:
    maybeAddConstructor(t,n);
  break;
case Token.FUNCTION:
if (NodeUtil.isStatement(n)) {
  maybeAddConstructor(t,n);
}
break;
case Token.CALL:
visitCallNode(n,parent);
break;
case Token.SCRIPT:
visitScriptNode(t);
break;
case Token.NEW:
visitNewNode(t,n);
}
}
