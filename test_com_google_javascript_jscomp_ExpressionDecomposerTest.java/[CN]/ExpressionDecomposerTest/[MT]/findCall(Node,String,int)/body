{
class Find {
    int found=0;
    Node find(    Node n){
      if (n.isCall()) {
        Node callee=n.getFirstChild();
        if (name == null || (callee.isName() && callee.getString().equals(name))) {
          found++;
          if (found == call) {
            return n;
          }
        }
      }
      for (      Node c : n.children()) {
        Node result=find(c);
        if (result != null) {
          return result;
        }
      }
      return null;
    }
  }
  return (new Find()).find(root);
}
