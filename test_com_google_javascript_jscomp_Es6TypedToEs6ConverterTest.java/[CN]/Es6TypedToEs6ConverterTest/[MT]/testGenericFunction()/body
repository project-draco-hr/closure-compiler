{
  test("function foo<T>() {}","/** @template T */ function foo() {}");
  test("var x = <K, V>(p) => 3;","var x = /** @template K, V */ (p) => 3");
  test("class Foo { f<T>() {} }","class Foo { /** @template T */ f() {} }");
  test("(function<T>() {})();","(/** @template T */ function() {})();");
  test("function* foo<T>() {}","/** @template T */ function* foo() {}");
}
