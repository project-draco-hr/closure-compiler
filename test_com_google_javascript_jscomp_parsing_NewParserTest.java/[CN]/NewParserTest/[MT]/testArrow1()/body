{
  mode=LanguageMode.ECMASCRIPT6;
  parse("()=>1;");
  parse("()=>{}");
  parse("(a,b) => a + b;");
  parse("a => b");
  parse("a => { return b }");
  parse("a => b");
  parse("var x = (a => b);");
  mode=LanguageMode.ECMASCRIPT5;
  parseWarning("a => b","this language feature is only supported in es6 mode: " + "short function syntax");
  mode=LanguageMode.ECMASCRIPT3;
  parseWarning("a => b;","this language feature is only supported in es6 mode: " + "short function syntax");
}
