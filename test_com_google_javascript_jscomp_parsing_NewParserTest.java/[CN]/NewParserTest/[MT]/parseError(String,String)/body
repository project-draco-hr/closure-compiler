{
  TestErrorReporter testErrorReporter=new TestErrorReporter(errors,null);
  ParseResult result=ParserRunner.parse(new SimpleSourceFile("input",false),source,ParserRunner.createConfig(isIdeMode,mode,false,null),testErrorReporter);
  Node script=result.ast;
  assertTrue(testErrorReporter.hasEncounteredAllErrors());
  assertTrue(testErrorReporter.hasEncounteredAllWarnings());
  return script;
}
