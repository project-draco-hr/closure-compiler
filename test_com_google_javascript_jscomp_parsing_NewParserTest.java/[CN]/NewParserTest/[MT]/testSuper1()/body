{
  mode=LanguageMode.ECMASCRIPT6;
  parse("super;");
  parse("function f() {super;};");
  mode=LanguageMode.ECMASCRIPT5;
  parse("super;","this language feature is only supported in es6 mode: super");
  mode=LanguageMode.ECMASCRIPT3;
  parse("super;","this language feature is only supported in es6 mode: super");
}
