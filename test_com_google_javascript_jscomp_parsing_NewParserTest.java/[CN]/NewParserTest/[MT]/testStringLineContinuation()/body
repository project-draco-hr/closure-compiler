{
  mode=LanguageMode.ECMASCRIPT3;
  Node n=parseError("'one\\\ntwo';","String continuations are not supported in this language mode.");
  assertEquals("onetwo",n.getFirstChild().getFirstChild().getString());
  mode=LanguageMode.ECMASCRIPT5;
  parseWarning("'one\\\ntwo';","String continuations are not recommended. See" + " https://google-styleguide.googlecode.com/svn/trunk/javascriptguide.xml#Multiline_string_literals");
  assertEquals("onetwo",n.getFirstChild().getFirstChild().getString());
  mode=LanguageMode.ECMASCRIPT6;
  parseWarning("'one\\\ntwo';","String continuations are not recommended. See" + " https://google-styleguide.googlecode.com/svn/trunk/javascriptguide.xml#Multiline_string_literals");
  assertEquals("onetwo",n.getFirstChild().getFirstChild().getString());
}
