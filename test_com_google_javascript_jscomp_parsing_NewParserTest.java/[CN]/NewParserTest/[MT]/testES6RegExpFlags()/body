{
  mode=LanguageMode.ECMASCRIPT6;
  parse("/a/y");
  parse("/a/u");
  mode=LanguageMode.ECMASCRIPT5;
  parseWarning("/a/y","this language feature is only supported in es6 mode: new RegExp flag 'y'");
  parseWarning("/a/u","this language feature is only supported in es6 mode: new RegExp flag 'u'");
  parseWarning("/a/yu","this language feature is only supported in es6 mode: new RegExp flag 'y'","this language feature is only supported in es6 mode: new RegExp flag 'u'");
}
