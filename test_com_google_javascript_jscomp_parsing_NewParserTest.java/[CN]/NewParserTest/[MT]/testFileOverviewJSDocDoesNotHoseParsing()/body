{
  assertThat(parse("/** @fileoverview Hi mom! \n */ function Foo() {}").getFirstChild().getType()).isEqualTo(Token.FUNCTION);
  assertThat(parse("/** @fileoverview Hi mom! \n * * * */ function Foo() {}").getFirstChild().getType()).isEqualTo(Token.FUNCTION);
  assertThat(parse("/** @fileoverview \n * x */ function Foo() {}").getFirstChild().getType()).isEqualTo(Token.FUNCTION);
  assertThat(parse("/** @fileoverview \n * x \n */ function Foo() {}").getFirstChild().getType()).isEqualTo(Token.FUNCTION);
}
