{
  parse("/a/");
  parse("/a/i");
  parse("/a/g");
  parse("/a/m");
  parse("/a/ig");
  parse("/a/gm");
  parse("/a/mgi");
  parseError("/a/a","Invalid RegExp flag 'a'");
  parseError("/a/b","Invalid RegExp flag 'b'");
  parseError("/a/abc","Invalid RegExp flag 'a'","Invalid RegExp flag 'b'","Invalid RegExp flag 'c'");
}
