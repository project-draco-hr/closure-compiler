{
  testTemplateLiteral("``");
  testTemplateLiteral("`\"`");
  testTemplateLiteral("`\\\"`");
  testTemplateLiteral("`\\``");
  testTemplateLiteral("`hello world`;");
  testTemplateLiteral("`hello\nworld`;");
  testTemplateLiteral("`string containing \\`escaped\\` backticks`;");
  testTemplateLiteral("{ `in block` }");
  testTemplateLiteral("{ `in ${block}` }");
}
