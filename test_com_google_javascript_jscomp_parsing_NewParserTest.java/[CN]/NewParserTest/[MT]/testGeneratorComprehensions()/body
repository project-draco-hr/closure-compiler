{
  mode=LanguageMode.ECMASCRIPT6;
  parse("(for (x of y) z);");
  parse("(for ({x,y} of z) x+y);");
  parse("(for (x of y) if (x<10) z);");
  parseError("(for (a = 5 of v) a);","'identifier' expected");
  mode=LanguageMode.ECMASCRIPT5;
  parseWarning("(for (x of y) z);","this language feature is only supported in es6 mode:" + " array/generator comprehensions");
}
