{
  mode=LanguageMode.ECMASCRIPT6;
  parseError("({x, y}) = foo();","invalid assignment target");
  expectedFeatures=FeatureSet.ES6;
  parse("({x, y} = foo());");
  parse("({x: x, y: y} = foo());");
  parse("({x: {y, z}} = foo());");
  parse("({k1: {k2 : x} = bar(), k3: y} = foo());");
  parse("({} = foo());");
}
