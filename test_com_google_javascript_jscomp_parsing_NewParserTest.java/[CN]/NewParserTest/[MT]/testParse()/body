{
  mode=LanguageMode.ECMASCRIPT5;
  Node a=Node.newString(Token.NAME,"a");
  a.addChildToFront(Node.newString(Token.NAME,"b"));
  List<ParserResult> testCases=ImmutableList.of(new ParserResult("3;",createScript(new Node(Token.EXPR_RESULT,Node.newNumber(3.0)))),new ParserResult("var a = b;",createScript(new Node(Token.VAR,a))),new ParserResult("\"hell\\\no\\ world\\\n\\\n!\"",createScript(new Node(Token.EXPR_RESULT,Node.newString(Token.STRING,"hello world!")))));
  for (  ParserResult testCase : testCases) {
    assertNodeEquality(testCase.node,parse(testCase.code));
  }
}
