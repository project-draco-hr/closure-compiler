{
  mode=LanguageMode.ECMASCRIPT6;
  parse("({x = 1}) = foo();");
  parse("({x = 1} = foo());");
  parse("({x: {y = 1}}) = foo();");
  parse("({x: {y = 1}} = foo());");
  parse("({x: y = 1}) = foo();");
  parse("({x: y = 1} = foo());");
  parse("({x: v1 = 5, y: v2 = 'str'}) = foo();");
  parse("({x: v1 = 5, y: v2 = 'str'} = foo());");
  parse("({k1: {k2 : x} = bar(), k3: y}) = foo();");
  parse("({k1: {k2 : x} = bar(), k3: y} = foo());");
}
