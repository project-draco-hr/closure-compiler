{
  mode=LanguageMode.ECMASCRIPT6;
  parseError("var {foo: bar.x} = baz();","'}' expected");
  parse("({foo: bar.x} = baz());");
  parse("for ({foo: bar.x} in baz());");
  parseError("var {foo: bar[x]} = baz();","'}' expected");
  parse("({foo: bar[x]} = baz());");
  parse("for ({foo: bar[x]} in baz());");
}
