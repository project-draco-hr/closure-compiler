{
  test(LINE_JOINER.join("/** @constructor */","function Example() {}","Example.staticMethod = function() { alert(1); }","","/** @constructor @extends {Example} */","function Subclass() {}","$jscomp.copyProperties(Subclass, Example);"),LINE_JOINER.join("/** @constructor */","function Example() {}","Example.staticMethod = function() { alert(1); }","","/** @constructor @extends {Example} */","function Subclass() {}","Subclass.staticMethod = Example.staticMethod;"));
}
