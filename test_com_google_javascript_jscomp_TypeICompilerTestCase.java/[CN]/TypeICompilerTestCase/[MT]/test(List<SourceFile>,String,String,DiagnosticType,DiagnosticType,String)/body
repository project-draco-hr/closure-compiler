{
  if (this.mode.runsOTI()) {
    enableTypeCheck();
    super.test(externs,js,expected,error,warning,description);
    disableTypeCheck();
  }
  if (this.mode.runsNTI()) {
    enableNewTypeInference();
    super.test(externs,js,expected,error,warning,description);
    disableNewTypeInference();
  }
}
