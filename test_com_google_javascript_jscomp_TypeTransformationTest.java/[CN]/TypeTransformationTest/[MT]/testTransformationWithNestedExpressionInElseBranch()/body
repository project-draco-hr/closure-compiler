{
  Node nested=buildConditional(buildEqtypePredicate(buildTypeVariable("T"),buildTypeVariable("R")),buildTypePredicate("string"),buildTypePredicate("String"));
  Node ttlExp=buildConditional(buildEqtypePredicate(buildTypeVariable("T"),buildTypeVariable("R")),buildTypePredicate("number"),nested);
  ImmutableMap<String,JSType> typeVars=ImmutableMap.of("T",NUMBER_TYPE);
  assertTypeEquals(STRING_OBJECT_TYPE,transform(ttlExp,typeVars));
}
