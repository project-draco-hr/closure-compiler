{
  Node ttlExp=buildConditional(buildEqtypePredicate(buildTypeVariable("T"),buildTypeVariable("R")),buildTypePredicate("string"),buildTypePredicate("number"));
  ImmutableMap<String,JSType> typeVars=ImmutableMap.of("T",NUMBER_TYPE,"R",STRING_TYPE);
  assertTypeEquals(NUMBER_TYPE,transform(ttlExp,typeVars));
}
