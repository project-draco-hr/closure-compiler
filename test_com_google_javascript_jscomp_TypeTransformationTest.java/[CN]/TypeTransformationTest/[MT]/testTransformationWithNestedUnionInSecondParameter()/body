{
  Node ttlExp=buildUnionType(buildTypeVariable("T"),buildUnionType(buildTypePredicate("null"),buildTypeVariable("R")));
  ImmutableMap<String,JSType> typeVars=ImmutableMap.of("T",NUMBER_TYPE,"R",STRING_TYPE);
  assertTypeEquals(createUnionType(NUMBER_TYPE,NULL_TYPE,STRING_TYPE),transform(ttlExp,typeVars));
}
