{
  addRefInternal(ref);
switch (ref.type) {
case SET_FROM_GLOBAL:
    if (declaration == null) {
      declaration=ref;
      docInfo=getDocInfoForDeclaration(ref);
    }
  globalSets++;
break;
case SET_FROM_LOCAL:
localSets++;
break;
case PROTOTYPE_GET:
case DIRECT_GET:
totalGets++;
break;
case ALIASING_GET:
aliasingGets++;
totalGets++;
break;
case CALL_GET:
callGets++;
totalGets++;
break;
case DELETE_PROP:
deleteProps++;
break;
default :
throw new IllegalStateException();
}
}
