{
  if (lineOffsets != null) {
    return;
  }
  try {
    String[] sourceLines=getCode().split("\n");
    lineOffsets=new int[sourceLines.length];
    for (int ii=1; ii < sourceLines.length; ++ii) {
      lineOffsets[ii]=lineOffsets[ii - 1] + sourceLines[ii - 1].length() + 1;
    }
  }
 catch (  IOException e) {
    lineOffsets=new int[1];
    lineOffsets[0]=0;
  }
}
