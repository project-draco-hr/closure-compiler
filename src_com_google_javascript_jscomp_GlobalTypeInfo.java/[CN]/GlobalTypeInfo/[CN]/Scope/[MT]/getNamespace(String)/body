{
  Preconditions.checkArgument(!name.contains("."));
  Namespace ns=localNamespaces.get(name);
  if (ns != null) {
    return ns;
  }
  ns=localClassDefs.get(name);
  if (ns != null) {
    return ns;
  }
  ns=localEnums.get(name);
  if (ns != null) {
    return ns;
  }
  return parent == null ? null : parent.getNamespace(name);
}
