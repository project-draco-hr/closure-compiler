{
  if (expr.isName()) {
    return isNamespace(expr.getString());
  }
  if (!expr.isGetProp()) {
    return false;
  }
  QualifiedName qname=QualifiedName.fromGetprop(expr);
  if (qname == null) {
    return false;
  }
  String leftmost=qname.getLeftmostName();
  if (!isNamespace(leftmost)) {
    return false;
  }
  JSType propType=getDeclaredTypeOf(leftmost).getProp(qname.getAllButLeftmost());
  if (propType == null) {
    return false;
  }
  return propType.isRecordType();
}
