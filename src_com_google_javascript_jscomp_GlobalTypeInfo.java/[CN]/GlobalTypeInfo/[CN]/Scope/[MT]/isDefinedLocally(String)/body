{
  Preconditions.checkNotNull(name);
  Preconditions.checkState(!name.contains("."));
  return locals.containsKey(name) || formals.contains(name) || localFunDefs.containsKey(name)|| "this".equals(name)|| externs.containsKey(name)|| localNamespaces.containsKey(name)|| localTypedefs.containsKey(name)|| localEnums.containsKey(name);
}
