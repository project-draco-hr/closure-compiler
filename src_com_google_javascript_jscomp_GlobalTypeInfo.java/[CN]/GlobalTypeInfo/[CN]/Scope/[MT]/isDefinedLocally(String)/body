{
  Preconditions.checkNotNull(name);
  Preconditions.checkState(!name.contains("."));
  return locals.containsKey(name) || formals.contains(name) || localFunDefs.containsKey(name)|| "this".equals(name)|| localNamespaces != null && localNamespaces.containsKey(name) || localTypedefs != null && localTypedefs.containsKey(name) || localEnums != null && localEnums.containsKey(name);
}
