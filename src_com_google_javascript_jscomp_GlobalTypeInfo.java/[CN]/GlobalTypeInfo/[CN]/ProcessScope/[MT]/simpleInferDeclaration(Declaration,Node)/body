{
  if (decl == null) {
    return null;
  }
  if (constDeclNode != null) {
    Namespace ns=decl.getNamespace();
    if (ns != null) {
      if (ns.finalizeNamespace(constDeclNode)) {
        return ns.toJSType(commonTypes);
      }
      return null;
    }
  }
  if (decl.getTypeOfSimpleDecl() != null) {
    return decl.getTypeOfSimpleDecl();
  }
  NTIScope funScope=(NTIScope)decl.getFunctionScope();
  if (funScope != null) {
    return commonTypes.fromFunctionType(funScope.getDeclaredFunctionType().toFunctionType());
  }
  return null;
}
