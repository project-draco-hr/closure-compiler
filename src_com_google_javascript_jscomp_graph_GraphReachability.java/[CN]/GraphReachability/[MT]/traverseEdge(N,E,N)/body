{
  if (graph.getNode(source).getAnnotation() == REACHABLE && (edgePredicate == null || edgePredicate.apply(new EdgeTuple<N,E>(source,e,destination)))) {
    GraphNode<N,E> destNode=graph.getNode(destination);
    if (destNode.getAnnotation() != REACHABLE) {
      destNode.setAnnotation(REACHABLE);
      return true;
    }
  }
  return false;
}
