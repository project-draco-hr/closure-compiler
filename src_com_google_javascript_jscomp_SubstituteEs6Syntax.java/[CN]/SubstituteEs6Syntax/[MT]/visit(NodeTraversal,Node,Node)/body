{
switch (n.getType()) {
case Token.FUNCTION:
    if (n.getFirstChild().getString().isEmpty() && !n.isArrowFunction() && !NodeUtil.isVarArgsFunction(n)&& !NodeUtil.referencesThis(n.getLastChild())) {
      n.setIsArrowFunction(true);
      compiler.reportCodeChange();
    }
  if (n.isArrowFunction()) {
    maybeSimplifyArrowFunctionBody(n,n.getLastChild());
  }
break;
}
}
