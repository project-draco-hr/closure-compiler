{
  if (!(other instanceof ArithmeticInstruction)) {
    return false;
  }
  ArithmeticInstruction that=(ArithmeticInstruction)other;
  return that.order == this.order && that.operation.equals(this.operation) && that.operand1.equals(this.operand1) && that.operand2.equals(this.operand2) && that.result.equals(this.result);
}
