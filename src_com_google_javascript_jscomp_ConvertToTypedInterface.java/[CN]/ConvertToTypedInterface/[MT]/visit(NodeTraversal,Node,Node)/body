{
switch (n.getType()) {
case Token.TRY:
case Token.DEFAULT_CASE:
    parent.replaceChild(n,n.getFirstChild().detachFromParent());
  compiler.reportCodeChange();
break;
case Token.IF:
case Token.SWITCH:
case Token.CASE:
parent.addChildrenAfter(n.removeChildren().getNext(),n);
n.detachFromParent();
compiler.reportCodeChange();
break;
}
}
