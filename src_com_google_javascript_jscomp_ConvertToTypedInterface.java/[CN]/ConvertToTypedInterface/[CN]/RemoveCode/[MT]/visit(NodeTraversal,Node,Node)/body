{
switch (n.getType()) {
case TRY:
case DEFAULT_CASE:
    parent.replaceChild(n,n.getFirstChild().detachFromParent());
  compiler.reportCodeChange();
break;
case IF:
case SWITCH:
case CASE:
n.removeFirstChild();
Node children=n.removeChildren();
parent.addChildrenAfter(children,n);
n.detachFromParent();
compiler.reportCodeChange();
break;
default :
break;
}
}
