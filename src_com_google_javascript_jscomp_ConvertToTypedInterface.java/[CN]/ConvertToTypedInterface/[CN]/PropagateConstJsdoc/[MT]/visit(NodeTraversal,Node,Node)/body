{
switch (n.getType()) {
case EXPR_RESULT:
    if (NodeUtil.isExprAssign(n)) {
      Node expr=n.getFirstChild();
      processName(t,expr.getFirstChild());
    }
  break;
case VAR:
case CONST:
case LET:
if (n.getChildCount() == 1) {
  processName(t,n.getFirstChild());
}
break;
default :
break;
}
}
