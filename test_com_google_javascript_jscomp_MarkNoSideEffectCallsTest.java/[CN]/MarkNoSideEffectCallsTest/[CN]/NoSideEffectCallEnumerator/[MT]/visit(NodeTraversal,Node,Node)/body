{
  if (n.getType() == Token.NEW) {
    if (!NodeUtil.constructorCallHasSideEffects(n)) {
      noSideEffectCalls.add(n.getFirstChild().getQualifiedName());
    }
  }
 else   if (n.getType() == Token.CALL) {
    if (!NodeUtil.functionCallHasSideEffects(n)) {
      noSideEffectCalls.add(n.getFirstChild().getQualifiedName());
    }
  }
}
