  testSame(LINE_JOINER.join("var someClass = {};","someClass.$clinit = function() {}","someClass.anotherMethod = function() {","  (false && someClass.$clinit());","  (true || someClass.$clinit());","  if (true) {","    someClass.$clinit();","  } else {","    someClass.$clinit();","  }","  while(false) {","    someClass.$clinit();","  }","  for(;false;) {","    someClass.$clinit();","  }","  try {","    someClass.$clinit();","  } catch(e) {","    someClass.$clinit();","  }","  switch(2) {","    case 1: someClass.$clinit(); break;","    case 2: break;","    default: someClass.$clinit();","  }","  var a = true ? (someClass.$clinit(), 0) : 0;","  var b = function() { someClass.$clinit(); }","  someClass.$clinit();","};"));
