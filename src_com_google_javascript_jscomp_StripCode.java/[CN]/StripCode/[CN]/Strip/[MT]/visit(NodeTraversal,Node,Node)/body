{
switch (n.getType()) {
case VAR:
    removeVarDeclarationsByNameOrRvalue(t,n,parent);
  break;
case NAME:
maybeRemoveReferenceToRemovedVariable(t,n,parent);
break;
case ASSIGN:
case ASSIGN_BITOR:
case ASSIGN_BITXOR:
case ASSIGN_BITAND:
case ASSIGN_LSH:
case ASSIGN_RSH:
case ASSIGN_URSH:
case ASSIGN_ADD:
case ASSIGN_SUB:
case ASSIGN_MUL:
case ASSIGN_DIV:
case ASSIGN_MOD:
maybeEliminateAssignmentByLvalueName(t,n,parent);
break;
case CALL:
case NEW:
maybeRemoveCall(t,n,parent);
break;
case OBJECTLIT:
eliminateKeysWithStripNamesFromObjLit(t,n);
break;
case EXPR_RESULT:
maybeEliminateExpressionByName(t,n,parent);
break;
default :
break;
}
}
