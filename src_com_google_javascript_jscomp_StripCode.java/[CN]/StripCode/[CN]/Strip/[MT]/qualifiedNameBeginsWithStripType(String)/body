{
  if (name != null) {
    for (    String type : stripTypes) {
      if (name.equals(type) || name.startsWith(type + ".")) {
        return true;
      }
    }
    for (    String type : stripTypePrefixes) {
      if (name.startsWith(type)) {
        return true;
      }
    }
  }
  return false;
}
