{
  JSDocInfo jsDoc=NodeUtil.getBestJSDocInfo(function);
  if (t.inGlobalScope() || jsDoc != null || hasAnyInlineJsDoc(function)) {
    checkParams(t,function,jsDoc);
  }
  String name=NodeUtil.getName(function);
  if (name != null && compiler.getCodingConvention().isPrivate(name) && (jsDoc == null || !jsDoc.getVisibility().equals(Visibility.PRIVATE))) {
    t.report(function,MUST_BE_PRIVATE,name);
  }
}
