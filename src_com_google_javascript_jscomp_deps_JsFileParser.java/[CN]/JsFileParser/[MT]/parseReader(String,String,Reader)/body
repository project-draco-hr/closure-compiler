{
  provides=new ArrayList<>();
  requires=new ArrayList<>();
  fileHasProvidesOrRequires=false;
  fileIsModule=false;
  logger.fine("Parsing Source: " + filePath);
  doParse(filePath,fileContents);
  DependencyInfo dependencyInfo=new SimpleDependencyInfo(closureRelativePath,filePath,provides,requires,fileIsModule);
  logger.fine("DepInfo: " + dependencyInfo);
  return dependencyInfo;
}
