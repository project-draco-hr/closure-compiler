{
  List<String> entries=new ArrayList<>();
  for (  Map.Entry<String,String> entry : map.entrySet()) {
    String key=entry.getKey().replace("'","\\'");
    String value=entry.getValue().replace("'","\\'");
    entries.add("'" + key + "': '"+ value+ "'");
  }
  out.print("{");
  out.print(Joiner.on(", ").join(entries));
  out.print("}");
}
