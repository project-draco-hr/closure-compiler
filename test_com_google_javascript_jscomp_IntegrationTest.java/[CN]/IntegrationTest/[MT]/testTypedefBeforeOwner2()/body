{
  CompilerOptions options=createCompilerOptions();
  options.setClosurePass(true);
  options.setCollapseProperties(true);
  test(options,LINE_JOINER.join("goog.provide('foo.Bar.Type');","goog.provide('foo.Bar');","/** @typedef {number} */ foo.Bar.Type;","foo.Bar = function() {};"),LINE_JOINER.join("/** @typedef {number} */","foo$Bar.Type;","var foo$Bar = function(){}"));
}
