{
  CompilerOptions options=createCompilerOptions();
  options.setCheckTypes(true);
  String code=Joiner.on('\n').join("/** @constructor @template T */","function F() {}","","/** @param {T} t */","F.prototype.foo = function(t) {","}","","/** @type {F<string>} */","var f = new F;","/** @type {?string} */","var s = null;","f.foo(s); // Type error: f.foo() takes a {string}, not a {?string}");
  test(options,code,TYPE_MISMATCH_WARNING);
}
