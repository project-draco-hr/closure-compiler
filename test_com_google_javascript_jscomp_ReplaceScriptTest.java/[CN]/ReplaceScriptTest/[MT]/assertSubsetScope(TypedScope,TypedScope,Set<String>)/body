{
  Iterator<TypedVar> iter=scope.getVars();
  while (iter.hasNext()) {
    TypedVar var1=iter.next();
    TypedVar var2=subScope.getVar(var1.getName());
    if (missingVars.contains(var1.getName())) {
      assertNull(var2);
    }
 else {
      assertNotNull(var2);
      assertEquals(var1.getType(),var2.getType());
    }
  }
}
