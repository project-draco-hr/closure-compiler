{
  if (node == null) {
    return null;
  }
 else   if (node.getType() == Token.FINALLY) {
    return node;
  }
 else   if (node != null && node.getType() == Token.TARGET) {
    Node fBlock=node.getNext();
    if (fBlock != null && fBlock.getType() == Token.FINALLY) {
      return fBlock;
    }
  }
  throw Kit.codeBug("bad finally target");
}
