{
  String methodName=codegen.getBodyMethodName(scriptOrFn) + "_literal" + count;
  initBodyGeneration();
  argsLocal=firstFreeLocal++;
  localsMax=firstFreeLocal;
  cfw.startMethod(methodName,"(Lorg/mozilla/javascript/Context;" + "Lorg/mozilla/javascript/Scriptable;" + "Lorg/mozilla/javascript/Scriptable;"+ "[Ljava/lang/Object;"+ ")Lorg/mozilla/javascript/Scriptable;",ACC_PRIVATE);
  visitObjectLiteral(node,node.getFirstChild(),true);
  cfw.add(ByteCode.ARETURN);
  cfw.stopMethod((short)(localsMax + 1));
}
