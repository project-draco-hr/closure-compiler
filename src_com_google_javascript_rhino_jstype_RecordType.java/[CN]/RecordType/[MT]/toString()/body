{
  StringBuilder sb=new StringBuilder();
  sb.append("{ ");
  int i=0;
  for (  String property : properties.keySet()) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(property);
    sb.append(" : ");
    sb.append(properties.get(property).toString());
    ++i;
  }
  sb.append(" }");
  return sb.toString();
}
