{
  for (int c=getChar(); c != EOF_CHAR; ) {
    addToString(c);
    if (c == ']' && peekChar() == ']') {
      c=getChar();
      addToString(c);
      if (peekChar() == '>') {
        c=getChar();
        addToString(c);
        return true;
      }
 else {
        continue;
      }
    }
    c=getChar();
  }
  stringBufferTop=0;
  this.string=null;
  parser.addError("msg.XML.bad.form");
  return false;
}
