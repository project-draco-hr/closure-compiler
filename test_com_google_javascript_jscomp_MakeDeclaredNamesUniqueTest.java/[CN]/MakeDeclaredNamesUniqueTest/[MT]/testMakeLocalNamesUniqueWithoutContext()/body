{
  this.useDefaultRenamer=false;
  test("var a;","var unique_a_0");
  testSame("a;");
  test("var a;" + "function foo(a){var b;a}","var unique_a_0;" + "function unique_foo_1(unique_a_2){var unique_b_3;unique_a_2}");
  test("var a;" + "function foo(){var b;a}" + "function boo(){var b;a}","var unique_a_0;" + "function unique_foo_1(){var unique_b_3;unique_a_0}" + "function unique_boo_2(){var unique_b_4;unique_a_0}");
  test("var a = function foo(){foo()};","var unique_a_0 = function unique_foo_1(){unique_foo_1()};");
  test("try { } catch(e) {e;}","try { } catch(unique_e_0) {unique_e_0;}");
  test("try { } catch(e) {e;};" + "try { } catch(e) {e;}","try { } catch(unique_e_0) {unique_e_0;};" + "try { } catch(unique_e_1) {unique_e_1;}");
  test("try { } catch(e) {e; " + "try { } catch(e) {e;}};","try { } catch(unique_e_0) {unique_e_0; " + "try { } catch(unique_e_1) {unique_e_1;} }; ");
}
