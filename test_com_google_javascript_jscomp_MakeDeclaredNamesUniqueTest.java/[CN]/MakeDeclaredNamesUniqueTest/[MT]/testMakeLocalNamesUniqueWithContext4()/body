  testInFunction("var e; try { } catch(e) {e; try { } catch(e) {e;}}","var e; try { } catch(e$$1) {e$$1; try { } catch(e$$2) {e$$2;} }");
  testInFunction("try { } catch(e) {e;}; try { } catch(e) {e;} var e;","try { } catch(e$$1) {e$$1;}; try { } catch(e$$2) {e$$2;} var e;");
