{
  Preconditions.checkState(n.getType() == Token.NAME || n.getType() == Token.STRING);
  boolean isObjLitKey=NodeUtil.isObjectLitKey(n,parent);
  boolean isProperty=isObjLitKey || (parent.getType() == Token.GETPROP && parent.getLastChild() == n);
  if (n.getType() == Token.NAME || isProperty) {
    if (NodeUtil.isConstantByConvention(compiler.getCodingConvention(),n,parent)) {
      n.putBooleanProp(Node.IS_CONSTANT_NAME,true);
    }
  }
}
