{
switch (n.getType()) {
case Token.SCRIPT:
case Token.BLOCK:
case Token.VAR:
case Token.FUNCTION:
case Token.ASSIGN:
    JSDocInfo info=n.getJSDocInfo();
  if (info != null && info.isNoTypeCheck()) {
    if (enterSection) {
      noTypeCheckSection++;
    }
 else {
      noTypeCheckSection--;
    }
  }
validator.setShouldReport(noTypeCheckSection == 0);
break;
}
}
