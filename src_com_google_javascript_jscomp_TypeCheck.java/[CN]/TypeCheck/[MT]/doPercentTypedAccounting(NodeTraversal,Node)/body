{
  JSType type=n.getJSType();
  if (type == null) {
    nullCount++;
  }
 else   if (type.isUnknownType()) {
    if (reportUnknownTypes.isOn()) {
      String unresolvedReference=getUnresolvedReference(type);
      if (unresolvedReference != null) {
        compiler.report(JSError.make(t,n,reportUnknownTypes,UNRESOLVED_TYPE,unresolvedReference));
      }
 else {
        compiler.report(JSError.make(t,n,reportUnknownTypes,UNKNOWN_EXPR_TYPE));
      }
    }
    unknownCount++;
  }
 else {
    typedCount++;
  }
}
