{
switch (n.getType()) {
case THIS:
    return false;
case FUNCTION:
  return false;
case NAME:
if (n.getString().equals("arguments")) {
  return false;
}
 else {
  Var v=s.getVar(n.getString());
  if (v != null && (v.isLocal() || v.nameNode.getParent().isCatch())) {
    return false;
  }
}
break;
default :
break;
}
for (Node c=n.getFirstChild(); c != null; c=c.getNext()) {
if (!isMovableValue(c,s)) {
return false;
}
}
return true;
}
