{
  Preconditions.checkArgument(NodeUtil.isFunction(function),"Node must be a function.");
  Node formalArgPtr=getArgumentAtIndex(function.getFirstChild().getNext().getFirstChild(),argIndex);
  if (formalArgPtr != null) {
    function.getFirstChild().getNext().removeChild(formalArgPtr);
  }
  return formalArgPtr;
}
