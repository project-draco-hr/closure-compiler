{
  if (argNode != null) {
    eliminateParamsAfter(fnNode,argNode.getNext());
    argNode.detachFromParent();
    Node var=IR.var(argNode).useSourceInfoIfMissingFrom(argNode);
    fnNode.getLastChild().addChildrenToFront(var);
    compiler.reportCodeChange();
    return true;
  }
  return false;
}
