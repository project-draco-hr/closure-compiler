{
  double rDbl=stack_double(frame,stackTop);
  --stackTop;
  double lDbl=stack_double(frame,stackTop);
  stack[stackTop]=DOUBLE_MARK;
switch (op) {
case Token.SUB:
    lDbl-=rDbl;
  break;
case Token.MUL:
lDbl*=rDbl;
break;
case Token.DIV:
lDbl/=rDbl;
break;
case Token.MOD:
lDbl%=rDbl;
break;
}
sDbl[stackTop]=lDbl;
return stackTop;
}
