{
  int lIntValue=stack_int32(frame,stackTop - 1);
  int rIntValue=stack_int32(frame,stackTop);
  stack[--stackTop]=DOUBLE_MARK;
switch (op) {
case Token.BITAND:
    lIntValue&=rIntValue;
  break;
case Token.BITOR:
lIntValue|=rIntValue;
break;
case Token.BITXOR:
lIntValue^=rIntValue;
break;
case Token.LSH:
lIntValue<<=rIntValue;
break;
case Token.RSH:
lIntValue>>=rIntValue;
break;
}
sDbl[stackTop]=lIntValue;
return stackTop;
}
