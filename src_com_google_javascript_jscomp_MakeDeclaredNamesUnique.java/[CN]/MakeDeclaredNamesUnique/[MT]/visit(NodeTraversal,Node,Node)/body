{
switch (n.getType()) {
case Token.NAME:
    String newName=getReplacementName(n.getString());
  if (newName != null) {
    Renamer renamer=nameStack.peek();
    if (renamer.stripConstIfReplaced()) {
      n.removeProp(Node.IS_CONSTANT_NAME);
    }
    n.setString(newName);
    t.getCompiler().reportCodeChange();
  }
break;
case Token.FUNCTION:
nameStack.pop();
nameStack.pop();
break;
case Token.PARAM_LIST:
break;
case Token.CATCH:
nameStack.pop();
break;
}
}
