{
  Node func=null;
  String funcName=null;
  if (n.isFunction() && isConstructor(n)) {
    func=n;
    funcName=n.getFirstChild().getString();
  }
 else   if (n.isAssign() && n.getFirstChild().getNext().isFunction() && isConstructor(n)) {
    func=n.getFirstChild().getNext();
    funcName=n.getFirstChild().getQualifiedName();
  }
  if (func != null) {
    FunctionType funcType=JSType.toMaybeFunctionType(func.getJSType());
    checkConstructor(func,(funcType != null) ? funcType.getInstanceType() : null,t.getSourceName(),funcName);
  }
}
