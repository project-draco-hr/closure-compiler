{
  if (oldSize != -1) {
    int delta=oldSize - newSize;
    Integer reduction=record.get(passName);
    if (delta > 0) {
      if (reduction == null) {
        reduction=delta;
      }
 else {
        reduction=reduction + delta;
      }
      record.put(passName,reduction);
    }
  }
  return newSize;
}
