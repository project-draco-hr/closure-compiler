{
  this.jsRoot=jsRoot;
switch (mode) {
case TIMING_ONLY:
    this.trackSize=false;
  this.trackGzippedSize=false;
break;
case RAW_SIZE:
this.trackSize=true;
this.trackGzippedSize=false;
break;
case ALL:
this.trackSize=true;
this.trackGzippedSize=true;
break;
case OFF:
default :
throw new UnsupportedOperationException();
}
}
