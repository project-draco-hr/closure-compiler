{
  this.jsRoot=jsRoot;
switch (mode) {
case TIMING_ONLY:
    this.trackSize=false;
  this.trackGzSize=false;
break;
case RAW_SIZE:
this.trackSize=true;
this.trackGzSize=false;
break;
case ALL:
this.trackSize=true;
this.trackGzSize=true;
break;
case OFF:
default :
throw new IllegalArgumentException("PerformanceTracker can't work without tracer data.");
}
}
