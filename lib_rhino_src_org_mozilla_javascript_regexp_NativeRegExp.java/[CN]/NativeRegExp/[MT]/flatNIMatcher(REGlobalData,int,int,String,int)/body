{
  if ((gData.cp + length) > end)   return false;
  char[] source=gData.regexp.source;
  for (int i=0; i < length; i++) {
    char c1=source[matchChars + i];
    char c2=input.charAt(gData.cp + i);
    if (c1 != c2 && upcase(c1) != upcase(c2)) {
      return false;
    }
  }
  gData.cp+=length;
  return true;
}
