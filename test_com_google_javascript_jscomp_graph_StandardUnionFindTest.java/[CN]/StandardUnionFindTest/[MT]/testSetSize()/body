{
  union.union("A","B");
  union.union("B","C");
  union.union("D","E");
  union.union("F","F");
  assertEquals(3,union.findAll("A").size());
  assertEquals(3,union.findAll("B").size());
  assertEquals(3,union.findAll("C").size());
  assertEquals(2,union.findAll("D").size());
  assertEquals(1,union.findAll("F").size());
}
