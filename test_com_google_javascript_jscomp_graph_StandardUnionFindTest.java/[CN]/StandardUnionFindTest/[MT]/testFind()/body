{
  union.add("A");
  union.add("B");
  assertEquals("A",union.find("A"));
  assertEquals("B",union.find("B"));
  union.union("A","B");
  assertEquals(union.find("A"),union.find("B"));
  try {
    union.find("Z");
    fail("find() on unknown element should not be allowed.");
  }
 catch (  IllegalArgumentException expected) {
  }
}
