{
  union.union("A","B");
  union.union("A","B");
  union.union("B","A");
  union.union("B","C");
  union.union("D","E");
  union.union("F","F");
  Collection<Set<String>> classes=union.allEquivalenceClasses();
  assertEquals(3,classes.size());
  assertContentsAnyOrder(classes,ImmutableSet.of("A","B","C"),ImmutableSet.of("D","E"),ImmutableSet.of("F"));
}
