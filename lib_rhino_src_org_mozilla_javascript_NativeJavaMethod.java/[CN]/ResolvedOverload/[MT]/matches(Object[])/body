{
  if (args.length != types.length) {
    return false;
  }
  for (int i=0, l=args.length; i < l; i++) {
    Object arg=args[i];
    if (arg instanceof Wrapper)     arg=((Wrapper)arg).unwrap();
    if (arg == null) {
      if (types[i] != null)       return false;
    }
 else     if (arg.getClass() != types[i]) {
      return false;
    }
  }
  return true;
}
