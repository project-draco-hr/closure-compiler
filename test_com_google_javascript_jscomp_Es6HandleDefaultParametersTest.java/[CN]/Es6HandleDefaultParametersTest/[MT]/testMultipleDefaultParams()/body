{
  test(Joiner.on('\n').join("function x() {}","var y = 1;","function f(z=x, w=y) {","  let x = y;","  var y = 3;","  return w;","}"),Joiner.on('\n').join("function x() {}","var y = 1;","function f(z=x, w=y) {","  let x$0 = y$1;","  var y$1 = 3;","  return w;","}"));
