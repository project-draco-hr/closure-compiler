{
switch (reach) {
case ALL:
    this.inlineVariables=true;
  this.inlineLocalVariables=true;
break;
case LOCAL_ONLY:
this.inlineVariables=false;
this.inlineLocalVariables=true;
break;
case NONE:
this.inlineVariables=false;
this.inlineLocalVariables=false;
break;
default :
throw new IllegalStateException("unexpected");
}
}
