{
  if (previousLine != line) {
    previousColumn=0;
  }
  if (line != nextLine || col != nextCol) {
    if (previousLine == line) {
      out.append(',');
    }
    writeEntry(m,col);
    previousLine=line;
    previousColumn=col;
  }
  for (int i=line; i <= nextLine; i++) {
    if (i == nextLine) {
      break;
    }
    closeLine(false);
    openLine(false);
  }
}
