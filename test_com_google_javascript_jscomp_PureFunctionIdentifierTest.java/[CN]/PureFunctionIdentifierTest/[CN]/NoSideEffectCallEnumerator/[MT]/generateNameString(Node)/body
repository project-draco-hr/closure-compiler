{
  if (node.getType() == Token.OR) {
    return "(" + generateNameString(node.getFirstChild()) + " || "+ generateNameString(node.getLastChild())+ ")";
  }
 else   if (node.getType() == Token.HOOK) {
    return "(" + generateNameString(node.getFirstChild().getNext()) + " : "+ generateNameString(node.getLastChild())+ ")";
  }
 else {
    return node.getQualifiedName();
  }
}
