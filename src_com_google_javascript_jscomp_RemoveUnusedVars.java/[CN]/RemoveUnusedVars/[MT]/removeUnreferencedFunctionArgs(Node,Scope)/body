{
  Node argList=function.getFirstChild().getNext();
  Node lastArg;
  while ((lastArg=argList.getLastChild()) != null) {
    Scope.Var var=fnScope.getVar(lastArg.getString());
    if (!var.referenced) {
      argList.removeChild(lastArg);
      fnScope.undeclare(var);
      numRemoved_++;
    }
 else {
      break;
    }
  }
}
