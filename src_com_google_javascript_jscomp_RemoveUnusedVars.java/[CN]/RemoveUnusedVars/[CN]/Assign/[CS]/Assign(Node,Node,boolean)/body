{
  Preconditions.checkState(NodeUtil.isAssignmentOp(assignNode));
  this.assignNode=assignNode;
  this.nameNode=nameNode;
  this.isPropertyAssign=isPropertyAssign;
  this.mayHaveSecondarySideEffects=!assignNode.getParent().isExprResult() || NodeUtil.mayHaveSideEffects(assignNode.getFirstChild()) || NodeUtil.mayHaveSideEffects(assignNode.getLastChild());
}
