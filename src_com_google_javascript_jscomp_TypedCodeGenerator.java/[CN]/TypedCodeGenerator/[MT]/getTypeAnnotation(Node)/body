{
  JSType type=node.getJSType();
  if (type instanceof FunctionType) {
    return getFunctionAnnotation(node);
  }
 else   if (type != null && !type.isUnknownType() && !type.isEmptyType() && !type.isVoidType()) {
    return "/** @type {" + node.getJSType() + "} */\n";
  }
 else {
    return "";
  }
}
