{
  Node pn=shiftExpr();
  int lineno;
  int charno;
  for (; ; ) {
    int tt=peekToken();
    lineno=ts.getLineno();
    charno=ts.getCharno();
switch (tt) {
case Token.IN:
      if (inForInit)       break;
case Token.INSTANCEOF:
case Token.LE:
case Token.LT:
case Token.GE:
case Token.GT:
    consumeToken();
  decompiler.addToken(tt);
pn=nf.createBinary(tt,pn,shiftExpr(),lineno,charno);
continue;
}
break;
}
return pn;
}
