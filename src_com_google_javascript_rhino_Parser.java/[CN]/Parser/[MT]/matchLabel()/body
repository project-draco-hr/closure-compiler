{
  int lineno=ts.getLineno();
  String label=null;
  int tt=peekTokenOrEOL();
  if (tt == Token.NAME) {
    consumeToken();
    label=ts.getString();
    decompiler.addName(label);
    Node n=null;
    if (labelSet != null) {
      n=labelSet.get(label);
    }
    if (n == null) {
      reportError("msg.undef.label");
    }
  }
  return label;
}
