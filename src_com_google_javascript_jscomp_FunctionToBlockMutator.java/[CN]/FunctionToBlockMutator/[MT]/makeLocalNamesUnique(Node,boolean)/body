{
  Supplier<String> idSupplier=compiler.getUniqueNameIdSupplier();
  NodeTraversal.traverse(compiler,fnNode,new MakeDeclaredNamesUnique(new InlineRenamer(compiler.getCodingConvention(),idSupplier,"inline_",isCallInLoop,true,null)));
  new RenameLabels(compiler,new LabelNameSupplier(idSupplier),false).process(null,fnNode);
}
