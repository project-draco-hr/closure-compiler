{
switch (n.getType()) {
case Token.IF:
case Token.THROW:
case Token.SWITCH:
case Token.EXPR_RESULT:
    return true;
case Token.RETURN:
  return n.hasChildren();
case Token.FOR:
if (NodeUtil.isForIn(n)) {
  return !mayHaveSideEffects(n.getFirstChild());
}
 else {
  return !n.getFirstChild().isVar();
}
case Token.LABEL:
return isFusableControlStatement(n.getLastChild());
case Token.BLOCK:
return isFusableControlStatement(n.getFirstChild());
}
return false;
}
