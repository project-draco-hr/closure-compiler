{
  if (!favorsCommaOverSemiColon && !block.isBlock()) {
    return false;
  }
  if (!block.hasChildren() || block.hasOneChild()) {
    return false;
  }
  Node last=block.getLastChild();
  for (Node c=block.getFirstChild(); c != null; c=c.getNext()) {
    if (!c.isExprResult() && c != last) {
      return false;
    }
  }
switch (last.getType()) {
case Token.IF:
case Token.THROW:
case Token.SWITCH:
case Token.EXPR_RESULT:
    return true;
case Token.RETURN:
  return last.hasChildren();
case Token.FOR:
return NodeUtil.isForIn(last) && !mayHaveSideEffects(last.getFirstChild());
}
return false;
}
