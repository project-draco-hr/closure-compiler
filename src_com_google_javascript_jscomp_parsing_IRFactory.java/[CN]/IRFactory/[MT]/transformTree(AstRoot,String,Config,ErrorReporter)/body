{
  IRFactory irFactory=new IRFactory(sourceString,node.getSourceName(),config,errorReporter);
  Node irNode=irFactory.transform(node);
  Node.FileLevelJsDocBuilder fileLevelJsDocBuilder=irNode.getJsDocBuilderForNode();
  JSDocInfo fileOverviewInfo=null;
  if (node.getComments() != null) {
    for (    Comment comment : node.getComments()) {
      if (comment.getCommentType() == JSDOC) {
        JsDocInfoParser jsDocParser=irFactory.createJsDocInfoParser(comment.getValue(),comment.getLineno(),comment.getAbsolutePosition(),fileLevelJsDocBuilder,fileOverviewInfo);
        if (jsDocParser.getFileOverviewJSDocInfo() != fileOverviewInfo) {
          fileOverviewInfo=jsDocParser.getFileOverviewJSDocInfo();
        }
 else {
          JSDocInfo info=jsDocParser.retrieveAndResetParsedJSDocInfo();
          if (info != null) {
            irFactory.attachJsDoc(comment,info);
          }
        }
      }
    }
    if (fileOverviewInfo != null) {
      if ((irNode.getJSDocInfo() != null) && (irNode.getJSDocInfo().getLicense() != null)) {
        fileOverviewInfo.setLicense(irNode.getJSDocInfo().getLicense());
      }
      irNode.setJSDocInfo(fileOverviewInfo);
    }
  }
  return irNode;
}
