{
  String comment=node.getValue();
  int lineno=node.getLineno();
  int position=node.getAbsolutePosition();
  int numOpeningChars=3;
  JsDocInfoParser parser=new JsDocInfoParser(new JsDocTokenStream(comment.substring(numOpeningChars),lineno,position2charno(position) + numOpeningChars),node,irNode,config,errorReporter);
  return parser.parseInlineTypeDoc();
}
