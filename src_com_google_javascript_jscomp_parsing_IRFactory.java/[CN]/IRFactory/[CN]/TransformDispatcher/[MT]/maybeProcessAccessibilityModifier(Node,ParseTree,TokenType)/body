{
  if (type != null) {
    Visibility access;
switch (type) {
case PUBLIC:
      access=Visibility.PUBLIC;
    break;
case PROTECTED:
  access=Visibility.PROTECTED;
break;
case PRIVATE:
access=Visibility.PRIVATE;
break;
default :
throw new IllegalStateException("Unexpected access modifier type");
}
n.putProp(Node.ACCESS_MODIFIER,access);
}
}
