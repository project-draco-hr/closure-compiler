{
  int tt=peekToken();
  if (tt == Token.YIELD) {
    return returnOrYield(tt,true);
  }
  AstNode pn=condExpr();
  tt=peekToken();
  if (Token.FIRST_ASSIGN <= tt && tt <= Token.LAST_ASSIGN) {
    consumeToken();
    markDestructuring(pn);
    int opPos=ts.tokenBeg;
    pn=new Assignment(tt,pn,assignExpr(),opPos);
  }
  return pn;
}
