{
  AstNode pn=shiftExpr();
  for (; ; ) {
    int tt=peekToken(), opPos=ts.tokenBeg;
switch (tt) {
case Token.IN:
      if (inForInit)       break;
case Token.INSTANCEOF:
case Token.LE:
case Token.LT:
case Token.GE:
case Token.GT:
    consumeToken();
  pn=new InfixExpression(tt,pn,shiftExpr(),opPos);
continue;
}
break;
}
return pn;
}
