{
  if (currentToken != Token.THROW)   codeBug();
  consumeToken();
  int pos=ts.tokenBeg, lineno=ts.lineno;
  if (peekTokenOrEOL() == Token.EOL) {
    reportError("msg.bad.throw.eol");
  }
  AstNode expr=expr();
  ThrowStatement pn=new ThrowStatement(pos,getNodeEnd(expr) - pos,expr);
  pn.setLineno(lineno);
  return pn;
}
