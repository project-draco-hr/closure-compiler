{
  if (n.getFirstChild().getType() == Token.NAME) {
    if ("arguments".equals(n.getFirstChild().getString())) {
      t.report(n,ARGUMENTS_ASSIGNMENT);
    }
 else     if ("eval".equals(n.getFirstChild().getString())) {
      if (noCajaChecks) {
        t.report(n,EVAL_ASSIGNMENT);
      }
    }
  }
}
