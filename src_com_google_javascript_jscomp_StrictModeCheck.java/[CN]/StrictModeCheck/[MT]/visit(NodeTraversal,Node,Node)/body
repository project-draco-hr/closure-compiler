{
  if (n.getType() == Token.NAME) {
    if (!isDeclaration(n)) {
      checkNameUse(t,n);
    }
  }
 else   if (n.getType() == Token.ASSIGN) {
    checkAssignment(t,n);
  }
 else   if (n.getType() == Token.DELPROP) {
    checkDelete(t,n);
  }
 else   if (n.getType() == Token.OBJECTLIT) {
    checkObjectLiteral(t,n);
  }
 else   if (n.getType() == Token.LABEL) {
    checkLabel(t,n);
  }
}
