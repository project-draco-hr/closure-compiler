{
  if (n.isName()) {
    if (!isDeclaration(n)) {
      checkNameUse(t,n);
    }
  }
 else   if (n.isAssign()) {
    checkAssignment(t,n);
  }
 else   if (n.getType() == Token.DELPROP) {
    checkDelete(t,n);
  }
 else   if (n.isObjectLit()) {
    checkObjectLiteral(t,n);
  }
 else   if (n.isLabel()) {
    checkLabel(t,n);
  }
}
