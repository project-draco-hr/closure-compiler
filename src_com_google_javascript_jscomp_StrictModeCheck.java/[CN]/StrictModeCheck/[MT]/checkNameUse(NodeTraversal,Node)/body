{
  Var v=t.getScope().getVar(n.getString());
  if (v == null) {
    if (!noVarCheck) {
      t.report(n,UNKNOWN_VARIABLE,n.getString());
    }
  }
  if (!noCajaChecks) {
    if ("eval".equals(n.getString())) {
      t.report(n,EVAL_USE);
    }
 else     if (n.getString().endsWith("__")) {
      t.report(n,ILLEGAL_NAME);
    }
  }
}
