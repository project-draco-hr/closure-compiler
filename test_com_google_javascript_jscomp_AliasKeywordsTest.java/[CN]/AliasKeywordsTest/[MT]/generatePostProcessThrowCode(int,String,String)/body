{
  StringBuilder sb=new StringBuilder();
  sb.append("function ");
  sb.append(AliasKeywords.ALIAS_THROW);
  sb.append("(t){throw t;}");
  sb.append(code);
  for (int i=0; i < repetitions; i++) {
    sb.append(AliasKeywords.ALIAS_THROW);
    sb.append("(");
    sb.append(whatToThrow);
    sb.append(");");
  }
  return sb.toString();
}
