{
  if (n == scope.getRootNode())   return;
  if (n.getType() == Token.LP && parent == scope.getRootNode()) {
    handleFunctionInputs(parent);
    return;
  }
switch (n.getType()) {
case Token.FUNCTION:
    if (parent.getType() == Token.NAME) {
      return;
    }
  define(n,parent);
break;
case Token.CATCH:
case Token.VAR:
define(n,parent);
break;
}
}
