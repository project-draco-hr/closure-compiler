{
  setLanguage(ES6,ES5);
  test("Math.clz32(x);","$jscomp.math.clz32(x);");
  assertTrue(getLastCompiler().needsEs6Runtime);
  setLanguage(ES6,ES3);
  test("Array.of(x);","$jscomp.array.of(x);");
  assertTrue(getLastCompiler().needsEs6Runtime);
  setLanguage(ES6,ES3);
  test("Object.keys(x);","$jscomp.object.keys(x);");
  assertTrue(getLastCompiler().needsEs6Runtime);
}
