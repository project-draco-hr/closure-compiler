{
  if (placeHolder.getParent() != null) {
    if (before == null) {
      placeHolder.getParent().addChildrenToFront(original);
      placeHolder.getParent().removeChildAfter(original);
    }
 else {
      placeHolder.getParent().addChildAfter(original,before);
      placeHolder.getParent().removeChildAfter(original);
    }
  }
}
