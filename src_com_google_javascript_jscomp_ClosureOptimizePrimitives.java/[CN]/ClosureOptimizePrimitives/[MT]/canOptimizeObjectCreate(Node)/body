{
  Node curParam=firstParam;
  while (curParam != null) {
    if (curParam.getType() != Token.STRING && curParam.getType() != Token.NUMBER) {
      return false;
    }
    curParam=curParam.getNext();
    if (curParam == null) {
      return false;
    }
    curParam=curParam.getNext();
  }
  return true;
}
