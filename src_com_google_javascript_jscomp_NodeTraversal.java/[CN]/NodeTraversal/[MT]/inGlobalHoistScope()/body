{
  if (curNode.isFunction() && getCfgRoot() == curNode) {
    if (cfgRoots.isEmpty()) {
      return getClosestHoistScope().isGlobal();
    }
 else {
      Node temp=cfgRoots.pop();
      boolean result=cfgRoots.isEmpty() || !cfgRoots.peek().isFunction();
      cfgRoots.push(temp);
      return result;
    }
  }
 else {
    return !getCfgRoot().isFunction();
  }
}
