{
  testSame(LINE_JOINER.join("var /** @const */ alias = ns.Foo;","function f(alias) {","  var x = alias","}"));
  testSame(LINE_JOINER.join("var /** @const */ alias = ns.Foo;","function f() {","  var /** @const */ alias = 5;","  var x = alias","}"));
  testSame(LINE_JOINER.join("/** @const */","var x = y;","function f() {","  var x = 123;","  function g() {","    return x;","  }","}"));
}
