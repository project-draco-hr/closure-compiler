{
  test("function Foo(){}; var /** @const */ alias = Foo; var x = new alias;","function Foo(){}; var /** @const */ alias = Foo; var x = new Foo;");
  test("function Foo(){}; /** @const */ mod.alias = Foo; var x = new mod.alias;","function Foo(){}; /** @const */ mod.alias = Foo; var x = new Foo;");
  test("function Foo(){}; /** @const */ mod.alias = Foo; var x = new mod.alias.Subfoo;","function Foo(){}; /** @const */ mod.alias = Foo; var x = new Foo.Subfoo;");
}
