{
  test("var /** @const */ alias = ns.Foo; /** @type {alias} */ var x;","var /** @const */ alias = ns.Foo; /** @type {ns.Foo} */ var x;");
  test("/** @const */ mod.alias = ns.Foo; /** @type {mod.alias} */ var x;","/** @const */ mod.alias = ns.Foo; /** @type {ns.Foo} */ var x;");
  test("/** @const */ mod.alias = ns.Foo; /** @type {mod.alias.Subfoo} */ var x;","/** @const */ mod.alias = ns.Foo; /** @type {ns.Foo.Subfoo} */ var x;");
}
