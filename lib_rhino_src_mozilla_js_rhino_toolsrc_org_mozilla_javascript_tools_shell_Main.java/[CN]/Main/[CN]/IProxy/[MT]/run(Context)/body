{
  if (type == PROCESS_FILES) {
    processFiles(cx,args);
  }
 else   if (type == EVAL_INLINE_SCRIPT) {
    Script script=loadScriptFromSource(cx,scriptText,"<command>",1,null);
    if (script != null) {
      evaluateScript(script,cx,getGlobal());
    }
  }
 else {
    throw Kit.codeBug();
  }
  return null;
}
