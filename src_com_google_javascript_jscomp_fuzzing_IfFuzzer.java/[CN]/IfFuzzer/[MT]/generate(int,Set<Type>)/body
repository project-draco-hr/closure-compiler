{
  int numComponents;
  if (budget <= 3) {
    numComponents=2;
  }
 else {
    numComponents=context.random.nextDouble() < getOwnConfig().get("hasElse").getAsDouble() ? 3 : 2;
  }
  AbstractFuzzer[] fuzzers=new AbstractFuzzer[numComponents];
  BlockFuzzer blockFuzzer=new BlockFuzzer(context);
  Arrays.fill(fuzzers,blockFuzzer);
  fuzzers[0]=new ExpressionFuzzer(context);
  Node[] components=distribute(budget - 1,fuzzers);
  return new Node(Token.IF,components);
}
