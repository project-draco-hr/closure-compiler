{
  setLanguage(CompilerOptions.LanguageMode.ECMASCRIPT6,CompilerOptions.LanguageMode.ECMASCRIPT5);
  setFilename("test");
  testModules("function foo() {}" + "module.exports = {" + "  prop: 'value',"+ "  foo"+ "}","goog.provide('module$test');" + "function foo$$module$test() {}" + "module$test = { prop: 'value', foo }");
  testModules("module.exports = {\n" + "  prop: 'value',\n" + "  foo() {\n"+ "    console.log('bar');\n"+ "  }\n"+ "};","goog.provide('module$test');" + "module$test = { prop: 'value', foo() { console.log('bar'); }}");
}
