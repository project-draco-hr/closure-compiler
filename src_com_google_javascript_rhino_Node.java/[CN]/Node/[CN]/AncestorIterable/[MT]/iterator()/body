{
  return new Iterator<Node>(){
    @Override public boolean hasNext(){
      return cur != null;
    }
    @Override public Node next(){
      if (!hasNext())       throw new NoSuchElementException();
      Node n=cur;
      cur=cur.getParent();
      return n;
    }
    @Override public void remove(){
      throw new UnsupportedOperationException();
    }
  }
;
}
