{
  Node prev=child.previous;
  if (first == child) {
    first=child.next;
  }
  if (prev != null) {
    prev.next=child.next;
  }
  if (last == child) {
    last=prev;
  }
  if (child.next != null) {
    child.next.previous=prev;
  }
  child.next=null;
  child.previous=null;
  child.parent=null;
}
