{
  if (item == null) {
    return null;
  }
 else   if (item.getType() == propType) {
    return item.getNext();
  }
 else {
    PropListItem result=removeProp(item.getNext(),propType);
    if (result != item.getNext()) {
      item.setNext(result);
      return item;
    }
 else {
      return item;
    }
  }
}
