{
  Preconditions.checkArgument(prev.parent == this,"prev is not a child of this node.");
  Preconditions.checkArgument(prev.next != null,"no next sibling.");
  Node child=prev.next;
  prev.next=child.next;
  if (child == last) {
    last=prev;
  }
  child.next=null;
  child.parent=null;
  return child;
}
