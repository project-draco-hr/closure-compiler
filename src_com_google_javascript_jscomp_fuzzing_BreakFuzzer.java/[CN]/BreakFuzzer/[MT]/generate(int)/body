{
  Node node=new Node(Token.BREAK);
  Scope localScope=context.scopeManager.localScope();
  double toLabel=getOwnConfig().optDouble("toLabel");
  if (budget > 1 && localScope.loopLabels.size() + localScope.otherLabels.size() > 0 && context.random.nextDouble() < toLabel) {
    node.addChildToBack(Node.newString(Token.LABEL_NAME,localScope.randomLabelForBreak(context.random)));
  }
  return node;
}
