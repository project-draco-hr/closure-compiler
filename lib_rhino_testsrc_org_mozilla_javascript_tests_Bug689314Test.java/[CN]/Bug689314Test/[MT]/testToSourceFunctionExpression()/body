{
  assertEquals("var x = function() 1 + 2;\n",toSource("var x = function () 1+2"));
  assertEquals("var x = function() {\n  return 1 + 2;\n};\n",toSource("var x = function () {return 1+2}"));
  assertEquals("var x = function F() 1 + 2;\n",toSource("var x = function F() 1+2"));
  assertEquals("var x = function F() {\n  return 1 + 2;\n};\n",toSource("var x = function F() {return 1+2}"));
}
