{
  testWarning(LINE_JOINER.join("/**"," * @param {string} x"," */","function f(x, y) {}"),MISSING_PARAM_JSDOC);
  testWarning(LINE_JOINER.join("/**"," * @param {string} x"," */","function f(x = 1) {}"),OPTIONAL_PARAM_NOT_MARKED_OPTIONAL);
  testWarning(LINE_JOINER.join("/**"," * @param {string} x"," */","function f(x, y = 1) {}"),MISSING_PARAM_JSDOC);
}
