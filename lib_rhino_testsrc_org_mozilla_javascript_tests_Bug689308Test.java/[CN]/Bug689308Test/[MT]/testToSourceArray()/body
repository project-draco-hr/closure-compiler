{
  assertEquals("[];\n",toSource("[]"));
  assertEquals("[,];\n",toSource("[,]"));
  assertEquals("[, ,];\n",toSource("[,,]"));
  assertEquals("[, , ,];\n",toSource("[,,,]"));
  assertEquals("[1];\n",toSource("[1]"));
  assertEquals("[1];\n",toSource("[1,]"));
  assertEquals("[, 1];\n",toSource("[,1]"));
  assertEquals("[1, 1];\n",toSource("[1,1]"));
}
