{
  enableTypeCheck();
  test("var x = {};\nif (x != null) throw 'a';\n","var x = {};\nif (x) throw 'a';\n");
  test("var x = {};\nif (x == null) throw 'a';\n","var x = {};\nif (!x) throw 'a';\n");
  test("var x = {};\nif (x !== null) throw 'a';\n","var x = {};\nif (x) throw 'a';\n");
  test("var x = {};\nif (x === null) throw 'a';\n","var x = {};\nif (!x) throw 'a';\n");
  test("var x = {};\nif (null != x) throw 'a';\n","var x = {};\nif (x) throw 'a';\n");
  test("var x = {};\nif (null == x) throw 'a';\n","var x = {};\nif (!x) throw 'a';\n");
  test("var x = {};\nif (null !== x) throw 'a';\n","var x = {};\nif (x) throw 'a';\n");
  test("var x = {};\nif (null === x) throw 'a';\n","var x = {};\nif (!x) throw 'a';\n");
}
