{
  late=false;
  foldSame("for(;;) if (a) break");
  foldSame("for(;;) if (a) { f(); break }");
  foldSame("for(;;) if (a) break; else f()");
  foldSame("for(;a;) if (b) break");
  foldSame("for(;a;) { if (b) break; if (c) break; }");
  foldSame("while(1) if (a) break");
  enableNormalize(true);
  foldSame("while(1) if (a) break");
}
