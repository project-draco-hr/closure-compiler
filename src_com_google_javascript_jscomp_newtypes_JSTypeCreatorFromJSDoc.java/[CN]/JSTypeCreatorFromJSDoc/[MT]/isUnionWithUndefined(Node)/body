{
  if (n == null || n.getType() != Token.PIPE) {
    return false;
  }
  for (  Node child : n.children()) {
    if (child.getType() == Token.VOID || child.getType() == Token.STRING && (child.getString().equals("void") || child.getString().equals("undefined"))) {
      return true;
    }
  }
  return false;
}
