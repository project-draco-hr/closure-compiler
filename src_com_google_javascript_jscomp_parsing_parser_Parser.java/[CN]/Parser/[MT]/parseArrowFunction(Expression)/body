{
  SourcePosition start=getTreeStartLocation();
  inGeneratorContext.addLast(false);
  FormalParameterListTree formalParameterList;
  if (peekId()) {
    ParseTree param=parseIdentifierExpression();
    formalParameterList=new FormalParameterListTree(getTreeLocation(start),ImmutableList.of(param));
  }
 else {
    formalParameterList=parseFormalParameterList();
  }
  if (peekImplicitSemiColon()) {
    reportError("No newline allowed before '=>'");
  }
  eat(TokenType.ARROW);
  ParseTree functionBody;
  if (peek(TokenType.OPEN_CURLY)) {
    functionBody=parseFunctionBody();
  }
 else {
    functionBody=parseAssignment(expressionIn);
  }
  FunctionDeclarationTree declaration=new FunctionDeclarationTree(getTreeLocation(start),null,false,false,FunctionDeclarationTree.Kind.ARROW,formalParameterList,functionBody);
  inGeneratorContext.removeLast();
  return declaration;
}
