{
  inGeneratorContext.addLast(isGenerator);
  GenericTypeListTree generics=maybeParseGenericTypes();
  FormalParameterListTree formalParameterList=parseFormalParameterList(ParamContext.IMPLEMENTATION);
  ParseTree returnType=maybeParseColonType();
  BlockTree functionBody=parseFunctionBody();
  FunctionDeclarationTree declaration=new FunctionDeclarationTree(getTreeLocation(start),name,generics,isStatic,isGenerator,false,kind,formalParameterList,returnType,functionBody);
  inGeneratorContext.removeLast();
  return declaration;
}
