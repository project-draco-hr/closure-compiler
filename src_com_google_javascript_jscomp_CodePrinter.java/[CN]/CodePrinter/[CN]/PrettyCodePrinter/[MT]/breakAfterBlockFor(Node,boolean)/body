{
  Preconditions.checkState(n.isBlock(),n);
  Node parent=n.getParent();
  if (parent != null) {
    Token type=parent.getType();
switch (type) {
case DO:
      return false;
case FUNCTION:
    return false;
case TRY:
  return n != parent.getFirstChild();
case CATCH:
return !NodeUtil.hasFinally(getTryForCatch(parent));
case IF:
return n == parent.getLastChild();
default :
break;
}
}
return true;
}
