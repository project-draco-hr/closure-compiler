{
  if (createSrcMap && node.getProp(Node.SOURCEFILE_PROP) != null && node.getLineno() > 0) {
    int line=getCurrentLineIndex();
    int index=getCurrentCharIndex();
    if (index >= 0) {
      Preconditions.checkState(!mappings.isEmpty(),"Mismatch in start and end of mapping");
      Mapping mapping=mappings.pop();
      mapping.end=new Position(line,index);
    }
  }
}
