{
switch (mode) {
case ECMASCRIPT3:
    return Mode.ES3;
case ECMASCRIPT5:
  return Mode.ES5;
case ECMASCRIPT5_STRICT:
return Mode.ES5_STRICT;
case ECMASCRIPT6:
return Mode.ES6;
case ECMASCRIPT6_STRICT:
return Mode.ES6_STRICT;
case ECMASCRIPT6_TYPED:
return Mode.ES6_TYPED;
default :
throw new IllegalStateException("unexpected language mode: " + mode);
}
}
