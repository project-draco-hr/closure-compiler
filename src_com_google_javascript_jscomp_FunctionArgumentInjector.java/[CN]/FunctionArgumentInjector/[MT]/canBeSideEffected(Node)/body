{
switch (n.getType()) {
case Token.CALL:
    if (!n.isNoSideEffectsCall()) {
      return true;
    }
  break;
case Token.NEW:
return true;
case Token.NAME:
if (!NodeUtil.isConstantName(n)) {
return true;
}
break;
}
for (Node c=n.getFirstChild(); c != null; c=c.getNext()) {
if (canBeSideEffected(c)) {
return true;
}
}
return false;
}
