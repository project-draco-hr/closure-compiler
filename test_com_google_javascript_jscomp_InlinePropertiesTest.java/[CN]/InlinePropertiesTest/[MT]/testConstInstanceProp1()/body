{
  test("/** @constructor */\n" + "function C() {\n" + "  this.foo = 1;\n"+ "}\n"+ "new C().foo;","function C() {\n" + "  this.foo = 1;\n" + "}\n"+ "new C(), 1;");
  test(LINE_JOINER.join("/** @constructor */","function C() {","  {","    this.foo = 1;","  }","}","new C().foo;"),LINE_JOINER.join("function C() {","  {","    this.foo = 1;","  }","}","new C(), 1;"));
}
