{
  boolean valid;
switch (n.getType()) {
case Token.GETPROP:
case Token.GETELEM:
case Token.NEW:
case Token.CALL:
    valid=maybeValidDerefType(n.getFirstChild());
  break;
case Token.IN:
valid=maybeValidDerefType(n.getLastChild());
break;
default :
valid=true;
break;
}
return valid ? ConformanceResult.CONFORMANCE : ConformanceResult.VIOLATION;
}
