{
  if (n.getType() == Token.NAME) {
    String name=n.getString();
    Scope.Var var=t.getScope().getVar(name);
    if (var != null && var.isLocal()) {
      return;
    }
    Symbol global=globals.get(name);
    if (global != null) {
      boolean isFirst=parent.getFirstChild() == n;
      if ((NodeUtil.isAssignmentOp(parent) && isFirst) || (parent.getType() == Token.NEW && isFirst) || parent.getType() == Token.INC || parent.getType() == Token.DEC) {
        global.recordMutator(t);
      }
 else {
        global.recordAccessor(t);
      }
      globalUses.add(n);
    }
  }
}
