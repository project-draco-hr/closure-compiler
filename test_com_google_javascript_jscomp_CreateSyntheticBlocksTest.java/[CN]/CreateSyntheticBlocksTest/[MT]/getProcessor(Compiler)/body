{
  return new CompilerPass(){
    @Override public void process(    Node externs,    Node js){
      new CreateSyntheticBlocks(compiler,START_MARKER,END_MARKER).process(externs,js);
      NodeTraversal.traverseEs6(compiler,js,new MinimizeExitPoints(compiler));
      new PeepholeOptimizationsPass(compiler,new PeepholeRemoveDeadCode(),new PeepholeMinimizeConditions(true),new PeepholeFoldConstants(true)).process(externs,js);
      new MinimizeExitPoints(compiler).process(externs,js);
      new Denormalize(compiler).process(externs,js);
    }
  }
;
}
