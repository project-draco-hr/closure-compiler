{
  validateNodeType(Token.ARRAY_PATTERN,n);
  for (Node c=n.getFirstChild(); c != null; c=c.getNext()) {
    if (c == n.getLastChild() && NodeUtil.isNameDeclaration(n.getParent())) {
      validateExpression(c);
    }
 else     if (c.isRest()) {
      validateRest(c);
    }
 else     if (c.isEmpty()) {
      validateChildless(c);
    }
 else {
      validateNameDeclarationChild(type,c);
    }
  }
}
