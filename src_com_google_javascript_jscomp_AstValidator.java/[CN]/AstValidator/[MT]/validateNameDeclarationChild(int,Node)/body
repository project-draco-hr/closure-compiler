{
  if (n.isName()) {
    validateNonEmptyString(n);
    validateMaximumChildCount(n,1);
    if (n.hasChildren()) {
      validateExpression(n.getFirstChild());
    }
  }
 else   if (n.isArrayPattern()) {
    validateArrayPattern(type,n);
  }
 else {
    violation("Invalid child for " + Token.name(type) + " node",n);
  }
}
