{
switch (n.getType()) {
case Token.VAR:
case Token.LET:
case Token.CONST:
    validateNameDeclarationHelper(n.getType(),n);
  break;
case Token.FUNCTION:
validateFunctionSignature(n);
break;
case Token.CLASS:
validateClassDeclaration(n,true);
break;
case Token.ENUM:
validateEnum(n);
break;
case Token.NAMESPACE:
validateNamespace(n,true);
break;
case Token.TYPE_ALIAS:
validateTypeAlias(n);
break;
case Token.EXPORT:
validateExport(n,true);
break;
}
}
