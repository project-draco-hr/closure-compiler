{
switch (n.getType()) {
case VAR:
case LET:
case CONST:
    validateNameDeclarationHelper(n.getType(),n);
  break;
case FUNCTION:
validateFunctionSignature(n);
break;
case CLASS:
validateClassDeclaration(n,true);
break;
case ENUM:
validateEnum(n);
break;
case NAMESPACE:
validateNamespace(n,true);
break;
case TYPE_ALIAS:
validateTypeAlias(n);
break;
case EXPORT:
validateExport(n,true);
break;
}
}
