{
  if (n.getType() != Token.MEMBER_DEF && n.getType() != Token.GETTER_DEF && n.getType() != Token.SETTER_DEF) {
    violation("Class contained member which was not a MEMBER_DEF, GETTER_DEF, or SETTER_DEF",n);
  }
  validateChildCount(n,Token.arity(n.getType()));
  validateFunctionExpression(n.getFirstChild());
}
