{
  if (!expression.isName()) {
    addNewWarning("msg.jsdoc.typetransformation.invalid.typevar",expression);
    return false;
  }
  if (!validTypeTransformationName(expression.getString())) {
    addNewWarning("msg.jsdoc.typetransformation.invalid.typevar",expression);
    return false;
  }
  return true;
}
