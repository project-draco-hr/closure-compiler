{
  if (!expression.isCall()) {
    addNewWarning("msg.jsdoc.typetransformation.invalid.expression","none",expression);
    return false;
  }
  String keyword=expression.getFirstChild().getString();
  if (!isKeyword(keyword,Keywords.NONE)) {
    addNewWarning("msg.jsdoc.typetransformation.invalid.expression","none",expression);
    return false;
  }
  if (expression.getChildCount() > 1) {
    addNewWarning("msg.jsdoc.typetransformation.extra.param","none",expression);
    return false;
  }
  return true;
}
