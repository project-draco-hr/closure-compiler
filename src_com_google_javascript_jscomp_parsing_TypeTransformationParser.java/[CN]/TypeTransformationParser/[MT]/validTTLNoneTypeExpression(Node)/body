{
  if (!isOperation(expr)) {
    warnInvalidExpression("none",expr);
    return false;
  }
  String keyword=expr.getFirstChild().getString();
  if (!isKeyword(keyword,Keywords.NONE)) {
    warnInvalidExpression("none",expr);
    return false;
  }
  if (expr.getChildCount() > 1) {
    warnExtraParam("none",expr);
    return false;
  }
  return true;
}
