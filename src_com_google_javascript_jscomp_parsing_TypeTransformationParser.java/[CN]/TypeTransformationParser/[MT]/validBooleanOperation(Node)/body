{
  boolean valid;
  if (expr.isNot()) {
    valid=validBooleanExpression(expr.getFirstChild());
  }
 else {
    valid=validBooleanExpression(expr.getFirstChild()) && validBooleanExpression(expr.getChildAtIndex(1));
  }
  if (!valid) {
    warnInvalidInside("boolean",expr);
    return false;
  }
  return true;
}
