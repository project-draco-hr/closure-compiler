{
  this.allowNonGlobalExports=false;
  test("var BAR;var FOO = {};\n/** @export */FOO.test = BAR = 5",null,FindExportableNodes.NON_GLOBAL_ERROR);
  this.allowNonGlobalExports=true;
  test("var BAR;var FOO = {};\n/** @export */FOO.test = BAR = 5",null,FindExportableNodes.EXPORT_ANNOTATION_NOT_ALLOWED);
}
