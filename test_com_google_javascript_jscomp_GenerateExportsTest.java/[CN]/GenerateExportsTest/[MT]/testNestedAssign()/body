{
  this.allowNonGlobalExports=false;
  testError("var BAR;var FOO = {};\n/** @export */FOO.test = BAR = 5",FindExportableNodes.NON_GLOBAL_ERROR);
  this.allowNonGlobalExports=true;
  testError("var BAR;var FOO = {};\n/** @export */FOO.test = BAR = 5",FindExportableNodes.EXPORT_ANNOTATION_NOT_ALLOWED);
}
