{
  Preconditions.checkNotNull(supplier);
  Set<String> result=new HashSet<String>();
  try {
    for (    String line : CharStreams.readLines(supplier)) {
      line=line.trim();
      if (line.isEmpty() || line.charAt(0) == '#') {
        continue;
      }
      result.add(line);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return result;
}
