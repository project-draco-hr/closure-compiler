{
  Node typeExpr=parseTypeExpression(token);
  if (typeExpr != null) {
    if (match(JsDocToken.PIPE)) {
      next();
      if (match(JsDocToken.PIPE)) {
        next();
      }
      skipEOLs();
      token=next();
      return parseUnionTypeWithAlternate(token,typeExpr);
    }
  }
  return typeExpr;
}
