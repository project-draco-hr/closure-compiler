{
  if (token == JsDocToken.LC) {
    skipEOLs();
    Node typeNode=parseTopLevelTypeExpression(next());
    if (typeNode != null) {
      skipEOLs();
      if (!match(JsDocToken.RC)) {
        reportTypeSyntaxWarning("msg.jsdoc.missing.rc");
      }
 else {
        next();
      }
    }
    return typeNode;
  }
 else {
    return parseTypeExpression(token);
  }
}
