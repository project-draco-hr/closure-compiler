{
  A=new Counter();
  B=new Counter();
  C=new Counter();
  D=new Counter();
  E=new Counter();
  graph=new LinkedDirectedGraph<Counter,String>();
  graph.createDirectedGraphNode(A);
  graph.createDirectedGraphNode(B);
  graph.createDirectedGraphNode(C);
  graph.createDirectedGraphNode(D);
  graph.createDirectedGraphNode(E);
  graph.connect(A,"->",B);
  graph.connect(A,"->",C);
  graph.connect(A,"->",D);
  graph.connect(B,"->",D);
  graph.connect(C,"->",E);
  graph.connect(D,"->",E);
  graph.connect(E,"->",D);
}
