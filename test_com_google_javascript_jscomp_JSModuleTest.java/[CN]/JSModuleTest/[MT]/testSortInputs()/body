{
  CompilerInput a=new CompilerInput(JSSourceFile.fromCode("a.js","goog.require('b');goog.require('c')"));
  CompilerInput b=new CompilerInput(JSSourceFile.fromCode("b.js","goog.provide('b');goog.require('d')"));
  CompilerInput c=new CompilerInput(JSSourceFile.fromCode("c.js","goog.provide('c');goog.require('d')"));
  CompilerInput d=new CompilerInput(JSSourceFile.fromCode("d.js","goog.provide('d')"));
  CompilerInput e=new CompilerInput(JSSourceFile.fromCode("e.js","goog.provide('e')"));
  CompilerInput f=new CompilerInput(JSSourceFile.fromCode("f.js","goog.provide('f')"));
  assertSortedInputs(ImmutableList.of(d,b,c,a),ImmutableList.of(a,b,c,d));
  assertSortedInputs(ImmutableList.of(d,b,c,a),ImmutableList.of(d,b,c,a));
  assertSortedInputs(ImmutableList.of(d,c,b,a),ImmutableList.of(d,c,b,a));
  assertSortedInputs(ImmutableList.of(d,b,c,a),ImmutableList.of(d,a,b,c));
}
