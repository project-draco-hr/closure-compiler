{
switch (n.getType()) {
case CLASS:
    if (needsInnerNameRewriting(n,parent)) {
      classStack.removeFirst();
      n.replaceChild(n.getFirstChild(),IR.empty().useSourceInfoFrom(n.getFirstChild()));
      compiler.reportCodeChange();
    }
  break;
case NAME:
maybeUpdateClassSelfRef(t,n,parent);
break;
default :
break;
}
}
