{
  parse("@type {bar::::x} */","Bad type annotation. expected closing }");
  parse("@type {::x} */","Bad type annotation. type not recognized due to syntax error");
  parse("@type {x::.<a>} */","Bad type annotation. expected closing }");
  parse("@type {'x:y'::z} */","Bad type annotation. expected closing }");
  parse("@type {\"x.y\"::z} */","Bad type annotation. expected closing }");
}
