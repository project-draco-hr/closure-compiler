{
  TestErrorReporter errorReporter=new TestErrorReporter(null,warnings);
  Config config=new Config(extraAnnotations,extraSuppressions,parseDocumentation,false,false);
  JsDocInfoParser jsdocParser=new JsDocInfoParser(stream(comment),new Comment(0,0,CommentType.JSDOC,comment),"testcode",config,errorReporter);
  if (fileLevelJsDocBuilder != null) {
    jsdocParser.setFileLevelJsDocBuilder(fileLevelJsDocBuilder);
  }
  jsdocParser.parse();
  assertTrue("expected warnings were not reported",errorReporter.hasEncounteredAllWarnings());
  if (parseFileOverview) {
    return jsdocParser.getFileOverviewJSDocInfo();
  }
 else {
    return jsdocParser.retrieveAndResetParsedJSDocInfo();
  }
}
