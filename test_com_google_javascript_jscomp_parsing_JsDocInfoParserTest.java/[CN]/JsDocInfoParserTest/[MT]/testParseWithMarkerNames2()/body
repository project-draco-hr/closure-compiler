{
  JSDocInfo jsdoc=parse("@param {SomeType} name somedescription \n" + "* @param {AnotherType} anothername des */",true);
  assertTypeInMarker(assertNameInMarker(assertAnnotationMarker(jsdoc,"param",0,0,0),"name",18),"SomeType",7,true);
  assertTypeInMarker(assertNameInMarker(assertAnnotationMarker(jsdoc,"param",1,2,1),"anothername",23),"AnotherType",9,true);
}
