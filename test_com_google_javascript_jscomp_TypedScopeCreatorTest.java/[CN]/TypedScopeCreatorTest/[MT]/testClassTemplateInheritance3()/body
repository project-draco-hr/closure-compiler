{
  testSame("/**\n" + " * @constructor\n" + " * @classTemplate T\n"+ " */\n"+ "function C() {\n"+ "  /** @type {T} */\n"+ "  this.foo;"+ "};\n"+ ""+ "/**\n"+ " * @constructor\n"+ " * @classTemplate T\n"+ " * @extends {C}"+ " */\n"+ "function D() {\n"+ "  /** @type {T} */\n"+ "  this.bar;"+ "};\n"+ ""+ "/** @type {D.<boolean>} */ var x = new D();\n"+ "var result1 = x.foo;\n"+ "var result2 = x.bar;\n");
  assertEquals("?",findNameType("result1",globalScope).toString());
  assertEquals("boolean",findNameType("result2",globalScope).toString());
}
