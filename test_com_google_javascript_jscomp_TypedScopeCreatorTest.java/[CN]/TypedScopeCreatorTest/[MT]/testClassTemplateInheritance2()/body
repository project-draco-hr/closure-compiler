{
  testSame("/**\n" + " * @constructor\n" + " * @classTemplate T\n"+ " */\n"+ "function C() {};\n"+ ""+ "/** @return {T} */\n"+ "C.prototype.method1 = function() {}\n"+ ""+ "/**\n"+ " * @constructor\n"+ " * @classTemplate T, U\n"+ " * @extends {C.<U>}"+ " */\n"+ "function D() {};\n"+ ""+ "/** @return {T} */\n"+ "D.prototype.method2 = function() {}\n"+ ""+ "/** @type {D.<boolean, string>} */ var x = new D();\n"+ "var result1 = x.method1();\n"+ "var result2 = x.method2();\n");
  assertEquals("string",findNameType("result1",globalScope).toString());
  assertEquals("boolean",findNameType("result2",globalScope).toString());
}
