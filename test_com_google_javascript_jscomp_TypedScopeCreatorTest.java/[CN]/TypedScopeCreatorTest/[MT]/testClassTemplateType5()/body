{
  testSame("/**\n" + " * @constructor\n" + " * @classTemplate T\n"+ " */\n"+ "function C() {\n"+ "};\n"+ ""+ "/** @type {T} */"+ "C.prototype.foo;\n"+ ""+ "/** @type {C.<string>} */ var x = new C();\n"+ "var result = x.foo;\n");
  assertEquals("string",findNameType("result",globalScope).toString());
}
