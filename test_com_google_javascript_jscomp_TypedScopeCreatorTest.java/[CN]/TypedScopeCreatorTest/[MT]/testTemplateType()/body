{
  testSame("/**\n" + " * @param {function(this:T, ...)} fn\n" + " * @param {T} thisObj\n"+ " * @template T\n"+ " */\n"+ "function bind(fn, thisObj) {}"+ "/** @constructor */\n"+ "function Foo() {}\n"+ "/** @return {number} */\n"+ "Foo.prototype.baz = function() {};\n"+ "bind(function() { var f = this.baz(); }, new Foo());");
  assertEquals("number",findNameType("f",lastLocalScope).toString());
}
