{
  testSame("/**\n" + " * @constructor\n" + " * @classTemplate T\n"+ " */\n"+ "function C() {};\n"+ ""+ "/** @return {T} */\n"+ "C.prototype.method = function() {}\n"+ ""+ "/**\n"+ " * @constructor\n"+ " * @classTemplate T, U\n"+ " * @extends {C.<U>}"+ " */\n"+ "function D() {};\n"+ ""+ "/** @override */\n"+ "D.prototype.method = function() {}\n"+ ""+ "/** @type {D.<boolean, string>} */ var x = new D();\n"+ "var result = x.method();\n");
  assertEquals("string",findNameType("result",globalScope).toString());
}
