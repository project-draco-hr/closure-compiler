{
  List<Integer> tokens=ImmutableList.of(ANNOTATION,LC,BANG,STRING,RC,EOC,EOF);
  List<String> strings=ImmutableList.of("param","Object");
  testJSDocTokenStream("@param {!Object}*/",tokens,strings);
  testJSDocTokenStream(" @param {!Object}*/",tokens,strings);
  testJSDocTokenStream("@param {! Object}*/",tokens,strings);
  testJSDocTokenStream("@param { !Object}*/",tokens,strings);
  testJSDocTokenStream("@param  {!Object  } */",tokens,strings);
  testJSDocTokenStream("@param {  ! Object  }*/",tokens,strings);
  testJSDocTokenStream("@param {!Object  }*/",tokens,strings);
}
