{
  testSame("while(1) { foo(); break }");
  testSame("while(1) { foo(); continue }");
  testSame("for(;;) { foo(); break }");
  testSame("for(;;) { foo(); continue }");
  testSame("for(;;) { if (x) { break } }");
  testSame("for(;;) { if (x) { continue } }");
  testSame("do { foo(); continue} while(1)");
}
