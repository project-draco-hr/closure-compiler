{
  testSame("if (true) {}");
  testSame("while (true) {}");
  testSame("do { things; } while (true);");
  testSame("switch (n) { case 3: alert(4); }");
  testSame("switch (n) { case 5: { alert(6); } }");
  testSame("alert(1); LABEL: while (true) { alert(2); }");
  testSame("for (;;) {}");
  testSameEs6("for (x of y) {}");
  testSame("for (x in y) {}");
}
