{
  if (!isInterface(n)) {
    return;
  }
  Node args=n.getFirstChild().getNext();
  if (args.hasChildren()) {
    t.report(args.getFirstChild(),INTERFACE_SHOULD_NOT_TAKE_ARGS);
  }
  Node block=n.getLastChild();
  if (block.hasChildren()) {
    t.report(block.getFirstChild(),INTERFACE_FUNCTION_NOT_EMPTY);
  }
}
