{
  graph.createNode("a");
  graph.createNode("b");
  graph.createNode("c");
  graph.createNode("d");
  assertEquals(0,graph.getNodeDegree("a"));
  graph.connect("a","-","b");
  assertEquals(1,graph.getNodeDegree("a"));
  graph.connect("b","-","c");
  assertEquals(1,graph.getNodeDegree("a"));
  graph.connect("a","-","c");
  assertEquals(2,graph.getNodeDegree("a"));
  graph.connect("d","-","a");
  assertEquals(3,graph.getNodeDegree("a"));
}
