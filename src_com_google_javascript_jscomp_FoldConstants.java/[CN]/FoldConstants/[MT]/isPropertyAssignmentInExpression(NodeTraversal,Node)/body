{
  final boolean[] found={false};
  new NodeTraversal(t.getCompiler(),new AbstractShallowCallback(){
    public void visit(    NodeTraversal t,    Node n,    Node parent){
      found[0]|=(n.getType() == Token.GETPROP && parent.getType() == Token.ASSIGN);
    }
  }
).traverse(n);
  return found[0];
}
