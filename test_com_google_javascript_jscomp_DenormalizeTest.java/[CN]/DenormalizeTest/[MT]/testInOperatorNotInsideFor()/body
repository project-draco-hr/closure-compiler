{
  testSame("function(){ var a; var i=\"length\" in a;" + "for(; a < 2 ; a++) foo() }");
  testSame("function(){ var a; var i=(\"length\" in a);" + "for(; a < 2 ; a++) foo() }");
  test("function(){var b,a=0; for (var i=(\"length\" in b);a<2; a++) foo()}","function(){var b; var a=0;var i=(\"length\" in b);" + "for (;a<2;a++) foo()}");
}
