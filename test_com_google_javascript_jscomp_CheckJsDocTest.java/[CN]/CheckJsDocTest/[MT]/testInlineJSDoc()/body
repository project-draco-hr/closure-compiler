{
  testSame("function f(/** string */ x) {}");
  testSame("function f(/** @type {string} */ x) {}");
  testSame("var /** string */ x = 'x';");
  testSame("var /** @type {string} */ x = 'x';");
  testSame("var /** string */ x, /** number */ y;");
  testSame("var /** @type {string} */ x, /** @type {number} */ y;");
}
