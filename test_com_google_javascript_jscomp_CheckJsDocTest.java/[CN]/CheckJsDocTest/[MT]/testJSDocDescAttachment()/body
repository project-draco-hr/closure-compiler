{
  testSameWithWarning("function f() { " + "  return /** @type {string} */ (g(1 /** @desc x */));" + "};",MISPLACED_ANNOTATION);
  testSameWithWarning("/** @desc Foo. */ var bar = goog.getMsg('hello');",MISPLACED_ANNOTATION);
  testSameWithWarning("/** @desc Foo. */ x.y.z.bar = goog.getMsg('hello');",MISPLACED_ANNOTATION);
  testSameWithWarning("var msgs = {/** @desc x */ x: goog.getMsg('x')}",MISPLACED_ANNOTATION);
  testSameWithWarning("/** @desc Foo. */ bar = goog.getMsg('x');",MISPLACED_ANNOTATION);
}
