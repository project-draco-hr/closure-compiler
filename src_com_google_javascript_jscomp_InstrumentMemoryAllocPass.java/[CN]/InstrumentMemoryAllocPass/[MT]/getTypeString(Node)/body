{
  if (currentNode.getType() == Token.NEW) {
    JSType type=currentNode.getFirstChild().getJSType();
    String typeName=(type != null) ? type.getDisplayName() : "Unknown";
    return IR.string("new " + typeName);
  }
  return currentNode.getType() == Token.ARRAYLIT ? IR.string("Array") : currentNode.getType() == Token.OBJECTLIT ? IR.string("Object") : currentNode.getType() == Token.FUNCTION ? IR.string("Function") : IR.string("Unknown");
}
