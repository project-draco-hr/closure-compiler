{
  if (max == Integer.MAX_VALUE) {
switch (min) {
case 0:
      return 1;
case 1:
    return 1;
default :
  return 3 + numDecimalDigits(min);
}
}
if (min == 0 && max == 1) {
return 1;
}
if (min == max) {
if (min == 1) {
return 0;
}
return 2 + numDecimalDigits(min);
}
return 3 + numDecimalDigits(min) + numDecimalDigits(max);
}
