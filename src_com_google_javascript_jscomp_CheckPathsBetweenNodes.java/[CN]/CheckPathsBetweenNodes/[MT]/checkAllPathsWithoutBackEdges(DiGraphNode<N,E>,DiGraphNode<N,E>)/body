{
  if (nodePredicate.apply(a.getValue())) {
    return true;
  }
  if (a == b) {
    return false;
  }
  for (  DiGraphEdge<N,E> e : a.getOutEdges()) {
    if (ignoreEdge(e)) {
      continue;
    }
    if (e.getAnnotation() == BACK_EDGE) {
      continue;
    }
    DiGraphNode<N,E> next=e.getDestination();
    if (!checkAllPathsWithoutBackEdges(next,b)) {
      return false;
    }
  }
  return true;
}
