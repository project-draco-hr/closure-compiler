{
switch (k) {
case 0:
    add(ByteCode.ICONST_0);
  break;
case 1:
add(ByteCode.ICONST_1);
break;
case 2:
add(ByteCode.ICONST_2);
break;
case 3:
add(ByteCode.ICONST_3);
break;
case 4:
add(ByteCode.ICONST_4);
break;
case 5:
add(ByteCode.ICONST_5);
break;
default :
add(ByteCode.LDC,itsConstantPool.addConstant(k));
break;
}
}
