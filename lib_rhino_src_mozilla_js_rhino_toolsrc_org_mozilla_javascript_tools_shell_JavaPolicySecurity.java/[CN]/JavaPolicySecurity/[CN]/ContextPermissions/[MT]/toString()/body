{
  StringBuffer sb=new StringBuffer();
  sb.append(getClass().getName());
  sb.append('@');
  sb.append(Integer.toHexString(System.identityHashCode(this)));
  sb.append(" (context=");
  sb.append(_context);
  sb.append(", static_permitions=");
  sb.append(_statisPermissions);
  sb.append(')');
  return sb.toString();
}
