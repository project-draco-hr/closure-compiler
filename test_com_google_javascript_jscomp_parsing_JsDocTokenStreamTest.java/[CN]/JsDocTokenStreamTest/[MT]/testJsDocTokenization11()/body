{
  List<JsDocToken> tokens=ImmutableList.of(ANNOTATION,LC,STRING,QMARK,RC,EOC,EOF);
  List<String> strings=ImmutableList.of("param","string");
  testJSDocTokenStream("@param {string?}*/",tokens,strings);
  testJSDocTokenStream(" @param {string?}*/",tokens,strings);
  testJSDocTokenStream("@param { string?}*/",tokens,strings);
  testJSDocTokenStream("@param {string ?}*/",tokens,strings);
  testJSDocTokenStream("@param  {string ?  } */",tokens,strings);
  testJSDocTokenStream("@param { string  ?  }*/",tokens,strings);
  testJSDocTokenStream("@param {string?  }*/",tokens,strings);
}
