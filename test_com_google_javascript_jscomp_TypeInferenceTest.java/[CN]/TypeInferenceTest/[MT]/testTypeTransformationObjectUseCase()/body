{
  inFunction("/** \n" + " * @param {T} a\n" + " * @return {R}\n"+ " * @template T \n"+ " * @template R := \n"+ " * mapunion(T, (x) => \n"+ " *      cond(eq(x, type('string')), type('String'),\n"+ " *      cond(eq(x, type('number')), type('Number'),\n"+ " *      cond(eq(x, type('boolean')), type('Boolean'),\n"+ " *      cond(eq(x, type('null')), type('Object'), \n"+ " *      cond(eq(x, type('undefined')), type('Object'),\n"+ " *      x)))))) \n"+ " * =:\n"+ " */\n"+ "function Object(a) {}\n"+ "/** @type {(string|number|boolean)} */\n"+ "var o;\n"+ "var r = Object(o);");
  verify("r",createMultiParamUnionType(STRING_OBJECT_TYPE,NUMBER_OBJECT_TYPE,JSTypeNative.BOOLEAN_OBJECT_TYPE));
}
