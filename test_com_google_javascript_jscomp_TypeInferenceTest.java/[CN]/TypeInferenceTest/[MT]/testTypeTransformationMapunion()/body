{
  inFunction("/**\n" + " * @param {U} a\n" + " * @return {R}\n"+ " * @template U\n"+ " * @template R := mapunion(U, "+ "(x) => cond(eq(x, type('string')), "+ "type('boolean'), "+ "type('null'))) =:\n"+ " */\n"+ "function f(a) {}\n"+ "/** @type {string|number} */ var x;"+ "var result = f(x);");
  verify("result",createUnionType(BOOLEAN_TYPE,NULL_TYPE));
}
