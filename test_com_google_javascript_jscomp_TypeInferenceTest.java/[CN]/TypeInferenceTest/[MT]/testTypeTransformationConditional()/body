{
  inFunction("/**\n" + " * @param {T} a\n" + " * @param {N} b\n"+ " * @return {R}\n"+ " * @template T, N\n"+ " * @template R := cond( eq(T, N), type('string'), type('boolean') ) =:\n"+ " */\n"+ "function f(a, b){}\n"+ "var result = f(1, 2);"+ "var result2 = f(1, 'a');");
  verify("result",STRING_TYPE);
  verify("result2",BOOLEAN_TYPE);
}
