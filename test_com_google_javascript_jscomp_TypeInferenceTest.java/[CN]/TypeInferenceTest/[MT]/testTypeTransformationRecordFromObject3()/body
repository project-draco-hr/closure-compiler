{
  inFunction("/** \n" + " * @param {T} a\n" + " * @return {R}\n"+ " * @template T \n"+ " * @template R := record(T) =:"+ " */\n"+ "function f(a) {}\n"+ "/** @type {{foo:{baz:!Object}, bar:!Object}} */"+ "var e;"+ "/** @type {!Object.<?,?>} */"+ "var something;"+ "var r = f({foo:{baz:{}}, bar:something});");
  assertTrue(getType("r").isRecordType());
  verify("r",getType("e"));
}
