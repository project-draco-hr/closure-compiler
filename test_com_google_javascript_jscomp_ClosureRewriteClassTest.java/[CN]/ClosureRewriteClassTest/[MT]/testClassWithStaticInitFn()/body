{
  test("x.y = goog.defineClass(some.Super, {\n" + "  constructor: function(){\n" + "    this.foo = 1;\n"+ "  },\n"+ "  statics: function(cls) {\n"+ "    cls.prop1 = 1;\n"+ "    /** @const */\n"+ "    cls.PROP2 = 2;\n"+ "  },\n"+ "  anotherProp: 1,\n"+ "  aMethod: function() {}\n"+ "});",Joiner.on('\n').join("/** @constructor */","x.y = function() { this.foo = 1; };","goog.inherits(x.y, some.Super);","x.y.prototype.anotherProp = 1;","x.y.prototype.aMethod = function() {};","(function(cls) {","  x.y.prop1 = 1;","  /** @const */","  x.y.PROP2 = 2;","})(x.y);"));
}
