{
  testError("var x = goog.defineClass(null, {" + "  constructor: function(){}," + "  statics: null"+ "});",GOOG_CLASS_STATICS_NOT_VALID);
  testError("var x = goog.defineClass(null, {" + "  constructor: function(){}," + "  statics: foo"+ "});",GOOG_CLASS_STATICS_NOT_VALID);
  testError("var x = goog.defineClass(null, {" + "  constructor: function(){}," + "  statics: {'foo': 1}"+ "});",GOOG_CLASS_STATICS_NOT_VALID);
  testError("var x = goog.defineClass(null, {" + "  constructor: function(){}," + "  statics: {1: 1}"+ "});",GOOG_CLASS_STATICS_NOT_VALID);
}
