{
  if (capos < fl.getLp()) {
    return new NodePos(fl,flpos);
  }
  NodePos res=attachComment(fl.getInitializer(),flpos);
  if (res != null) {
    return res;
  }
  res=attachComment(fl.getCondition(),flpos);
  if (res != null) {
    return res;
  }
  res=attachComment(fl.getIncrement(),flpos);
  if (res != null) {
    return res;
  }
  if (capos < fl.getRp()) {
    return new NodePos(fl,flpos);
  }
  return null;
}
