{
  int ntype=n.getType();
  if (betweenJsdocAndCast(n)) {
    if (!attachOnlyToParen) {
      attachOnlyToParen=true;
      possibleJsdocTarget=n;
      possibleApos=napos;
    }
    return true;
  }
  if (ntype == Token.SCRIPT || ntype == Token.HOOK || ntype == Token.RETURN || n instanceof ExpressionStatement || n instanceof InfixExpression) {
    return true;
  }
  return false;
}
