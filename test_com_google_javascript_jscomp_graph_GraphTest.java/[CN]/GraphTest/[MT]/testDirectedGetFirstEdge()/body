{
  DiGraph<String,String> graph=LinkedDirectedGraph.create();
  graph.createNode("a");
  graph.createNode("b");
  graph.createNode("c");
  graph.connect("a","-","b");
  assertThat("-").isEqualTo(graph.getFirstEdge("a","b").getValue());
  assertThat("-").isEqualTo(graph.getFirstEdge("b","a").getValue());
  assertNull(graph.getFirstEdge("a","c"));
}
