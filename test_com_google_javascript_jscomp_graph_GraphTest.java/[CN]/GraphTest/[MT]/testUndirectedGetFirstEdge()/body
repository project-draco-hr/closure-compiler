{
  UndiGraph<String,String> graph=LinkedUndirectedGraph.create();
  graph.createNode("a");
  graph.createNode("b");
  graph.createNode("c");
  graph.connect("a","-","b");
  assertEquals(graph.getFirstEdge("a","b").getValue(),"-");
  assertEquals(graph.getFirstEdge("b","a").getValue(),"-");
  assertNull(graph.getFirstEdge("a","c"));
}
