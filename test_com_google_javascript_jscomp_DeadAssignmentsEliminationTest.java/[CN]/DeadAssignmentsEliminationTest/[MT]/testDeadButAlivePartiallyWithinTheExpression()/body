{
  inFunction("var x; x = 100, print(x), x = 101;","var x; x = 100, print(x),     101;");
  inFunction("var x; x = 100, print(x), print(x), x = 101;","var x; x = 100, print(x), print(x),     101;");
  inFunction("var x; x = 100, print(x), x = 0, print(x), x = 101;","var x; x = 100, print(x), x = 0, print(x),     101;");
}
