{
  inFunction("var a; a = 1, a++; a");
  inFunction("var a; a = 1, ++a; a");
  inFunction("var a; a = 1, a--; a");
  inFunction("var a; a = 1, --a; a");
  inFunction("var a; a = 1, a++, print(a)");
  inFunction("var a; a = 1, ++a, print(a)");
  inFunction("var a; a = 1, a--, print(a)");
  inFunction("var a; a = 1, --a, print(a)");
  inFunction("var a; a = 1, print(a++)");
  inFunction("var a; a = 1, print(++a)");
  inFunction("var a; a = 1, print(a++)");
  inFunction("var a; a = 1, print(++a)");
  inFunction("var a; a = 1, print(a--)");
  inFunction("var a; a = 1, print(--a)");
}
