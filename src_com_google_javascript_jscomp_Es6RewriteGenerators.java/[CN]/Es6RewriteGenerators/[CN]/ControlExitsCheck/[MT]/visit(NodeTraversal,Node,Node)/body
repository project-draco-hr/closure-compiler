{
switch (n.getType()) {
case Token.DO:
case Token.WHILE:
case Token.FOR:
    continueCatchers--;
  breakCatchers--;
break;
case Token.SWITCH:
breakCatchers--;
break;
case Token.BLOCK:
parent=n.getParent();
if (parent != null && parent.isTry() && parent.getFirstChild() == n && n.getNext().hasChildren()) {
throwCatchers--;
}
break;
}
}
