{
switch (n.getType()) {
case Token.YIELD:
    visitYieldExpression(n);
  break;
case Token.DO:
case Token.FOR:
case Token.WHILE:
visitLoop(n);
break;
case Token.CASE:
if (controlCanExit(n.getFirstChild())) {
compiler.report(JSError.make(currentStatement,Es6ToEs3Converter.CANNOT_CONVERT_YET,"Case statements that contain yields"));
return false;
}
break;
}
return true;
}
