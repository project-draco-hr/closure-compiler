{
  if (!currentLoopContinueStatement.peekFirst().isEmpty()) {
    enclosingCase.getLastChild().addChildToBack(currentLoopContinueStatement.peekFirst().cloneTree());
  }
  enclosingCase.getLastChild().addChildToBack(createStateUpdate(currentLoopEndCase.peekFirst() - 1));
  enclosingCase.getLastChild().addChildToBack(createSafeBreak());
}
