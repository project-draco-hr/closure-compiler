{
switch (n.getType()) {
case Token.FUNCTION:
    if (n.isGeneratorFunction()) {
      generatorCaseCount=0;
      visitGenerator(n,parent);
    }
  break;
case Token.YIELD:
if (!n.getParent().isExprResult()) {
  compiler.report(JSError.make(currentStatement,Es6ToEs3Converter.CANNOT_CONVERT_YET,"Yield may only be used as a statement."));
}
break;
}
}
