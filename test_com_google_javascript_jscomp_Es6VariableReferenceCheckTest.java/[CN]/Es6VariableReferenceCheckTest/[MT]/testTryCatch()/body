{
  assertRedeclareError(Joiner.on('\n').join("function f() {","  try {","    let e = 0;","    if (true) {","      let e = 1;","    }","  } catch (e) {","    let e;","  }","}"));
  assertRedeclareError(Joiner.on('\n').join("function f() {","  try {","    let e = 0;","    if (true) {","      let e = 1;","    }","  } catch (e) {","      var e;","  }","}"));
  assertRedeclareError(Joiner.on('\n').join("function f() {","  try {","    let e = 0;","    if (true) {","      let e = 1;","    }","  } catch (e) {","    function e() {","      var e;","    }","  }","}"));
}
