{
  assertRedeclare(LINE_JOINER.join("if (a) {","  var x;","  var x;","}"));
  assertRedeclareError(LINE_JOINER.join("if (a) {","  var x;","  let x;","}"));
  assertRedeclare(LINE_JOINER.join("function f() {","  var x;","  if (a) {","    var x;","  }","}"));
  assertRedeclareError(LINE_JOINER.join("function f() {","  if (a) {","    var x;","  }","  let x;","}"));
  assertNoWarning(LINE_JOINER.join("function f() {","  var x;","  if (a) {","    let x;","  }","}"));
  assertNoWarning(LINE_JOINER.join("function f() {","  if (a) {","    var x;","  }","  if (b) {","    let x;","  }","}"));
}
