{
  Node node=new Node(Token.CONTINUE);
  Scope localScope=context.scopeManager.localScope();
  double toLabel=getOwnConfig().get("toLabel").getAsDouble();
  if (budget > 1 && !localScope.loopLabels.isEmpty() && context.random.nextDouble() < toLabel) {
    node.addChildToBack(Node.newString(Token.LABEL_NAME,localScope.randomLabelForContinue(context.random)));
  }
  return node;
}
