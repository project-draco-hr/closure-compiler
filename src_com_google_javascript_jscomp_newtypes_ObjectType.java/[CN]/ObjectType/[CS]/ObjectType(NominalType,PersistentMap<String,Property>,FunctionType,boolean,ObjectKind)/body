{
  Preconditions.checkArgument(fn == null || fn.isLoose() == isLoose,"isLoose: %s, fn: %s",isLoose,fn);
  Preconditions.checkArgument(nominalType == null || !isLoose);
  Preconditions.checkArgument(nominalType == null || fn == null,"Cannot create object of %s that is callable",nominalType);
  this.nominalType=nominalType;
  this.props=props;
  this.fn=fn;
  this.isLoose=isLoose;
  this.objectKind=objectKind;
}
