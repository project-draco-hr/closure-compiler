{
  Preconditions.checkState(areRelatedClasses(obj1.nominalType,obj2.nominalType));
  return ObjectType.makeObjectType(NominalType.pickSuperclass(obj1.nominalType,obj2.nominalType),(obj1.isLoose || obj2.isLoose) ? joinPropsLoosely(obj1.props,obj2.props) : joinProps(obj1.props,obj2.props),FunctionType.join(obj1.fn,obj2.fn),obj1.isLoose || obj2.isLoose);
}
