{
  for (  ObjectType obj1 : objs1) {
    boolean foundSupertype=false;
    for (    ObjectType obj2 : objs2) {
      if (obj1.isSubtypeOf(keepLoosenessOfThis,obj2)) {
        foundSupertype=true;
        break;
      }
    }
    if (!foundSupertype) {
      return false;
    }
  }
  return true;
}
