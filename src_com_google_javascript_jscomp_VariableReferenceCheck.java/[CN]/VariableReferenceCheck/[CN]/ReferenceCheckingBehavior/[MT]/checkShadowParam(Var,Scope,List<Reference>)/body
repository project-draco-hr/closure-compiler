{
  Var maybeParam=functionScope.getVar(v.getName());
  if (maybeParam != null && maybeParam.isParam() && maybeParam.getScope() == functionScope) {
    for (    Reference r : references) {
      if (r.isDeclaration() && !r.getNode().equals(v.getNameNode())) {
        if (!compiler.getLanguageMode().isEs6OrHigher() && r.getParent().isCatch()) {
          continue;
        }
        compiler.report(JSError.make(r.getNode(),(r.isVarDeclaration() || r.isHoistedFunction()) ? REDECLARED_VARIABLE : PARAMETER_SHADOWED_ERROR,v.name));
      }
    }
  }
}
