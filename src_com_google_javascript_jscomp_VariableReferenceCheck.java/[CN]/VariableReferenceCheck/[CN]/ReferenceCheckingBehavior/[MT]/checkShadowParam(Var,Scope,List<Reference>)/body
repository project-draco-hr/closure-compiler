{
  Scope functionScope=scope.getParent();
  Var maybeParam=functionScope.getVar(v.getName());
  if (maybeParam != null && maybeParam.isParam() && maybeParam.getScope() == functionScope) {
    for (    Reference r : references) {
      if (!r.isDeclaration() || r.getScope() != scope) {
        continue;
      }
      compiler.report(JSError.make(r.getNode(),(r.isVarDeclaration() || r.isHoistedFunction()) && !(maybeParam.getNode().getParent().isDefaultValue() || maybeParam.getNode().isRest()) ? REDECLARED_VARIABLE : PARAMETER_SHADOWED_ERROR,v.name));
    }
  }
}
