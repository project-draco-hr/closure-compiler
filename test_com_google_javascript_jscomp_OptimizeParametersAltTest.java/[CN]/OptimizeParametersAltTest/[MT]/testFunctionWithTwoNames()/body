{
  test("var foo = function bar(a,b) {};","var foo = function bar() {var a; var b;}");
  test("var foo = function bar(a,b) {}; foo(1)","var foo = function bar() {var a = 1; var b;}; foo()");
  test("var foo = function bar(a,b) {}; bar(1);","var foo = function bar() {var a = 1; var b;}; bar()");
  test("var foo = function bar(a,b) {}; foo(1); foo(2)","var foo = function bar(a) {var b}; foo(1); foo(2)");
  test("var foo = function bar(a,b) {}; foo(1); bar(1)","var foo = function bar() {var a = 1; var b}; foo(); bar()");
  test("var foo = function bar(a,b) {}; foo(1); bar(2)","var foo = function bar(a) {var b}; foo(1); bar(2)");
  testSame("var foo = function bar(a,b) {}; foo(1,2); bar(2,1)");
}
