{
  if (other == null || other.isQmarkFunction() || !this.isLoose() && other.isLoose()) {
    return this;
  }
  FunctionType result=meet(this,other);
  Preconditions.checkState(isInhabitable(result));
  if (this.isLoose() && !result.isLoose()) {
    result=result.withLoose();
  }
  return result;
}
