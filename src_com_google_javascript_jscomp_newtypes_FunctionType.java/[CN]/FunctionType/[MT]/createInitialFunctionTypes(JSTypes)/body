{
  LinkedHashMap<String,FunctionType> functions=new LinkedHashMap<>();
  functions.put("QMARK_FUNCTION",FunctionType.normalized(commonTypes,null,null,commonTypes.UNKNOWN,commonTypes.UNKNOWN,null,null,null,null,true));
  functions.put("BOTTOM_FUNCTION",FunctionType.normalized(commonTypes,null,null,null,commonTypes.BOTTOM,null,null,null,null,false));
  functions.put("TOP_FUNCTION",new FunctionType(commonTypes,false));
  functions.put("LOOSE_TOP_FUNCTION",new FunctionType(commonTypes,true));
  return functions;
}
