{
  mode=LanguageMode.ECMASCRIPT6;
  expectedFeatures=FeatureSet.ES6;
  parseError("var {foo: bar.x} = baz();","Only an identifier or destructuring pattern is allowed here.");
  parse("({foo: bar.x} = baz());");
  parse("for ({foo: bar.x} in baz());");
  parseError("var {foo: bar[x]} = baz();","Only an identifier or destructuring pattern is allowed here.");
  parse("({foo: bar[x]} = baz());");
  parse("for ({foo: bar[x]} in baz());");
}
