{
  mode=LanguageMode.ECMASCRIPT6;
  expectedFeatures=FeatureSet.ES6;
  parse("var [x=1,y] = foo();");
  parse("[x=1,y] = foo();");
  parse("var [x,y=2] = foo();");
  parse("[x,y=2] = foo();");
  parse("var [[a] = ['b']] = [];");
  parse("[[a] = ['b']] = [];");
  parse("[[a.x] = ['b']] = [];");
  parseError("var [[a.x] = ['b']] = [];","Only an identifier or destructuring pattern is allowed here.");
}
