{
  assertNodeEquality(parse("x * y * z"),parse("(x * y) * z"));
  assertNodeEquality(parse("x + y + z"),parse("(x + y) + z"));
  assertNodeEquality(parse("x | y | z"),parse("(x | y) | z"));
  assertNodeEquality(parse("x & y & z"),parse("(x & y) & z"));
  assertNodeEquality(parse("x ^ y ^ z"),parse("(x ^ y) ^ z"));
  assertNodeEquality(parse("x || y || z"),parse("(x || y) || z"));
  assertNodeEquality(parse("x && y && z"),parse("(x && y) && z"));
}
