{
  expectedFeatures=FeatureSet.ES6_IMPL;
  mode=LanguageMode.ECMASCRIPT6;
  parse("/a/y");
  expectedFeatures=FeatureSet.ES6;
  parse("/a/u");
  mode=LanguageMode.ECMASCRIPT5;
  expectedFeatures=FeatureSet.ES6_IMPL;
  parseWarning("/a/y","this language feature is only supported in es6 mode: RegExp flag 'y'");
  expectedFeatures=FeatureSet.ES6;
  parseWarning("/a/u","this language feature is only supported in es6 mode: RegExp flag 'u'");
  parseWarning("/a/yu","this language feature is only supported in es6 mode: RegExp flag 'y'","this language feature is only supported in es6 mode: RegExp flag 'u'");
}
