{
  mode=LanguageMode.ECMASCRIPT6;
  parseError("for(var a, b of c) d;","for-of statement may not have more than one variable declaration");
  parseError("for(let a, b of c) d;","for-of statement may not have more than one variable declaration");
  parseError("for(const a, b of c) d;","for-of statement may not have more than one variable declaration");
}
