{
  mode=LanguageMode.ECMASCRIPT6;
  expectFeatures(Feature.DESTRUCTURING);
  parse("({x: y} = z);");
  parse("({x: (y)} = z);");
  parse("({x: ((y))} = z);");
  parse("([x] = y);");
  parse("[(x), y] = z;");
  parse("[x, (y)] = z;");
  parseError("[x, ([y])] = z;","invalid assignment target");
  parseError("[x, (([y]))] = z;","invalid assignment target");
}
