{
  if (skipAmbiguating) {
    return;
  }
  ++numOccurrences;
  if (newType.isUnionType()) {
    newType=newType.restrictByNotNullOrUndefined();
    if (newType.isUnionType()) {
      for (      JSType alt : newType.toMaybeUnionType().getAlternates()) {
        addNonUnionType(alt);
      }
      return;
    }
  }
  addNonUnionType(newType);
}
