{
  if (restFormals != null) {
    throw new WrongParameterOrderException("Cannot add optional formal after rest args");
  }
  if (t == null) {
    optionalFormals.add(null);
  }
 else {
    Preconditions.checkArgument(!t.isBottom());
    optionalFormals.add(JSType.join(t,JSType.UNDEFINED));
  }
  return this;
}
