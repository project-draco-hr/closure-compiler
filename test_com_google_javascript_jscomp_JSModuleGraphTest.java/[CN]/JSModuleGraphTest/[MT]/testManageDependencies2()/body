{
  List<CompilerInput> inputs=setUpManageDependenciesTest();
  List<CompilerInput> results=graph.manageDependencies(ImmutableList.of("c2"),inputs);
  assertInputs(A,"a1","a3");
  assertInputs(B,"a2","b2");
  assertInputs(C,"c1","c2");
  assertInputs(E,"e1","e2");
  assertEquals(Lists.newArrayList("a1","a3","a2","b2","c1","c2","e1","e2"),sourceNames(results));
}
