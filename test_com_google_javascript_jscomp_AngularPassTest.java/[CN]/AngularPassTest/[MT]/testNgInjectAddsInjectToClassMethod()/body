{
  setAcceptedLanguage(LanguageMode.ECMASCRIPT6);
  test(LINE_JOINER.join("class FnClass {","  constructor(a, b) {}","  /** @ngInject */ ","  methodA(c, d){}","}"),LINE_JOINER.join("class FnClass {","  constructor(a, b){}","  methodA(c, d){}","}","FnClass.prototype.methodA['$inject'] = ['c','d']"));
}
