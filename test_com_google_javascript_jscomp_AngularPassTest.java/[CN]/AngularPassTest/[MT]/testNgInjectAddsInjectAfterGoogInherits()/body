{
  test("/** @ngInject \n @constructor */ function fn(a, b) {}" + "goog.inherits(fn, parent);","function fn(a, b) {}\n" + "goog.inherits(fn, parent); fn['$inject']=['a', 'b']");
  test("/** @ngInject \n @constructor */" + "function fn(a, b) {}" + "goog.inherits(fn, parent);"+ "var foo = 42;","function fn(a, b) {}\n" + "goog.inherits(fn, parent); fn['$inject']=['a', 'b'];" + "var foo = 42;");
}
