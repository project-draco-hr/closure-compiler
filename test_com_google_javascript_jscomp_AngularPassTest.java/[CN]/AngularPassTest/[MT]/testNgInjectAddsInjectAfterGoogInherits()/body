{
  test(LINE_JOINER.join("/** @ngInject @constructor */ ","function fn(a, b) {}","goog.inherits(fn, parent);"),LINE_JOINER.join("function fn(a, b) {}","goog.inherits(fn, parent);","fn['$inject']=['a', 'b']"));
  test(LINE_JOINER.join("/** @ngInject @constructor */","function fn(a, b) {}","goog.inherits(fn, parent);","var foo = 42;"),LINE_JOINER.join("function fn(a, b) {}","goog.inherits(fn, parent);","fn['$inject']=['a', 'b'];","var foo = 42;"));
}
