{
  foldSame("while (foo(), true) boo()");
  foldSame("var a = (foo(), true);");
  foldSame("a = (foo(), true);");
  foldSame("a:a(),b()");
  fold("(x=2), foo()","x=2; foo()");
  fold("foo(), boo();","foo(); boo()");
  fold("(a(), b()), (c(), d());","a(); b(); c(); d();");
  fold("foo(), true","foo();");
  fold("function x(){foo(), true}","function x(){foo();}");
}
