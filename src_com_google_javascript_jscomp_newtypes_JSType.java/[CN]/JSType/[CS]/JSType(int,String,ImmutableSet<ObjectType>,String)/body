{
  this.typeVar=typeVar;
  this.location=location;
  if (objs == null) {
    this.mask=mask;
    this.objs=null;
  }
 else   if (objs.size() == 0) {
    this.mask=mask & ~NON_SCALAR_MASK;
    this.objs=null;
  }
 else {
    this.mask=mask | NON_SCALAR_MASK;
    this.objs=objs;
  }
  Preconditions.checkState(this.isValidType(),"Cannot create type with bits <<<" + Integer.toHexString(mask) + ">>>, objs <<<"+ objs+ ">>>, and typeVar <<<"+ typeVar+ ">>>");
}
