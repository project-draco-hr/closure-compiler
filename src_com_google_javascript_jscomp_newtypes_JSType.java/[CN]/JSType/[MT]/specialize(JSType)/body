{
  JSType t=specializeHelper(other);
  if (t.isBottom() && (isLoose() || other.isLoose())) {
    t=autobox().specializeHelper(other.autobox());
    if (!t.isBottom()) {
      return this;
    }
  }
  if (t.isLoose()) {
    JSType maybeScalar=ObjectType.mayTurnLooseObjectToScalar(t,JSType.commonTypes);
    if (t != maybeScalar) {
      return maybeScalar;
    }
  }
  return t;
}
