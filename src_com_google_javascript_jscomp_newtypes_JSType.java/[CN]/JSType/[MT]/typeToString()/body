{
switch (type) {
case BOTTOM_MASK:
case TOP_MASK:
case UNKNOWN_MASK:
    return tagToString(type,null);
default :
  int tags=type;
Set<String> types=Sets.newTreeSet();
for (int mask=1; mask != END_MASK; mask<<=1) {
if ((tags & mask) != 0) {
  types.add(tagToString(mask,objs));
  tags=tags & ~mask;
}
}
if (tags == 0) {
return Joiner.on("|").join(types);
}
 else {
return "Unrecognized type: " + tags;
}
}
}
