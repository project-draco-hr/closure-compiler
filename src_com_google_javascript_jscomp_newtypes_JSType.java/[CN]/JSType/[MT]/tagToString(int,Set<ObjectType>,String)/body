{
switch (tag) {
case TRUE_MASK:
case FALSE_MASK:
    return "boolean";
case BOTTOM_MASK:
  return "bottom";
case STRING_MASK:
return "string";
case NON_SCALAR_MASK:
Set<String> strReps=Sets.newHashSet();
for (ObjectType obj : objs) {
strReps.add(obj.toString());
}
return Joiner.on("|").join(strReps);
case NULL_MASK:
return "null";
case NUMBER_MASK:
return "number";
case TOP_MASK:
return "top";
case UNDEFINED_MASK:
return "undefined";
case UNKNOWN_MASK:
return "?";
case TYPEVAR_MASK:
return T;
default :
return null;
}
}
