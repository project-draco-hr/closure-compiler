{
  testSame("/./");
  test("/[\\0-\\uffff]/","/[\\S\\s]/");
  test("/[^\\0-\\uffff]/","/(?!)/");
  test("/[^\\0-\\x40\\x42-\\uffff]/","/A/");
  test("/[0-9a-fA-F]/i","/[\\da-f]/i");
  test("/[0-9a-zA-Z_$]/i","/[\\w$]/");
  test("/[()*+\\-,]/g","/[(--]/g");
  test("/[()*+\\-,z]/g","/[(--z]/g");
  test("/[\\-\\.\\/0]/g","/[--0]/g");
  test("/[\\-\\.\\/0\\n]/g","/[\\n\\--0]/g");
  test("/[\\[\\\\\\]]/g","/[[-\\]]/g");
  test("/[\\[\\\\\\]\\^]/g","/[[-^]/g");
  test("/[\\^`_]/g","/[\\^-`]/g");
  test("/[^\\^`_]/g","/[^^-`]/g");
  test("/^(?=[^a-z])/i","/^(?=[\\W\\d_])/");
  test("/^[^a-z0-9]/i","/^[\\W_]/");
  test("/[0-FA-Z]/","/[0-Z]/");
  test("/[0-9]/","/\\d/");
  test("/[^0-9]/","/\\D/");
  testSame("/\\D/");
  test("/[_a-z0-9]/i","/\\w/");
  test("/[0-9_a-z]/i","/\\w/");
  test("/[_a-z0-9]/","/[\\d_a-z]/");
  test("/[_E-Za-f0-9]/i","/\\w/");
  test("/[E-Za-f]/i","/[a-z]/i");
  test("/[_E-Za-f0-9]/","/[\\dE-Z_a-f]/");
  test("/[\\u00ca\\u00ea]/","/[\\xca\\xea]/");
  test("/[\\u00ca\\u00ea]/i","/\\xca/i");
  testSame("/^[\\s\\xa0]*$/");
  test("/^(?:\\s|\\xA0)*$/","/^[\\s\\xa0]*$/");
}
