{
  testSame("/a*/");
  testSame("/a+/");
  testSame("/a+?/");
  testSame("/a?/");
  testSame("/a{6}/");
  testSame("/a{4,}/");
  test("/a{3,}/","/aaa+/");
  testSame("/a{4,6}/");
  testSame("/a{4,6}?/");
  test("/(?:a?)?/","/a?/");
  test("/(?:a?)*/","/a*/");
  test("/(?:a*)?/","/a*/");
  test("/a(?:a*)?/","/a+/");
  test("/(?:a{2,3}){3,4}/","/a{6,12}/");
  test("/a{2,3}a{3,4}/","/a{5,7}/");
  testSame("/a{5,7}b{5,6}/");
  test("/a{2,3}b{3,4}/","/aaa?bbbb?/");
  test("/a{3}b{3,4}/","/aaabbbb?/");
  testSame("/[a-z]{1,2}/");
  test("/\\d{1,2}/","/\\d\\d?/");
  test("/a*a*/","/a*/");
  test("/a+a+/","/aa+/");
  test("/a+a*/","/a+/");
  testSame("/a\\{3,1}/");
  test("/a(?:{3,1})/","/a\\{3,1}/");
  test("/a{3\\,1}/","/a\\{3,1}/");
  testSame("/a\\{3}/");
  testSame("/a\\{3,}/");
  testSame("/a\\{1,3}/");
  testSame("/a{/");
  testSame("/a{}/");
  testSame("/a{x}/");
  testSame("/a{-1}/");
  testSame("/a{,3}/");
  testSame("/{{[a-z]+}}/");
  testSame("/{\\{0}}/");
  testSame("/{\\{0?}}/");
}
