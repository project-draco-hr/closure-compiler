{
  testSame("/a*/");
  testSame("/a+/");
  testSame("/a+?/");
  testSame("/a?/");
  testSame("/a{6}/");
  testSame("/a{4,}/");
  testSame("/a{4,6}/");
  testSame("/a{4,6}?/");
  test("/(?:a?)?/","/a?/");
  test("/(?:a?)*/","/a*/");
  test("/(?:a*)?/","/a*/");
  test("/a(?:a*)?/","/a+/");
  test("/(?:a{2,3}){3,4}/","/a{6,12}/");
  test("/a{2,3}a{3,4}/","/a{5,7}/");
  testSame("/a{2,3}b{3,4}/");
  test("/a*a*/","/a*/");
  test("/a+a+/","/a{2,}/");
  test("/a+a*/","/a+/");
  test("/a{/","/a\\{/");
  test("/a{}/","/a\\{}/");
  test("/a{,3}/","/a\\{,3}/");
  test("/a{x}/","/a\\{x}/");
  test("/a{-1}/","/a\\{-1}/");
  test("/a{3,1}/","/a\\{3,1}/");
}
