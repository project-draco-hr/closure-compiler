{
  Scope scope=t.getScope();
  if (t.inGlobalHoistScope() || !shouldTemporarilyRenameLocalsInScope(scope)) {
    return;
  }
  Iterator<Var> it=scope.getVars();
  while (it.hasNext()) {
    Var current=it.next();
    if (current.isBleedingFunction()) {
      localBleedingFunctions.add(current);
      localBleedingFunctionsPerScope.put(scope.getParent(),current);
    }
  }
}
