{
  if (useEval) {
    out.append("(0,eval(\"");
    if (useStrict) {
      appendEscaped(out,"'use strict';");
    }
    append(out,Mode.ESCAPED,contents);
    appendSourceUrl(out,Mode.ESCAPED);
    out.append("));");
  }
 else {
    out.append(contents.read());
    appendSourceUrl(out,Mode.NORMAL);
  }
}
