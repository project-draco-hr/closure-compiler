{
  Predicate<Node> taintingPredicate=new Predicate<Node>(){
    @Override public boolean apply(    Node value){
switch (value.getType()) {
case ASSIGN:
        return false;
case THIS:
      return false;
case NAME:
    return false;
case GETELEM:
case GETPROP:
  return false;
case CALL:
return false;
default :
break;
}
return false;
}
}
;
return NodeUtil.evaluatesToLocalValue(value,taintingPredicate);
}
