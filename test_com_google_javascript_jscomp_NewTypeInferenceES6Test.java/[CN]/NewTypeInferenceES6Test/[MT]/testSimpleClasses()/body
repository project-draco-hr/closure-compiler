{
  typeCheck("class Foo {}");
  typeCheck("class Foo {}\n" + "class Bar {}\n" + "/** @type {!Foo} */ var x = new Bar;",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("class Foo {\n" + "  constructor(x) {\n" + "    /** @type {string} */\n"+ "    this.x = x;\n"+ "  }\n"+ "}\n"+ "(new Foo('')).x - 5;\n",NewTypeInference.INVALID_OPERAND_TYPE);
}
