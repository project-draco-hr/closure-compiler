{
  typeCheck("function f() {\n" + "  x < 'str';\n" + "}"+ "var x = 5;\n"+ "f()",NewTypeInference.CROSS_SCOPE_GOTCHA);
  typeCheck("/** @type {string} */ var s;\n" + "function f() {\n" + "  s = 123;\n"+ "}\n"+ "f();",NewTypeInference.MISTYPED_ASSIGN_RHS);
  checkNoWarnings("function g(x) {\n" + "  function f() { x < 'str'; z < 'str'; x = 5; }\n" + "  var z = x;\n"+ "  f();\n"+ "  x - 5;\n"+ "  z < 'str';\n"+ "}");
}
