{
  typeCheck("function f() { return 'str'; }\n" + "var x = f();\n" + "x - 7;",NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck("function f() { return 'str'; }\n" + "f() - 5;",NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck("(function() {\n" + "  function f() { return 'str'; }\n" + "  f() - 5;\n"+ "})();",NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck("(function() {\n" + "  function f() { return 'str'; }\n" + "  f() - 5;\n"+ "})();",NewTypeInference.INVALID_OPERAND_TYPE);
}
