{
  checkNoWarnings(CLOSURE_BASE + "function f(/** !Function */ func) {};\n" + "function g(obj) {\n"+ "    if (goog.isFunction(obj)) {\n"+ "      f(obj);\n"+ "    }\n"+ "};");
  checkNoWarnings("function f(/** !Function */ func) {};\n" + "function g(obj) {\n" + "    if (typeof obj === 'function') {\n"+ "      f(obj);\n"+ "    }\n"+ "};");
  checkNoWarnings("/** @param {function(T)} fn @param {T} x @template T */\n" + "function reqGenFun(fn, x) {};\n" + "function g(obj, str) {\n"+ "  var member = obj[str];\n"+ "  if (typeof member === 'function') {\n"+ "    reqGenFun(member, str);\n"+ "  }\n"+ "};");
