{
  typeCheck("var x = true ? null : 'str';\n" + "while (x !== null) {}\n" + "var /** null */ y = x;");
  typeCheck("var x = true ? null : 'str';\n" + "for (;x !== null;) {}\n" + "var /** null */ y = x;");
  typeCheck("for (var x = true ? null : 'str'; x === null;) {}\n" + "var /** string */ y = x;");
  typeCheck("var x;\n" + "for (x = true ? null : 'str'; x === null;) {}\n" + "var /** string */ y = x;");
  typeCheck("var x = true ? null : 'str';\n" + "do {} while (x === null);\n" + "var /** string */ y = x;");
}
