{
  typeCheck("x; var x;",VariableReferenceCheck.UNDECLARED_REFERENCE);
  typeCheck("x = 7; var x;",VariableReferenceCheck.UNDECLARED_REFERENCE);
  checkInferenceWithWarning("var /** undefined */ y = x; var x;",VariableReferenceCheck.UNDECLARED_REFERENCE,"x",JSType.UNDEFINED);
  checkNoWarnings("function f() { return 9; }\n" + "var x = f();\n" + "x - 7;");
}
