{
  typeCheck("/** @constructor */\n" + "function Foo() {}\n" + "/** @param {Foo} x */\n"+ "function f(x) {}\n"+ "f(new Foo);");
  typeCheck("/** @constructor */ function Foo(){ this.prop = 123; }\n" + "function f(/** Foo */ obj) { obj.prop; }",NewTypeInference.NULLABLE_DEREFERENCE);
  typeCheck("/** @interface */\n" + "function I() {}\n" + "I.prototype.method = function() {};\n"+ "/** @param {I} x */\n"+ "function foo(x) { x.method(); }",NewTypeInference.NULLABLE_DEREFERENCE);
}
