{
  typeCheck("/** @const */ var x = 1; x = 2;",NewTypeInference.CONST_REASSIGNED);
  typeCheck("/** @const */ var x = 1; x += 2;",NewTypeInference.CONST_REASSIGNED);
  typeCheck("/** @const */ var x = 1; x -= 2;",NewTypeInference.CONST_REASSIGNED);
  typeCheck("/** @const */ var x = 1; x++;",NewTypeInference.CONST_REASSIGNED);
  typeCheck("/** @const */ var x = 1;\n" + "function f() { x = 2; }",NewTypeInference.CONST_REASSIGNED);
  typeCheck("/** @const {number} */ var x;","x = 2;",NewTypeInference.CONST_REASSIGNED);
}
