{
  typeCheck("(/** @type {number} */ ('asdf'));",TypeValidator.INVALID_CAST);
  checkNoWarnings("function f(/** (number|string) */ x) {\n" + "  var y = /** @type {number} */ (x);\n" + "}");
  checkNoWarnings("(/** @type {(number|string)} */ (1));");
  checkNoWarnings("(/** @type {number} */ (/** @type {?} */ ('asdf')))");
}
