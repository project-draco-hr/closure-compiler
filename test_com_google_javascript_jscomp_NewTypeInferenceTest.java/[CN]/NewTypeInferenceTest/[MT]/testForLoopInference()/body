{
  checkInference("var x = 5;\n" + "for (;true;) {\n" + "  x = 'str';\n"+ "}","x",JSType.join(JSType.NUMBER,JSType.STRING));
  checkInference("var x = 5;" + "while (true) {" + "  x = 'str';"+ "}","x",JSType.join(JSType.NUMBER,JSType.STRING));
  checkInference("while (true) {" + "  var x = 'str';" + "}","x",JSType.join(JSType.STRING,JSType.UNDEFINED));
  checkInference("for (var x = 5; x < 10; x++) {\n" + "}","x",JSType.NUMBER);
}
