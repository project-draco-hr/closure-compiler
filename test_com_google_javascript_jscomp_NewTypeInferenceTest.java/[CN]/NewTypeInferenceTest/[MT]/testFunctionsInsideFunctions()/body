{
  typeCheck("(function() {\n" + "  function f() {}; f(5);\n" + "})();",TypeCheck.WRONG_ARGUMENT_COUNT);
  typeCheck("(function() {\n" + "  function f() { return 'str'; }\n" + "  function g() { f() - 5; }\n"+ "})();",NewTypeInference.INVALID_INFERRED_RETURN_TYPE);
  typeCheck("var /** number */ x;\n" + "function f() { x = 'str'; }",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("var x;\n" + "function f() { x - 5; x < 'str'; }",NewTypeInference.INVALID_OPERAND_TYPE);
}
