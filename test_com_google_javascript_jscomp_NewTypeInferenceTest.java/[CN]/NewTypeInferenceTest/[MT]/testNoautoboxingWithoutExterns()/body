{
  typeCheck("","var /** number */ n = 123;\n" + "n.toString();",NewTypeInference.PROPERTY_ACCESS_ON_NONOBJECT);
  typeCheck("","var /** string */ s = '';\n" + "s.toString();",NewTypeInference.PROPERTY_ACCESS_ON_NONOBJECT);
  typeCheck("","var /** boolean */ b = true;\n" + "b.toString();",NewTypeInference.PROPERTY_ACCESS_ON_NONOBJECT);
}
