{
  checkNoWarnings("/** @constructor */ function Foo(){}\n" + "function f() {\n" + "  if (true) {\n"+ "    function g() { new Foo; }\n"+ "    g();\n"+ "  }\n"+ "}");
  typeCheck("function f() {\n" + "  if (true) {\n" + "    function g() { new Foo; }\n"+ "    g();\n"+ "  }\n"+ "}",VarCheck.UNDEFINED_VAR_ERROR);
}
