{
  typeCheck("/** @constructor */ function Foo() { this.prop = 123; }\n" + "function f(x) { if (x instanceof Foo) { var y = x.prop; } }");
  typeCheck("/** @constructor */ function Foo() { this.prop = 123; }\n" + "/** @constructor */ function Bar() { this.prop = 123; }\n" + "function f(cond, x) {\n"+ "  x = cond ? new Foo : new Bar;\n"+ "  var y = x.prop;\n"+ "}");
}
