{
  if (parent.isSpread() || (parent.isGetProp() && parent.matchesQualifiedName("arguments.length")) || (parent.isForOf() && arguments == parent.getSecondChild())|| (parent.isGetElem() && arguments == parent.getFirstChild())) {
    return;
  }
  if (parent.isCall() && arguments == parent.getLastChild()) {
    Node callee=parent.getFirstChild();
    if (callee.isGetProp() && callee.getLastChild().getString().equals("apply")) {
      return;
    }
  }
  report(arguments);
}
