{
  if (numSpaces > 16) {
    logger.warning("Tracer.appendSpaces called with large numSpaces");
    numSpaces=16;
  }
  while (numSpaces >= 5) {
    sb.append("     ");
    numSpaces-=5;
  }
switch (numSpaces) {
case 1:
    sb.append(" ");
  break;
case 2:
sb.append("  ");
break;
case 3:
sb.append("   ");
break;
case 4:
sb.append("    ");
break;
}
}
