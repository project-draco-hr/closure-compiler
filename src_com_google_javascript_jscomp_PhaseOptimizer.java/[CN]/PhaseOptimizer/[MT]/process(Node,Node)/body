{
  progress=0.0;
  progressStep=0.0;
  if (progressRange != null) {
    progressStep=(progressRange.maxValue - progressRange.initialValue) / passes.size();
    progress=progressRange.initialValue;
  }
  jsRoot=root;
  for (  CompilerPass pass : passes) {
    pass.process(externs,root);
    if (hasHaltingErrors()) {
      return;
    }
  }
}
