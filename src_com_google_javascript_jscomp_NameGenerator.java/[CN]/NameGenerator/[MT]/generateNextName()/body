{
  while (true) {
    String name=prefix;
    int i=nameCount;
    if (name.isEmpty()) {
      int pos=i % firstChars.length;
      name+=firstChars[pos].name;
      i/=firstChars.length;
    }
    while (i > 0) {
      i--;
      int pos=i % nonFirstChars.length;
      name+=nonFirstChars[pos].name;
      i/=nonFirstChars.length;
    }
    nameCount++;
    if (TokenStream.isKeyword(name) || reservedNames.contains(name)) {
      continue;
    }
    return name;
  }
}
