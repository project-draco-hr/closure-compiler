{
  final String CONTENTS=" // hi ! \n /* this is a comment */ " + "goog.provide('yes1');\n /* and another comment */ \n" + "goog.provide('yes2'); // include this\n"+ "function foo() {}\n"+ "goog.provide('no1');";
  DependencyInfo EXPECTED=new SimpleDependencyInfo(CLOSURE_PATH,SRC_PATH,ImmutableList.of("yes1","yes2"),Collections.<String>emptyList());
  DependencyInfo result=parser.parseFile(SRC_PATH,CLOSURE_PATH,CONTENTS);
  assertEquals(EXPECTED,result);
  assertEquals(0,errorManager.getErrorCount());
  assertEquals(0,errorManager.getWarningCount());
}
