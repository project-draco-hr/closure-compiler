{
  if (n.getType() == Token.CALL) {
    Node callee=n.getFirstChild();
    if (callee.getType() == Token.NAME && callee.getString().equals(name)) {
      return n;
    }
  }
  for (  Node c : n.children()) {
    Node result=findCall(c,name);
    if (result != null) {
      return result;
    }
  }
  return null;
}
