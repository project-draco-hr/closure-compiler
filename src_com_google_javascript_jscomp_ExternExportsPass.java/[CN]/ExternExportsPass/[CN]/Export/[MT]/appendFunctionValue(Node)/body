{
  sb.append(" = ");
  sb.append("function(");
  Node parameters=definition.getFirstChild().getNext();
  int i=0;
  for (Node current=parameters.getFirstChild(); current != null; current=current.getNext()) {
    if (i > 0) {
      sb.append(", ");
    }
    sb.append(current.getString());
    ++i;
  }
  sb.append(") {}");
}
