{
  List<String> pieces=Lists.newArrayList(path.split("\\."));
  for (int i=0; i < pieces.size(); ++i) {
    String cPath=Joiner.on(".").join(Iterables.limit(pieces,i + 1));
    if (i == pieces.size() - 1 || !paths.contains(cPath)) {
      if (i == 0) {
        sb.append("var ");
      }
      sb.append(cPath);
      if (i < pieces.size() - 1) {
        sb.append(";\n");
      }
      paths.add(cPath);
    }
  }
}
