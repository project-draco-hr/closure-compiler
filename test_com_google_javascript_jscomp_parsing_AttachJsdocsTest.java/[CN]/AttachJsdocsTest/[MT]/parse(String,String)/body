{
  TestErrorReporter testErrorReporter=new TestErrorReporter(null,warnings);
  Node script=ParserRunner.parse(new SimpleSourceFile("input",false),source,ParserRunner.createConfig(true,mode,null),testErrorReporter).ast;
  testErrorReporter.assertHasEncounteredAllErrors();
  testErrorReporter.assertHasEncounteredAllWarnings();
  return script;
}
