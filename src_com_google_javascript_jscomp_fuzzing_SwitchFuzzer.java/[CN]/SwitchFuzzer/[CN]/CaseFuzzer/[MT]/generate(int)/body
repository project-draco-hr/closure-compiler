{
  Node clause=new Node(nodeType);
  if (nodeType == Token.CASE) {
    int valueBudget=(int)(budget * getOwnConfig().optDouble("valueBudget"));
    if (valueBudget == 0) {
      valueBudget=1;
    }
    clause.addChildToBack(new ExpressionFuzzer(random,scopeManager,config,snGenerator).generate(valueBudget));
    budget-=valueBudget;
  }
  Node block=new BlockFuzzer(random,scopeManager,config,snGenerator).generate(budget + 1);
  block.setIsSyntheticBlock(true);
  clause.addChildrenToBack(block);
  return clause;
}
