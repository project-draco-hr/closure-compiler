{
  enableTypeCheck();
  testWarning("/** @type {number} */ let x = 5; x = 'str';",TypeValidator.TYPE_MISMATCH_WARNING);
  testWarning("let /** number */ x = 5; x = 'str';",TypeValidator.TYPE_MISMATCH_WARNING);
  test(LINE_JOINER.join("while (true) {","  /** @type {number} */ let x = 5;","  (function() { x++; })();","  x = 7;","}"),null);
  test(LINE_JOINER.join("for (/** @type {number} */ let x = 5;;) {","  (function() { x++; })();","  x = 7;","}"),null);
  test(LINE_JOINER.join("while (true) {","  /** @type {number} */ let x = 5;","  (function() { x++; })();","  x = 5;","}"),null);
  test(LINE_JOINER.join("for (/** @type {number} */ let x = 5;;) {","  (function() { x++; })();","  x = 'str';","}"),null);
}
