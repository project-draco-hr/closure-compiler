{
  final String source1="function f2() { throw 'hello'; }; f2();";
  final String source2="function f2() { 'H'.toLowerCase(); throw 'hello'; }; f2();";
  runWithExpectedStackTrace(source1,"	at test.js (f2)\n\tat test.js\n");
  runWithExpectedStackTrace(source2,"	at test.js (f2)\n\tat test.js\n");
}
