{
  FoldArrayAction action=FoldArrayAction.NOT_SAFE_TO_FOLD;
  if (arg == null) {
    action=FoldArrayAction.SAFE_TO_FOLD_WITHOUT_ARGS;
  }
 else   if (arg.getNext() != null) {
    action=FoldArrayAction.SAFE_TO_FOLD_WITH_ARGS;
  }
 else {
switch (arg.getType()) {
case STRING:
      action=FoldArrayAction.SAFE_TO_FOLD_WITH_ARGS;
    break;
case NUMBER:
  if (arg.getDouble() == 0) {
    action=FoldArrayAction.SAFE_TO_FOLD_WITHOUT_ARGS;
  }
break;
case ARRAYLIT:
action=FoldArrayAction.SAFE_TO_FOLD_WITH_ARGS;
break;
default :
}
}
return action;
}
