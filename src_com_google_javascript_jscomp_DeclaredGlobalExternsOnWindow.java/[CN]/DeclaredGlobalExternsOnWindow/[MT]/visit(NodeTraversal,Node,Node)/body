{
  if (n.isFunction()) {
    nodes.add(n.getFirstChild());
  }
 else   if (n.isVar()) {
    for (    Node c : n.children()) {
      if (!c.getString().equals("location")) {
        nodes.add(c);
      }
      if (!windowInExterns && c.getString().equals("window")) {
        windowInExterns=true;
      }
    }
  }
}
