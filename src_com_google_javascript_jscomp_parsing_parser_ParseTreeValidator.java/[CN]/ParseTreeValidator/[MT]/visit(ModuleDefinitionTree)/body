{
  for (  ParseTree element : tree.elements) {
    check((element.isStatement() && element.type != ParseTreeType.BLOCK) || element.type == ParseTreeType.CLASS_DECLARATION || element.type == ParseTreeType.EXPORT_DECLARATION || element.type == ParseTreeType.IMPORT_DECLARATION || element.type == ParseTreeType.MODULE_DEFINITION,element,"module element expected");
  }
}
