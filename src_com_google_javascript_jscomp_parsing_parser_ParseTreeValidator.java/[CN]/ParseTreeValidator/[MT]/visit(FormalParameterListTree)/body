{
  for (int i=0; i < tree.parameters.size(); i++) {
    ParseTree parameter=tree.parameters.get(i);
switch (parameter.type) {
case REST_PARAMETER:
      checkVisit(i == tree.parameters.size() - 1,parameter,"rest parameters must be the last parameter in a parameter list");
case IDENTIFIER_EXPRESSION:
    break;
case DEFAULT_PARAMETER:
  break;
default :
fail(parameter,"parameters must be identifiers or rest parameters");
break;
}
visitAny(parameter);
}
}
