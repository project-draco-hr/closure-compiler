{
  int i=0;
  for (  ParseTree element : tree.elements) {
    checkVisit(element.isNull() || element.isLeftHandSideExpression() || element.isPattern()|| element.isSpreadPatternElement(),element,"null, sub pattern, left hand side expression or spread expected");
    if (element.isSpreadPatternElement()) {
      check(i == tree.elements.size() - 1,element,"spread in array patterns must be the last element");
    }
    i++;
  }
}
