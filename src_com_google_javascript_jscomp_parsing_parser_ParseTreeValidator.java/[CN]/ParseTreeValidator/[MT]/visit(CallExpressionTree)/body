{
  check(tree.operand.isLeftHandSideExpression(),tree.operand,"left hand side expression expected");
  if (tree.operand instanceof NewExpressionTree) {
    check(tree.operand.asNewExpression().arguments != null,tree.operand,"new arguments expected");
  }
  visitAny(tree.operand);
  visitAny(tree.arguments);
}
