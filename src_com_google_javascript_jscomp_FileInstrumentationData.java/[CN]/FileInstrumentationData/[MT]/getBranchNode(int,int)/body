{
  Preconditions.checkArgument(lineNumber > 0,"Expected non-zero positive integer as line number: %s",lineNumber);
  Preconditions.checkArgument(branchNumber > 0,"Expected non-zero positive integer as branch number: %s",branchNumber);
  return branchNodes.get(BranchIndexPair.of(lineNumber - 1,branchNumber - 1));
}
