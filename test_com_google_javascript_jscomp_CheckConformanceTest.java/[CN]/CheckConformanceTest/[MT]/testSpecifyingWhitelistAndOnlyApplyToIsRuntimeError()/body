{
  configuration="requirement: {\n" + "  type: BANNED_NAME\n" + "  value: 'eval'\n"+ "  error_message: 'eval is not allowed'\n"+ "  whitelist: 'blah'\n"+ "  only_apply_to_regexp: 'test.js$'\n "+ "}";
  try {
    testSame(ImmutableList.of(SourceFile.fromCode("bar.js","eval()")));
    fail("expected IllegalArgumentException");
  }
 catch (  Exception e) {
    assertTrue(e instanceof IllegalArgumentException);
  }
}
