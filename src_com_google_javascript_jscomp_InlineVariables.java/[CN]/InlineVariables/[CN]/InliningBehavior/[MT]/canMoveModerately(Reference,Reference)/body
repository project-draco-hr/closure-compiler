{
  Iterator<Node> it=new NodeIterators.LocalVarMotion(declaration.getNameNode(),declaration.getParent(),declaration.getGrandparent());
  Node targetName=reference.getNameNode();
  while (it.hasNext()) {
    Node curNode=it.next();
    if (curNode == targetName) {
      return true;
    }
  }
  return false;
}
