{
switch (element.getType()) {
case ArrayExpr:
case AssignExpr:
case BinaryExpr:
case CallExpr:
case ConditionalExpr:
case CountExpr:
case DeleteExpr:
case EvalExpr:
case FunctionExpr:
case IdExpr:
case InvokeExpr:
case LiteralExpr:
case LogicalAndExpr:
case LogicalOrExpr:
case MemberExpr:
case NewExpr:
case ObjectExpr:
case RegExpExpr:
case ThisExpr:
case TypeofExpr:
case UnaryExpr:
    return true;
default :
  return false;
}
}
