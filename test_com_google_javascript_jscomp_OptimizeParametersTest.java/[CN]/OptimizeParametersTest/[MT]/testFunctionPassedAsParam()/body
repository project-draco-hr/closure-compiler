{
  String src="/** @constructor */ function person(){}; " + "person.prototype.run = function(a, b) {};" + "person.prototype.walk = function() {};"+ "person.prototype.foo = function() { this.run(this.walk, 0.1)};"+ "person.foo();";
  String expected="function person(){}; person.prototype.run = function(a) {" + "  var b = 0.1;};" + "person.prototype.walk = function() {};"+ "person.prototype.foo = function() { this.run(this.walk)};"+ "person.foo();";
  test(src,expected);
