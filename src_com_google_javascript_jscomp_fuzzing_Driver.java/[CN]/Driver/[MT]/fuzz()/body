{
  if (seed == -1) {
    seed=System.currentTimeMillis();
  }
  Random random=new Random(seed);
  ScriptFuzzer fuzzer=new ScriptFuzzer(random,getConfig());
  Node script=null;
  try {
    script=fuzzer.generate(maxASTSize);
  }
 catch (  Exception e) {
    getLogger().log(Level.SEVERE,"Fuzzer error!\nSeed: " + seed,e);
  }
  return script;
}
