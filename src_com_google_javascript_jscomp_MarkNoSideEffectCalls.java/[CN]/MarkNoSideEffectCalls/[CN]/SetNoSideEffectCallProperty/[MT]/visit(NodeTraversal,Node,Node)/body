{
  if (!node.isCall() && !node.isNew()) {
    return;
  }
  Collection<Definition> definitions=defFinder.getDefinitionsReferencedAt(node.getFirstChild());
  if (definitions == null) {
    return;
  }
  for (  Definition def : definitions) {
    Node lValue=def.getLValue();
    Preconditions.checkNotNull(lValue);
    if (!noSideEffectFunctionNames.contains(lValue) && definitionTypeContainsFunctionType(def)) {
      return;
    }
  }
  node.setSideEffectFlags(Node.NO_SIDE_EFFECTS);
}
