{
  if (block.getType() == Token.BLOCK && block.getParent().getType() == Token.TRY && block.getParent().getFirstChild() == block) {
    for (Node s=block.getNext(); s != null; s=s.getNext()) {
      if (NodeUtil.hasCatchHandler(s)) {
        return s.getFirstChild();
      }
    }
  }
  return null;
}
