{
  if (n.isName()) {
    String name=n.getString();
    if (suffix.equals(name)) {
      return;
    }
    if (EXPORTS.equals(name)) {
      return;
    }
    Scope.Var var=t.getScope().getVar(name);
    if (var != null && var.isGlobal()) {
      n.setString(name + "$$" + suffix);
      n.putProp(Node.ORIGINALNAME_PROP,name);
    }
  }
}
