{
  return MODULE_NAME_PREFIX + filename.replaceAll("^\\." + Pattern.quote(ES6ModuleLoader.MODULE_SLASH),"").replaceAll(Pattern.quote(ES6ModuleLoader.MODULE_SLASH),MODULE_NAME_SEPARATOR).replaceAll(Pattern.quote("\\"),MODULE_NAME_SEPARATOR).replaceAll("\\.js$","").replaceAll("-","_").replaceAll(":","_").replaceAll("\\.","");
}
