{
  if (isArray(type)) {
    return true;
  }
  if (type.isUnionType()) {
    JSType arrayType=typeRegistry.getNativeType(ARRAY_TYPE);
    for (    JSType alternate : type.toMaybeUnionType().getAlternates()) {
      if (alternate.isSubtype(arrayType)) {
        return true;
      }
    }
  }
  return false;
}
