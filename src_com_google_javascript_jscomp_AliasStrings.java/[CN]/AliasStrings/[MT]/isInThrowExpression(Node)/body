{
  for (  Node ancestor : n.getAncestors()) {
switch (ancestor.getType()) {
case Token.THROW:
      return true;
case Token.IF:
case Token.WHILE:
case Token.DO:
case Token.FOR:
case Token.SWITCH:
case Token.CASE:
case Token.DEFAULT_CASE:
case Token.BLOCK:
case Token.SCRIPT:
case Token.FUNCTION:
case Token.TRY:
case Token.CATCH:
case Token.RETURN:
case Token.EXPR_RESULT:
    return false;
}
}
return false;
}
