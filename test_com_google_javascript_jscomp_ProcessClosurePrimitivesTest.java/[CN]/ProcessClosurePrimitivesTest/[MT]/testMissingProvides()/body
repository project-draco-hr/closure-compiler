{
  test("goog.require('foo');","",MISSING_PROVIDE_ERROR);
  test("goog.provide('foo'); goog.require('Foo');","var foo={};",MISSING_PROVIDE_ERROR);
  test("goog.provide('foo'); goog.require('foo.bar');","var foo={};",MISSING_PROVIDE_ERROR);
  test("goog.provide('foo'); var EXPERIMENT_FOO = true; " + "if (EXPERIMENT_FOO) {goog.require('foo.bar');}","var foo={}; var EXPERIMENT_FOO = true; if (EXPERIMENT_FOO) {}",MISSING_PROVIDE_ERROR);
}
