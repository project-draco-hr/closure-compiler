{
  expectErrors("Parse error. Unexpected token ';' in type expression");
  parse("var n: (p1:string) =>;");
  expectErrors("Parse error. Unexpected token '{' in type expression");
  parse("var n: (p1:string) => {};");
  expectErrors("Parse error. Unexpected token '=>' in type expression");
  parse("var n: => boolean;");
}
