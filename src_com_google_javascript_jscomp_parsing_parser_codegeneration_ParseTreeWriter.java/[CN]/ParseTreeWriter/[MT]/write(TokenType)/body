{
  if (type == TokenType.CLOSE_CURLY) {
    indentDepth--;
  }
  boolean spaceBefore=true;
  boolean spaceAfter=true;
switch (type) {
case PERIOD:
case OPEN_SQUARE:
case OPEN_PAREN:
case CLOSE_SQUARE:
    spaceBefore=false;
  spaceAfter=false;
break;
case COLON:
case COMMA:
case SEMI_COLON:
case CLOSE_PAREN:
spaceBefore=false;
break;
}
write(type.toString(),spaceBefore,spaceAfter);
if (type == TokenType.OPEN_CURLY) {
indentDepth++;
}
}
