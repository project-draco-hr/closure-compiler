{
  if (tree != null && tree == HIGHLIGHTED) {
    write("\033[41m");
  }
  if (tree != null && tree.location != null && tree.location.start != null && SHOW_LINE_NUMBERS) {
    currentLineComment="Line: " + (tree.location.start.line + 1);
  }
  super.visitAny(tree);
  if (tree != null && tree == HIGHLIGHTED) {
    write("\033[0m");
  }
}
