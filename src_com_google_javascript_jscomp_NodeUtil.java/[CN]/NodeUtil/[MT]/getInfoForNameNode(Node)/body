{
  JSDocInfo info=null;
  Node parent=null;
  if (nameNode != null) {
    info=nameNode.getJSDocInfo();
    parent=nameNode.getParent();
  }
  if (info == null && parent != null && ((parent.getType() == Token.VAR && parent.hasOneChild()) || parent.getType() == Token.FUNCTION)) {
    info=parent.getJSDocInfo();
  }
  return info;
}
