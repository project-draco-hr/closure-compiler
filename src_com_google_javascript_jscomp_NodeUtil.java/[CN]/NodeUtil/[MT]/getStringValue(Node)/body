{
switch (n.getType()) {
case Token.NAME:
case Token.STRING:
    return n.getString();
case Token.NUMBER:
  double value=n.getDouble();
long longValue=(long)value;
if (longValue == value) {
return Long.toString(longValue);
}
 else {
return Double.toString(n.getDouble());
}
case Token.FALSE:
case Token.TRUE:
case Token.NULL:
return Node.tokenToName(n.getType());
case Token.VOID:
return "undefined";
}
return null;
}
