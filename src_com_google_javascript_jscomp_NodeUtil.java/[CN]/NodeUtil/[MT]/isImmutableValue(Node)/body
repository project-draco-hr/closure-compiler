{
switch (n.getType()) {
case STRING:
case NUMBER:
case NULL:
case TRUE:
case FALSE:
    return true;
case CAST:
case NOT:
case VOID:
case NEG:
  return isImmutableValue(n.getFirstChild());
case NAME:
String name=n.getString();
return "undefined".equals(name) || "Infinity".equals(name) || "NaN".equals(name);
default :
break;
}
return false;
}
