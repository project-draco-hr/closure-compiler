{
  if (!fn.isFunction()) {
    return false;
  }
  if (fn.getFirstChild().getJSDocInfo() != null) {
    return true;
  }
  Node param=fn.getChildAtIndex(1).getFirstChild();
  while (param != null) {
    if (param.getJSDocInfo() != null) {
      return true;
    }
    param=param.getNext();
  }
  return false;
}
