{
  Node current=n;
  Node parent;
  while ((parent=current.getParent()) != null) {
switch (parent.getType()) {
case Token.EXPR_RESULT:
case Token.GETPROP:
      break;
case Token.ASSIGN:
    if (parent.getFirstChild() != current) {
      return true;
    }
  break;
case Token.SCRIPT:
case Token.FUNCTION:
case Token.LP:
case Token.BLOCK:
case Token.VAR:
return false;
default :
return true;
}
current=parent;
}
return false;
}
