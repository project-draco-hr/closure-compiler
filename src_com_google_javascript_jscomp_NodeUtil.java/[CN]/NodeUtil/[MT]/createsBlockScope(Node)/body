{
switch (n.getType()) {
case Token.BLOCK:
    if (n.isSyntheticBlock() || n.getParent() == null || n.getGrandparent() == null || n.getParent().isCatch()) {
      return false;
    }
  return true;
case Token.FOR:
case Token.FOR_OF:
case Token.SWITCH:
case Token.CLASS:
return true;
}
return false;
}
