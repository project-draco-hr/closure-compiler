{
switch (n.getType()) {
case ASSIGN_BITOR:
    return Token.BITOR;
case ASSIGN_BITXOR:
  return Token.BITXOR;
case ASSIGN_BITAND:
return Token.BITAND;
case ASSIGN_LSH:
return Token.LSH;
case ASSIGN_RSH:
return Token.RSH;
case ASSIGN_URSH:
return Token.URSH;
case ASSIGN_ADD:
return Token.ADD;
case ASSIGN_SUB:
return Token.SUB;
case ASSIGN_MUL:
return Token.MUL;
case ASSIGN_DIV:
return Token.DIV;
case ASSIGN_MOD:
return Token.MOD;
}
throw new IllegalArgumentException("Not an assignment op:" + n);
}
