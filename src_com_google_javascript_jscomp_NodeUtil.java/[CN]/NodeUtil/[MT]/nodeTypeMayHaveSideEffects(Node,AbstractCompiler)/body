{
  if (isAssignmentOp(n)) {
    return true;
  }
switch (n.getType()) {
case DELPROP:
case DEC:
case INC:
case YIELD:
case THROW:
    return true;
case CALL:
  return NodeUtil.functionCallHasSideEffects(n,compiler);
case NEW:
return NodeUtil.constructorCallHasSideEffects(n);
case NAME:
return n.hasChildren();
default :
return false;
}
}
