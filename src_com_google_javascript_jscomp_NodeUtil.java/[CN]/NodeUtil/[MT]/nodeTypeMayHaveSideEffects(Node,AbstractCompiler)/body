{
  if (isAssignmentOp(n)) {
    return true;
  }
switch (n.getType()) {
case Token.DELPROP:
case Token.DEC:
case Token.INC:
case Token.YIELD:
case Token.THROW:
    return true;
case Token.CALL:
  return NodeUtil.functionCallHasSideEffects(n,compiler);
case Token.NEW:
return NodeUtil.constructorCallHasSideEffects(n);
case Token.NAME:
return n.hasChildren();
default :
return false;
}
}
