{
  Preconditions.checkState(n.isFunction());
  Node parent=n.getParent();
switch (parent.getType()) {
case Token.NAME:
    return parent;
case Token.ASSIGN:
  return parent.getFirstChild();
default :
Node funNameNode=n.getFirstChild();
return funNameNode.getString().isEmpty() ? null : funNameNode;
}
}
