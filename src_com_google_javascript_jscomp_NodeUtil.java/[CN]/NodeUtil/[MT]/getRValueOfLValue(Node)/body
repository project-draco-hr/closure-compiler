{
  Node parent=n.getParent();
switch (parent.getType()) {
case ASSIGN:
case ASSIGN_BITOR:
case ASSIGN_BITXOR:
case ASSIGN_BITAND:
case ASSIGN_LSH:
case ASSIGN_RSH:
case ASSIGN_URSH:
case ASSIGN_ADD:
case ASSIGN_SUB:
case ASSIGN_MUL:
case ASSIGN_DIV:
case ASSIGN_MOD:
    return n.getNext();
case VAR:
case LET:
case CONST:
case OBJECTLIT:
  return n.getFirstChild();
case FUNCTION:
case CLASS:
return parent;
}
return null;
}
