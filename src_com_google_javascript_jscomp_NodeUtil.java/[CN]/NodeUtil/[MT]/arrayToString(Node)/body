{
  Node first=literal.getFirstChild();
  StringBuilder result=new StringBuilder();
  int nextSlot=0;
  int nextSkipSlot=0;
  for (Node n=first; n != null; n=n.getNext()) {
    String childValue=getArrayElementStringValue(n);
    if (childValue == null) {
      return null;
    }
    if (n != first) {
      result.append(',');
    }
    result.append(childValue);
    nextSlot++;
  }
  return result.toString();
}
