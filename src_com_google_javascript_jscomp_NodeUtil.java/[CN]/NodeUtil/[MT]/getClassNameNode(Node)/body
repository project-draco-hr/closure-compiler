{
  Node parent=clazz.getParent();
  if (NodeUtil.isStatement(clazz)) {
    return clazz.getFirstChild();
  }
 else   if (parent.isAssign() && parent.getParent().isExprResult()) {
    return parent.getFirstChild();
  }
 else   if (parent.isName()) {
    return parent;
  }
 else {
    return null;
  }
}
