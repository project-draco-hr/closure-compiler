{
  if (parent.isGetProp() && node == parent.getLastChild()) {
    return convention.isConstantKey(node.getString());
  }
 else   if (isObjectLitKey(node,parent)) {
    return convention.isConstantKey(node.getString());
  }
 else   if (node.isName()) {
    return convention.isConstant(node.getString());
  }
  return false;
}
