{
switch (token) {
case Token.NUMBER:
    return TypeConsts.NUMBER;
case Token.STRING:
  return TypeConsts.STRING;
case Token.TRUE:
return TypeConsts.TRUE_TYPE;
case Token.FALSE:
return TypeConsts.FALSE_TYPE;
case Token.NULL:
return TypeConsts.NULL;
default :
throw new RuntimeException("The token isn't a scalar value " + Token.name(token));
}
}
