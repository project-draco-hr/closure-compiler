{
switch (token) {
case Token.NUMBER:
    return JSType.NUMBER;
case Token.STRING:
  return JSType.STRING;
case Token.TRUE:
return JSType.TRUE_TYPE;
case Token.FALSE:
return JSType.FALSE_TYPE;
case Token.NULL:
return JSType.NULL;
default :
throw new RuntimeException("The token isn't a scalar value " + Token.name(token));
}
}
