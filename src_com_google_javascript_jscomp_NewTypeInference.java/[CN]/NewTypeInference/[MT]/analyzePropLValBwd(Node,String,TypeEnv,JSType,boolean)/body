{
  LValueResultBwd lvalue=analyzeLValueBwd(obj,outEnv,JSType.TOP_OBJECT.withProperty(pname,type),false,true);
  if (lvalue.ptr != null) {
    lvalue.ptr+="." + pname;
    if (doSlicing) {
      String objName=TypeUtils.getLeftmostName(lvalue.ptr);
      String props=TypeUtils.getAllButLeftmost(lvalue.ptr);
      JSType objType=envGetType(lvalue.env,objName);
      JSType propDeclType=lvalue.type.getDeclaredProp(pname);
      JSType slicedObjType=propDeclType == null ? objType.withoutProperty(props) : objType.withDeclaredProperty(props,propDeclType);
      lvalue.env=envPutType(lvalue.env,objName,slicedObjType);
    }
  }
  lvalue.type=lvalue.type.mayHaveProp(pname) ? lvalue.type.getProp(pname) : JSType.UNKNOWN;
  return lvalue;
}
