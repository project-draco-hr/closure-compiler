{
  Node scopeRoot=t.getScopeRoot();
  Node scopeRootParent=scopeRoot.getParent();
  return (deprecatedDepth > 0) || (getTypeDeprecationInfo(t.getScope().getTypeOfThis()) != null) || (scopeRootParent != null && scopeRootParent.getType() == Token.ASSIGN && getTypeDeprecationInfo(getClassOfMethod(scopeRoot,scopeRootParent)) != null);
}
