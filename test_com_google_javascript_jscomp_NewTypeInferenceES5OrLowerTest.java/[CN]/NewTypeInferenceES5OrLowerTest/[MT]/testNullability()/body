{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @param {Foo} x */","function f(x) {}","f(new Foo);"));
  typeCheck(Joiner.on('\n').join("/** @constructor */ function Foo(){ this.prop = 123; }","function f(/** Foo */ obj) { obj.prop; }"),NewTypeInference.NULLABLE_DEREFERENCE);
  typeCheck(Joiner.on('\n').join("/** @interface */","function I() {}","I.prototype.method = function() {};","/** @param {I} x */","function foo(x) { x.method(); }"),NewTypeInference.NULLABLE_DEREFERENCE);
}
