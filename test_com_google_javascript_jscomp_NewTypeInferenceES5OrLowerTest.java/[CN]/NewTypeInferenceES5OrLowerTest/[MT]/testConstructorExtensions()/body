{
  typeCheck("/** @constructor */ function I() {}\n" + "/** @param {number} x */\n" + "I.prototype.method = function(x) {};\n"+ "/** @constructor @extends{I} */ function C() {}\n"+ "C.prototype.method = function(x) {};\n"+ "(new C).method('asdf');",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("/** @constructor */ function I() {}\n" + "/** @param {number} x */\n" + "I.prototype.method = function(x, y) {};\n"+ "/** @constructor @extends{I} */ function C() {}\n"+ "/** @param {string} y */\n"+ "C.prototype.method = function(x, y) {};\n"+ "(new C).method('asdf', 'fgr');",NewTypeInference.INVALID_ARGUMENT_TYPE);
}
