{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/**"," * @param {function(new:Foo, (number|string))} x "," * @param {function(new:Foo, number)} y "," */","function f(x, y) {","  var z = 1 < 2 ? x : y;","  return new z(123);","}"));
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @constructor */","function Bar() {}","/**"," * @param {function(new:Foo)} x "," * @param {function(new:Bar)} y "," */","function f(x, y) {","  var z = 1 < 2 ? x : y;","  return new z();","}"),NewTypeInference.NOT_A_CONSTRUCTOR);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","function f(/** function(new:Foo) */ x, /** function() */ y) {","  var z = 1 < 2 ? x : y;","  return new z();","}"),NewTypeInference.NOT_A_CONSTRUCTOR);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","function f(/** function(new:Foo) */ x, /** function() */ y) {","  var z = 1 < 2 ? x : y;","  return z();","}"));
}
