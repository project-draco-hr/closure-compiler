{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo(x) { this.prop = x; }","function f(/** !Foo */ obj) {","  obj.prop = true ? 1 : 'asdf';","  obj.prop - 5;","}"));
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo(x) { this.prop = x; }","function f(/** !Foo */ obj) {","  if (!(typeof obj.prop == 'number')) {","    obj.prop < 'asdf';","  }","}"));
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo(x) { this.prop = x; }","function f(/** !Foo */ obj) {","  obj.prop = true ? 1 : 'asdf';","  obj.prop - 5;","  obj.prop < 'asdf';","}"),NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck(Joiner.on('\n').join("function /** string */ f(/** ?number */ x) {","  var o = { prop: 'str' };","  if (x) {","    o.prop = x;","  }","  return o.prop;","}"),NewTypeInference.RETURN_NONDECLARED_TYPE);
}
