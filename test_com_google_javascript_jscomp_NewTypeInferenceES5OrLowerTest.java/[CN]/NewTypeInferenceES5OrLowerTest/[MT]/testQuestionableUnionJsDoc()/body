{
  typeCheck("/** @type {string|?} */ var x;");
  typeCheck(Joiner.on('\n').join("goog.forwardDeclare('a');","/** @type {a|number} */","var x;"));
  typeCheck(Joiner.on('\n').join("","/**"," * @return {T|S}"," * @template T, S"," */","function f(){};"));
  typeCheck("/** @param {(?)} x */ function f(x) {}");
}
