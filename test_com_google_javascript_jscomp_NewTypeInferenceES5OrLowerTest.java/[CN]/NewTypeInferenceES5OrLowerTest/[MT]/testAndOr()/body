{
  typeCheck("function f(x, y, z) { return x || y && z;}");
  typeCheck(Joiner.on('\n').join("function f(/** number */ x, /** string */ y) {","  var /** number */ n = x || y;","}"),NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck(Joiner.on('\n').join("function f(/** number */ x, /** string */ y) {","  var /** number */ n = y || x;","}"),NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck(Joiner.on('\n').join("function /** number */ f(/** ?number */ x) {","  return x || 42;","}"));
  typeCheck(Joiner.on('\n').join("function /** (number|string) */ f(/** ?number */ x) {","  return x || 'str';","}"));
}
