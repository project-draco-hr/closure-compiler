{
  typeCheck("function f(x, y, z) { return x || y && z;}");
  typeCheck("function f(/** number */ x, /** string */ y) {\n" + "  var /** number */ n = x || y;\n" + "}",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("function f(/** number */ x, /** string */ y) {\n" + "  var /** number */ n = y || x;\n" + "}",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("function /** number */ f(/** ?number */ x) {\n" + "  return x || 42;\n" + "}");
  typeCheck("function /** (number|string) */ f(/** ?number */ x) {\n" + "  return x || 'str';\n" + "}");
}
