{
  typeCheck(Joiner.on('\n').join("/** @constructor */ function Foo() {}","function takesObj(/** Object */ x) {}","takesObj(new Foo);"));
  typeCheck(Joiner.on('\n').join("function takesObj(/** Object */ x) {}","takesObj(null);"));
  typeCheck(Joiner.on('\n').join("/** @constructor */ function Foo() {}","function /** Object */ returnsObj() { return {}; }","function takesFoo(/** Foo */ x) {}","takesFoo(returnsObj());"),NewTypeInference.INVALID_ARGUMENT_TYPE);
}
