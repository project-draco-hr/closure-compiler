{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @constructor */","function Bar() {}","function f(/** function(new:Foo) */ x) {}","f(Bar);"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","function f(/** function(new:Foo) */ x) {}","f(function() {});"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @constructor @extends {Foo} */","function Bar() {}","function f(/** function(new:Foo) */ x) {}","f(Bar);"));
}
