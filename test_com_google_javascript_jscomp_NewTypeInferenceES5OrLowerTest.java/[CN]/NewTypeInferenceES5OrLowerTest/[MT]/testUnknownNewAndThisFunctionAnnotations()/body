{
  typeCheck(Joiner.on('\n').join("/** @this {Number|String} */","function f() {","  return this.toString();","}"));
  typeCheck(Joiner.on('\n').join("/** @type {function(this:(Number|String))} */","function f() {","  return this.toString();","}"));
  typeCheck(Joiner.on('\n').join("/** @type {function(new:(Number|String))} */","function f() {}","var x = new f();"));
}
