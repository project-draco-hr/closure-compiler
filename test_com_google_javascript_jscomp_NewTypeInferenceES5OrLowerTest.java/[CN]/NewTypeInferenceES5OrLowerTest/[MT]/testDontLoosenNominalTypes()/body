{
  typeCheck(Joiner.on('\n').join("/** @constructor */ function Foo() { this.prop = 123; }","function f(x) { if (x instanceof Foo) { var y = x.prop; } }"));
  typeCheck(Joiner.on('\n').join("/** @constructor */ function Foo() { this.prop = 123; }","/** @constructor */ function Bar() { this.prop = 123; }","function f(cond, x) {","  x = cond ? new Foo : new Bar;","  var y = x.prop;","}"));
}
