{
  typeCheck(Joiner.on('\n').join("/** @constructor @extends {Bar} */","function Foo() {}","/** @interface */","function Bar() {}"),TypeCheck.CONFLICTING_EXTENDED_TYPE);
  typeCheck(Joiner.on('\n').join("/** @constructor @implements {Bar} */","function Foo() {}","/** @constructor */","function Bar() {}"),RhinoErrorReporter.BAD_JSDOC_ANNOTATION);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @interface @implements {Foo} */","function Bar() {}"),RhinoErrorReporter.BAD_JSDOC_ANNOTATION);
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @interface @extends {Foo} */","function Bar() {}"),RhinoErrorReporter.BAD_JSDOC_ANNOTATION);
}
