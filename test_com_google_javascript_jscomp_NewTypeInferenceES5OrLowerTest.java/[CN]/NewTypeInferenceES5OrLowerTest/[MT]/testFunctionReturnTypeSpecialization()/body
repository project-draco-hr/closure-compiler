{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {}","/** @return {?boolean} */","Foo.prototype.method = function() { return true; };","function f(/** !Foo */ x) {","  var /** boolean */ b = x.method() || true;","  b = x.method();","}"),NewTypeInference.MISTYPED_ASSIGN_RHS);
}
