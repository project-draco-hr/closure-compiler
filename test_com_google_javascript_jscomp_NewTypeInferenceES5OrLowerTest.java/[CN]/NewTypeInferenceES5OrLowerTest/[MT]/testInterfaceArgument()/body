{
  typeCheck(Joiner.on('\n').join("/** @interface */","function I() {}","/** @param {number} x */","I.prototype.method = function(x) {};","/** @param {!I} x */","function foo(x) { x.method('asdf'); }"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("/** @interface */","function IParent() {}","/** @param {number} x */","IParent.prototype.method = function(x) {};","/** @interface @extends {IParent} */","function IChild() {}","/** @param {!IChild} x */","function foo(x) { x.method('asdf'); }"),NewTypeInference.INVALID_ARGUMENT_TYPE);
}
