{
  typeCheck(LINE_JOINER.join("var x = 5;","for (;true;) {","  x = 'str';","}","var /** (string|number) */ y = x;","(function(/** string */ s){})(x);"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(LINE_JOINER.join("var x = 5;","while (true) {","  x = 'str';","}","(function(/** string */ s){})(x);","var /** (string|number) */ y = x;"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(LINE_JOINER.join("while (true) {","  var x = 'str';","}","var /** (string|undefined) */ y = x;","(function(/** string */ s){})(x);"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(LINE_JOINER.join("for (var x = 5; x < 10; x++) {}","(function(/** string */ s){})(x);","var /** number */ y = x;"),NewTypeInference.INVALID_ARGUMENT_TYPE);
}
