{
  typeCheck(Joiner.on('\n').join("function f() { return 'str'; }","var x = f();","x - 7;"),NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck(Joiner.on('\n').join("function f() { return 'str'; }","f() - 5;"),NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck(Joiner.on('\n').join("(function() {","  function f() { return 'str'; }","  f() - 5;","})();"),NewTypeInference.INVALID_OPERAND_TYPE);
  typeCheck(Joiner.on('\n').join("(function() {","  function f() { return 'str'; }","  f() - 5;","})();"),NewTypeInference.INVALID_OPERAND_TYPE);
}
