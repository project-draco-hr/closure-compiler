{
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {","  /** @const */ var x = this","}"));
  typeCheck(Joiner.on('\n').join("/** @constructor */","function Foo() {","  /** @type {string} */","  this.p = 'asdf';","}","Foo.prototype.m = function() {","  goog.bind(function() { this.p - 5; }, this);","};"),NewTypeInference.INVALID_OPERAND_TYPE);
}
