{
  typeCheck("/** @interface */ function Foo() {}\n" + "function reqFoo(/** Foo */ foo) {};\n" + "/** @param {Function} fun */\n"+ "function f(fun) {\n"+ "    reqFoo(fun);\n"+ "}",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("/** @constructor */ function Foo(){}\n" + "function f(x) {\n" + "  if (typeof x == 'function') {\n"+ "    var /** !Foo */ y = x;\n"+ "  }\n"+ "}",NewTypeInference.MISTYPED_ASSIGN_RHS);
}
