{
  typeCheck(Joiner.on('\n').join("function f(x) { x - 5; }","f(123);","f('asdf')"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("function f(x) { x++; }","f(123);","f('asdf')"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("function f(y) { var x = y; x - 5; }","f(123);","f('asdf')"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("function f(y) { var x; x = y; x - 5; }","f(123);","f('asdf')"),NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck(Joiner.on('\n').join("function f(x) { x + 5; }","f(123);","f('asdf')"));
}
