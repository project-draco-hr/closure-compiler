{
  typeCheck("function f(x) { x - 5; }\n" + "f(123);\n" + "f('asdf')",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("function f(x) { x++; }" + "f(123);\n" + "f('asdf')",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("function f(y) { var x = y; x - 5; }" + "f(123);\n" + "f('asdf')",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("function f(y) { var x; x = y; x - 5; }" + "f(123);\n" + "f('asdf')",NewTypeInference.INVALID_ARGUMENT_TYPE);
  typeCheck("function f(x) { x + 5; }" + "f(123);\n" + "f('asdf')");
}
