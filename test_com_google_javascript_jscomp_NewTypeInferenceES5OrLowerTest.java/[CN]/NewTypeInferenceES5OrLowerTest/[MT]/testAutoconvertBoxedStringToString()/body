{
  typeCheck("var /** !String */ s = '';",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("var /** string */ s = new String('');",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck(Joiner.on('\n').join("function f() {","  var /** !String */ x;","  for (x in { p1: 123, p2: 234 }) ;","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !String */ x) {","  return x + 1;","}"));
}
