{
  typeCheck(Joiner.on('\n').join("function f() {","  x < 'str';","}","var x = 5;","f()"),NewTypeInference.CROSS_SCOPE_GOTCHA);
  typeCheck(Joiner.on('\n').join("/** @type {string} */ var s;","function f() {","  s = 123;","}","f();"),NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck(Joiner.on('\n').join("function g(x) {","  function f() { x < 'str'; z < 'str'; x = 5; }","  var z = x;","  f();","  x - 5;","  z < 'str';","}"));
}
