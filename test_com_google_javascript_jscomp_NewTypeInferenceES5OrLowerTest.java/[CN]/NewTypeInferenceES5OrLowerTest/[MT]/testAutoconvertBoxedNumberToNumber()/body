{
  typeCheck("var /** !Number */ n = 123;",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck("var /** number */ n = new Number(123);",NewTypeInference.MISTYPED_ASSIGN_RHS);
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x, y) {","  return x - y;","}"));
  typeCheck(Joiner.on('\n').join("function f(x, /** !Number */ y) {","  return x - y;","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x) {","  return x + 'asdf';","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x) {","  return -x;","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x) {","  x -= 123;","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x, y) {","  y -= x;","}"));
  typeCheck(Joiner.on('\n').join("function f(/** !Number */ x, /** !Array<string>*/ arr) {","  return arr[x];","}"));
}
