{
  typeCheck("(/** @type {number} */ ('asdf'));",TypeValidator.INVALID_CAST);
  typeCheck("function f(/** (number|string) */ x) {\n" + "  var y = /** @type {number} */ (x);\n" + "}");
  typeCheck("(/** @type {(number|string)} */ (1));");
  typeCheck("(/** @type {number} */ (/** @type {?} */ ('asdf')))");
  typeCheck("/** @constructor */\n" + "function Parent() {}\n" + "/** @constructor @extends {Parent} */\n"+ "function Child() {}\n"+ "/** @type {Child|null} */ (new Parent);\n");
}
