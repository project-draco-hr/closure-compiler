{
  test(Joiner.on('\n').join("class Child {","  /** @param {Child} child */","  useChild(child) {}","}"),Joiner.on('\n').join("class Child$$module$testcode {","  /** @param {Child$$module$testcode} child */","  useChild(child) {}","}"));
  test(Joiner.on('\n').join("class Child {","  /** @param {Child.Foo.Bar.Baz} baz */","  useBaz(baz) {}","}"),Joiner.on('\n').join("class Child$$module$testcode {","  /** @param {Child$$module$testcode.Foo.Bar.Baz} baz */","  useBaz(baz) {}","}"));
}
