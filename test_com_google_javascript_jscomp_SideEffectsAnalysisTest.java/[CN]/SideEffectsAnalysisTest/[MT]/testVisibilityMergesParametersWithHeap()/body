{
  assertUnsafeMoveVisibility("var x = {};" + "function f(y){" + "src: x[0]++;"+ "env: y;"+ "dest: 3;"+ "}");
  assertUnsafeMoveVisibility("var x = {};" + "function f(y){" + "src: x[0];"+ "env: y++;"+ "dest: 3;"+ "}");
  assertUnsafeMoveVisibility("var x = {};" + "function f(y){" + "src: arguments[0]++;"+ "env: y;"+ "dest: 3;"+ "}");
  assertUnsafeMoveVisibility("var x = {};" + "function f(y){" + "src: arguments[0];"+ "env: y++;"+ "dest: 3;"+ "}");
}
