{
  assertSafeMoveDegenerate("src: 1; env: ; dest: 3;");
  assertSafeMoveDegenerate("src: 1; env: 2; dest: 3;");
  assertSafeMoveDegenerate("src: 1; env: x; dest: 3;");
  assertSafeMoveDegenerate("src: x; env: 1; dest: 3;");
  assertSafeMoveDegenerate("src: 1; env: x++; dest: 3;");
  assertSafeMoveDegenerate("src: x++; env: 1; dest: 3;");
}
