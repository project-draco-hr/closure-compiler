{
  testValidation("" + "['LogicalAndExpr',{}," + "['IdExpr',{'name':'a'}],"+ "['IdExpr',{'name':'b'}]]");
  testValidation("" + "['LogicalOrExpr',{}," + "['IdExpr',{'name':'a'}],"+ "['IdExpr',{'name':'b'}]]");
  testTooManyChildrenValidation("" + "['LogicalAndExpr',{}," + "['IdExpr',{'name':'a'}],"+ "['IdExpr',{'name':'b'}],"+ "['IdExpr',{'name':'c'}]]",TagType.LogicalAndExpr,2,3);
  testNotEnoughChildrenValidation("" + "['LogicalAndExpr',{}," + "['IdExpr',{'name':'a'}]]",TagType.LogicalAndExpr,2,1);
}
