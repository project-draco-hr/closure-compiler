{
  testValidation("" + "['UnaryExpr',{'op':'-'}," + "['IdExpr',{'name':'x'}]]");
  testValidation("" + "['UnaryExpr',{'op':'!'}," + "['CallExpr',{},"+ "['IdExpr',{'name':'f'}],"+ "['IdExpr',{'name':'x'}]]]");
  testNotEnoughChildrenValidation("" + "['UnaryExpr',{'op':'-'}]",TagType.UnaryExpr,1,0);
  testTooManyChildrenValidation("" + "['UnaryExpr',{'op':'+'}," + "['IdExpr',{'name':'x'}],"+ "['IdExpr',{'name':'y'}]]",TagType.UnaryExpr,1,2);
  testMissingArgument("" + "['UnaryExpr',{}," + "['IdExpr',{'name':'x'}]]",TagAttr.OP,TagType.UnaryExpr);
}
