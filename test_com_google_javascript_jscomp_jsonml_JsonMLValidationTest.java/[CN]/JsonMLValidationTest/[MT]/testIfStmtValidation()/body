{
  testValidation("" + "['IfStmt',{}," + "['LiteralExpr',{'type':'boolean','value':true}],"+ "['BlockStmt',{}],"+ "['EmptyStmt',{}]]");
  testValidation("" + "['IfStmt',{}," + "['LiteralExpr',{'type':'boolean','value':true}],"+ "['BlockStmt',{}],"+ "['BlockStmt',{}]]");
  testNotEnoughChildrenValidation("" + "['IfStmt',{}," + "['LiteralExpr',{'type':'boolean','value':true}],"+ "['BlockStmt',{}]]",TagType.IfStmt,3,2);
