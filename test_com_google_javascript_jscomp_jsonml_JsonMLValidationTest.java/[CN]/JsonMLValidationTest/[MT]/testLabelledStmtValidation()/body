{
  testValidation("" + "['LabelledStmt',{'label':'s'}," + "['IdExpr',{'name':'x'}]]");
  testNotEnoughChildrenValidation("" + "['LabelledStmt',{'label':'s'}]",TagType.LabelledStmt,1,0);
  testTooManyChildrenValidation("" + "['LabelledStmt',{'label':'s'}," + "['IdExpr',{'name':'x'}],"+ "['IdExpr',{'name':'y'}]]",TagType.LabelledStmt,1,2);
  testMissingArgument("" + "['LabelledStmt',{}," + "['IdExpr',{'name':'x'}]]",TagAttr.LABEL,TagType.LabelledStmt);
}
