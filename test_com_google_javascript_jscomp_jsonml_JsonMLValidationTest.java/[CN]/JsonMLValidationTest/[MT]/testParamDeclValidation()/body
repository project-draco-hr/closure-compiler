{
  testValidation("" + "['ParamDecl',{}]");
  testValidation("" + "['ParamDecl',{}," + "['IdPatt',{'name':'x'}],"+ "['IdPatt',{'name':'y'}]]");
  testWrongChildTypeValidation("" + "['ParamDecl',{}," + "['IdPatt',{'name':'x'}],"+ "['IdExpr',{'name':'y'}]]",TagType.ParamDecl,TagType.IdPatt,TagType.IdExpr,1);
}
