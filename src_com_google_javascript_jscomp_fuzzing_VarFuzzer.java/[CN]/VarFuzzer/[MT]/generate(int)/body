{
  Node identifier=new IdentifierFuzzer(random,scopeManager,config,snGenerator).generate(1);
  Node node=new Node(Token.VAR,identifier);
  ExpressionFuzzer exprFuzzer=new ExpressionFuzzer(random,scopeManager,config,snGenerator);
  if (exprFuzzer.isEnough(budget - 2)) {
    Node assn=exprFuzzer.generate(budget - 2);
    identifier.addChildToBack(assn);
  }
  return node;
}
