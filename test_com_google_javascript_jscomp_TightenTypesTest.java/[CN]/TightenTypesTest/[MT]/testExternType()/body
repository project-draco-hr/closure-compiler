{
  testSame("/** @constructor */ function T() {};\n" + "/** @constructor */ function Ext() {};\n" + "/** @return T */\n"+ "Ext.prototype.getT = function() {};\n"+ "/** @type T */ Ext.prototype.prop;\n"+ "/** @type Ext */ var ext;","var b = ext.getT();\n" + "var p = ext.prop;",null);
  assertType("Ext",getType("ext"));
  assertType("T",getType("b"));
  assertType("T",getType("p"));
}
