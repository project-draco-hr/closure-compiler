{
  test("var a = 1; f1(); var b;","var a = 1, b; f1();");
  test("var a = \"mangoes\"; f1(); alert(a); var b;","var a = \"mangoes\", b; f1(); alert(a);");
  test("var a = 1; {var c; alert(c);} var b;","var a = 1, c, b; {alert(c);}");
  test("var a = 1; var b = 1; f1(); f2(); var c; var d;","var a = 1, b, c, d; b = 1; f1(); f2();");
  test("var a = 1; var b = 2; var c; f1(); f2(); var d, e;","var a = 1, b, c, d, e; b = 2; f1(); f2();");
  test("var a = 1, b = 2, c; f1(); f2(); var d; var e; " + "f3(); f4(); var f = 10; var g; var h = a + b;","var a = 1, b = 2, c, d, e, f, g, h; f1(); f2(); f3(); f4(); " + "f = 10; h = a + b;");
}
