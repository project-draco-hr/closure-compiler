{
  String comment=node.value;
  int lineno=node.location.start.line + 1;
  int charno=node.location.start.column;
  int numOpeningChars=3;
  JsDocInfoParser parser=new JsDocInfoParser(new JsDocTokenStream(comment.substring(numOpeningChars),lineno,charno + numOpeningChars),comment,node.location.start.offset,irNode,config,errorReporter);
  return parser.parseInlineTypeDoc();
}
