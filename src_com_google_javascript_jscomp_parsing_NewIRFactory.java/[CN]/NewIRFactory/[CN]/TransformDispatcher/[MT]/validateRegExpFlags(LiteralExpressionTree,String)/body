{
  for (  char flag : Lists.charactersOf(flags)) {
switch (flag) {
case 'g':
case 'i':
case 'm':
      break;
case 'u':
case 'y':
    maybeWarnEs6Feature(tree,"new RegExp flag '" + flag + "'");
  break;
default :
errorReporter.error("Invalid RegExp flag '" + flag + "'",sourceName,lineno(tree),"",charno(tree));
}
}
}
