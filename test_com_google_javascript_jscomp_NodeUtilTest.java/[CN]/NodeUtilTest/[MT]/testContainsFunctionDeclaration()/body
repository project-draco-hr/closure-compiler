{
  assertTrue(NodeUtil.containsFunctionDeclaration(getNode("function foo(){}")));
  assertTrue(NodeUtil.containsFunctionDeclaration(getNode("(b?function(){}:null)")));
  assertFalse(NodeUtil.containsFunctionDeclaration(getNode("(b?foo():null)")));
  assertFalse(NodeUtil.containsFunctionDeclaration(getNode("foo()")));
}
