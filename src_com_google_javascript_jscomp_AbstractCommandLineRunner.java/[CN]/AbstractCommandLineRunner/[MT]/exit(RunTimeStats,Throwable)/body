{
  int result=0;
  if (error instanceof AbstractCommandLineRunner.FlagUsageException) {
    System.err.println(error.getMessage());
    result=-1;
  }
 else   if (error != null) {
    error.printStackTrace();
    result=-2;
  }
  if (config.computePhaseOrdering) {
    runTimeStats.outputBestPhaseOrdering();
  }
  System.exit(result);
}
