{
  if (isInTestMode()) {
    return inputsSupplierForTesting.get();
  }
  if (files.isEmpty()) {
    files=Collections.singletonList("-");
  }
  try {
    return createInputs(files,true);
  }
 catch (  FlagUsageException e) {
    throw new FlagUsageException("Bad --js flag. " + e.getMessage());
  }
}
