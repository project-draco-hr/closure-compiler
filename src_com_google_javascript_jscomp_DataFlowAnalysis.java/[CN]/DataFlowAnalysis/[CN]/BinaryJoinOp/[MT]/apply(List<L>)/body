{
  Preconditions.checkArgument(!values.isEmpty());
  int size=values.size();
  if (size == 1) {
    return values.get(0);
  }
 else   if (size == 2) {
    return apply(values.get(0),values.get(1));
  }
 else {
    int mid=computeMidPoint(size);
    return apply(apply(values.subList(0,mid)),apply(values.subList(mid,size)));
  }
}
