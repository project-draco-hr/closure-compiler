{
  assertPrint("function(){if(e1){do foo();while(e2)}else foo()}","function(){if(e1){do foo();while(e2)}else foo()}");
  assertPrint("function(){if(e1)do foo();while(e2)else foo()}","function(){if(e1){do foo();while(e2)}else foo()}");
  assertPrint("if(x){do{foo()}while(y)}else bar()","if(x){do foo();while(y)}else bar()");
  assertPrint("if(x)do{foo()}while(y);else bar()","if(x){do foo();while(y)}else bar()");
}
