{
  assertPrintSame("var x=\"\\b\"");
  assertPrintSame("var x=\"\\f\"");
  assertPrintSame("var x=\"\\n\"");
  assertPrintSame("var x=\"\\r\"");
  assertPrintSame("var x=\"\\t\"");
  assertPrintSame("var x=\"\\v\"");
  assertPrint("var x=\"\\\"\"","var x='\"'");
  assertPrint("var x=\"\\\'\"","var x=\"'\"");
  assertPrintSame("var x=/\\b/");
  assertPrintSame("var x=/\\f/");
  assertPrintSame("var x=/\\n/");
  assertPrintSame("var x=/\\r/");
  assertPrintSame("var x=/\\t/");
  assertPrintSame("var x=/\\v/");
}
