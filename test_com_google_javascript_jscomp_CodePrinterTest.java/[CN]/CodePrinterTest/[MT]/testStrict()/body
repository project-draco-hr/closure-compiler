{
  String result=new CodePrinter.Builder(parse("var x",true)).setCompilerOptions(newCompilerOptions(new CompilerOptionBuilder(){
    @Override void setOptions(    CompilerOptions options){
      options.setPrettyPrint(false);
      options.setLineBreak(false);
      options.setLineLengthThreshold(0);
    }
  }
)).setOutputTypes(false).setTypeRegistry(lastCompiler.getTypeIRegistry()).setTagAsStrict(true).build();
  assertEquals("'use strict';var x",result);
}
