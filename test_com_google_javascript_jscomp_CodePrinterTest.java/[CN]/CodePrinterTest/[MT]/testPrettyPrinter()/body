{
  assertPrettyPrint("(function(){})();","(function() {\n})()");
  assertPrettyPrint("var a = (function() {});alert(a);","var a = function() {\n};\nalert(a)");
  assertPrettyPrint("if (1) {}","if(1);");
  assertPrettyPrint("if (1) {alert(\"\");}","if(1) {\n" + "  alert(\"\")\n" + "}");
  assertPrettyPrint("if (1)alert(\"\");","if(1) {\n" + "  alert(\"\")\n" + "}");
  assertPrettyPrint("if (1) {alert();alert();}","if(1) {\n" + "  alert();\n" + "  alert()\n"+ "}");
  assertPrettyPrint("label: alert();","label:alert()");
  assertPrettyPrint("if (1) alert();","if(1) {\n" + "  alert()\n" + "}");
  assertPrettyPrint("for (;;) alert();","for(;;) {\n" + "  alert()\n" + "}");
  assertPrettyPrint("while (1) alert();","while(1) {\n" + "  alert()\n" + "}");
  assertPrettyPrint("if (1) {} else {alert(a);}","if(1);else {\n  alert(a)\n}");
  assertPrettyPrint("if (1) alert(a); else alert(b);","if(1) {\n" + "  alert(a)\n" + "}else {\n"+ "  alert(b)\n"+ "}");
  assertPrettyPrint("for(;;) { alert();}","for(;;) {\n" + "  alert()\n" + "}");
  assertPrettyPrint("for(;;) {}","for(;;);");
  assertPrettyPrint("for(;;) { alert(); alert(); }","for(;;) {\n" + "  alert();\n" + "  alert()\n"+ "}");
  assertPrettyPrint("do { alert(); } while(true);","do {\n" + "  alert()\n" + "}while(true)");
  assertPrettyPrint("myLabel: { alert();}","myLabel: {\n" + "  alert()\n" + "}");
  assertPrettyPrint("myLabel: for(;;) continue myLabel;","myLabel:for(;;) {\n" + "  continue myLabel\n" + "}");
}
