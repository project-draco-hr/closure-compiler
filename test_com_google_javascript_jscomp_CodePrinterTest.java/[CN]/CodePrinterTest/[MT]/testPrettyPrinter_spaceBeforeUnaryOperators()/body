{
  languageMode=LanguageMode.ECMASCRIPT6;
  assertPrettyPrint("var f = function() { return !b; };","var f = function() {\n" + "  return !b;\n" + "};\n");
  assertPrettyPrint("var f = function*(){yield -b}","var f = function*() {\n" + "  yield -b;\n" + "};\n");
  assertPrettyPrint("var f = function() { return +b; };","var f = function() {\n" + "  return +b;\n" + "};\n");
  assertPrettyPrint("var f = function() { throw ~b; };","var f = function() {\n" + "  throw ~b;\n" + "};\n");
  assertPrettyPrint("var f = function() { return ++b; };","var f = function() {\n" + "  return ++b;\n" + "};\n");
  assertPrettyPrint("var f = function() { return --b; };","var f = function() {\n" + "  return --b;\n" + "};\n");
}
