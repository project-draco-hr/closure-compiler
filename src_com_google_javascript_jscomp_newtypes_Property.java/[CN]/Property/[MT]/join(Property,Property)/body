{
  JSType declType, p1decl=p1.declaredType, p2decl=p2.declaredType;
  if (p1decl == null || p2decl == null) {
    declType=null;
  }
 else   if (p1decl.equals(p2decl)) {
    declType=p1decl;
  }
 else {
    declType=null;
  }
  return new Property(JSType.join(p1.inferredType,p2.inferredType),declType,p1.isOptional || p2.isOptional);
}
