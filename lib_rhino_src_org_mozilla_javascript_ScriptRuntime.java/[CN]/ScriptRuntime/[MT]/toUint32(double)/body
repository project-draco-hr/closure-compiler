{
  long l=(long)d;
  if (l == d) {
    return l & 0xffffffffL;
  }
  if (Double.isNaN(d) || d == Double.POSITIVE_INFINITY || d == Double.NEGATIVE_INFINITY) {
    return 0;
  }
  d=(d >= 0) ? Math.floor(d) : Math.ceil(d);
  double two32=4294967296.0;
  l=(long)Math.IEEEremainder(d,two32);
  return l & 0xffffffffL;
}
