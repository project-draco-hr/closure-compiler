{
  testSame("var a = {};" + "/** @constructor */" + "a.F = function() {"+ "  for (this.a = 0; this.a < 10; ++this.a) {"+ "    this.b = 3;"+ "  }"+ "};",CheckPropertyOrder.UNASSIGNED_PROPERTY);
  testSame("var a = {};" + "/** @constructor */" + "a.F = function() {"+ "  for (; !this.b; this.b = 1) {}"+ "  this.a = 1;"+ "};",CheckPropertyOrder.UNEQUAL_PROPERTIES);
}
