{
  testSame("var a = {};" + "/** @constructor */" + "a.F = function(a) {"+ "  if (1) {"+ "    try { this.a = 2; }"+ "    finally { }"+ "  } else {"+ "    this.a = 3;"+ "  }"+ "};");
  testSame("var a = {};" + "/** @constructor */" + "a.F = function(a) {"+ "  if (1) {"+ "    try {}"+ "    finally { this.a = 2; }"+ "  } else {"+ "    this.a = 3;"+ "  }"+ "};");
  testSame("var a = {};" + "/** @constructor */" + "a.F = function(a) {"+ "  if (1) {"+ "    do {"+ "      try { break; }"+ "      finally {this.a = 2; }"+ "    } while(1);"+ "  } else {"+ "    this.a = 3;"+ "  }"+ "};");
  testSame("var a = {};" + "/** @constructor */" + "a.F = function(a) {"+ "  if (a) {"+ "    this.a = 1;"+ "  } else {"+ "    try {"+ "      this.a = (function() { throw 1; })();"+ "    } finally {"+ "    }"+ "  }"+ "};");
}
