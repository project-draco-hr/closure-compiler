{
  if (NodeUtil.isLatin(s)) {
    return s;
  }
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < s.length(); i++) {
    char c=s.charAt(i);
    if (c > 0x1F && c < 0x7F) {
      sb.append(c);
    }
 else {
      appendHexJavaScriptRepresentation(sb,c);
    }
  }
  return sb.toString();
}
