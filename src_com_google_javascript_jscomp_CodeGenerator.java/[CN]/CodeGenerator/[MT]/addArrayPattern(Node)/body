{
  add("[");
  for (Node child=n.getFirstChild(); child != null; child=child.getNext()) {
    if (child == n.getLastChild() && NodeUtil.isNameDeclaration(n.getParent())) {
      add("]");
      add("=");
    }
 else     if (child != n.getFirstChild()) {
      add(",");
    }
    add(child);
  }
  if (!NodeUtil.isNameDeclaration(n.getParent())) {
    add("]");
  }
}
