{
  boolean hasInitializer=false;
  add("[");
  for (Node child=n.getFirstChild(); child != null; child=child.getNext()) {
    if (isPatternInitializer(child)) {
      hasInitializer=true;
      add("]");
      add("=");
    }
 else     if (child != n.getFirstChild()) {
      add(",");
    }
    add(child);
  }
  if (!hasInitializer) {
    add("]");
  }
}
