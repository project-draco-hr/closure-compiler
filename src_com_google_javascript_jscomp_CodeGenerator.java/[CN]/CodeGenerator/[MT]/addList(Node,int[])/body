{
  int nextSlot=0;
  int nextSkipSlot=0;
  for (Node n=firstInList; n != null; n=n.getNext()) {
    while (skipIndexes != null && nextSkipSlot < skipIndexes.length) {
      if (nextSlot == skipIndexes[nextSkipSlot]) {
        cc.listSeparator();
        nextSlot++;
        nextSkipSlot++;
      }
 else {
        break;
      }
    }
    if (n != firstInList) {
      cc.listSeparator();
    }
    addExpr(n,1);
    nextSlot++;
  }
}
