{
  test(Joiner.on("\n").join("(function() {","  Polymer({","    is: 'x',","    sayHi: function() { alert('hi'); },","  });","})()"),Joiner.on("\n").join("/** @constructor @extends {PolymerElement} @export */","var XElement = function() {};","(function() {","  Polymer(/** @lends {XElement.prototype} */ {","    is: 'x',","    /** @this {XElement} */","    sayHi: function() { alert('hi'); },","  });","})()"));
