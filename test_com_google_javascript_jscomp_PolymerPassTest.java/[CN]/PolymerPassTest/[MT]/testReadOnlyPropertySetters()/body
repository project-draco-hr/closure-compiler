{
  String js=Joiner.on("\n").join("var a = {};","a.B = Polymer({","  is: 'x-element',","  properties: {","    pets: {","      type: Array,","      readOnly: true,","    },","    name: String,","  },","});");
  test(js,Joiner.on("\n").join("var a = {};","/** @constructor @extends {PolymerElement} @implements {Polymera_BInterface} @export */","a.B = function() {};","/** @type {!Array} @export */","a.B.prototype.pets;","/** @type {string} @export */","a.B.prototype.name;","/** @override */","a.B.prototype._setPets = function(pets) {};","a.B = Polymer(/** @lends {a.B.prototype} */ {","  is: 'x-element',","  properties: {","    pets: {","      type: Array,","      readOnly: true,","    },","    name: String,","  },","});"));
  testExternChanges(EXTERNS,js,READONLY_EXTERNS);
}
