{
  String js=LINE_JOINER.join("var a = {};","a.B = Polymer({","  is: 'x-element',","  properties: {","    /** @type {!Array<string>} */","    pets: {","      type: Array,","      readOnly: true,","    },","    name: String,","  },","});");
  test(js,LINE_JOINER.join("var a = {};","/** @constructor @extends {PolymerElement} @implements {Polymera_BInterface} */","a.B = function() {};","/** @type {!Array<string>} */","a.B.prototype.pets;","/** @type {string} */","a.B.prototype.name;","/** @override */","a.B.prototype._setPets = function(pets) {};","a.B = Polymer(/** @lends {a.B.prototype} */ {","  is: 'x-element',","  properties: {","    pets: {","      type: Array,","      readOnly: true,","    },","    name: String,","  },","});"));
  testExternChanges(EXTERNS,js,READONLY_EXTERNS);
}
