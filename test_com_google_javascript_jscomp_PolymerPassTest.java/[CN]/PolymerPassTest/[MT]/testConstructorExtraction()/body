{
  test(Joiner.on("\n").join("var X = Polymer({","  is: 'x-element',","  /**","   * @param {string} name","   */","  constructor: function(name) { alert('hi, ' + name); },","});"),Joiner.on("\n").join("/**"," * @param {string} name"," * @constructor @extends {PolymerElement}"," * @export "," */","var X = function(name) { alert('hi, ' + name); };","X = Polymer(/** @lends {X.prototype} */ {","  is: 'x-element',","  constructor: function(name) { alert('hi, ' + name); },","});"));
}
