{
  test(Joiner.on("\n").join("(function() {","  var FooThing = Polymer({","    is: 'x',","    sayHi: function() { alert('hi'); },","  });","})()"),Joiner.on("\n").join("/** @constructor @extends {PolymerElement} */","var FooThing = function() {};","(function() {","  FooThing = Polymer(/** @lends {FooThing.prototype} */ {","    is: 'x',","    /** @this {FooThing} */","    sayHi: function() { alert('hi'); },","  });","})()"));
}
