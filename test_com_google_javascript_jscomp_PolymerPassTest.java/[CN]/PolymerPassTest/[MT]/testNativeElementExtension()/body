{
  String js=Joiner.on("\n").join("Polymer({","  is: 'x-input',","  extends: 'input',","});");
  test(js,Joiner.on("\n").join("/** @constructor @extends {PolymerInputElement} */","var XInputElement = function() {};","Polymer(/** @lends {XInputElement.prototype} */ {","  is: 'x-input',","  extends: 'input',","});"));
  testExternChanges(EXTERNS,js,INPUT_EXTERNS);
}
