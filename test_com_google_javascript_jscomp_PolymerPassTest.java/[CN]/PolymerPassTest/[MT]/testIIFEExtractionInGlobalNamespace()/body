{
  test(Joiner.on("\n").join("var x = {};","(function() {","  x.Z = Polymer({","    is: 'x-element',","    sayHi: function() { alert('hi'); },","  });","})()"),Joiner.on("\n").join("var x = {};","(function() {","  /** @constructor @extends {PolymerElement} */","  x.Z = function() {};","  x.Z = Polymer(/** @lends {x.Z.prototype} */ {","    is: 'x-element',","    /** @this {x.Z} */","    sayHi: function() { alert('hi'); },","  });","})()"));
}
