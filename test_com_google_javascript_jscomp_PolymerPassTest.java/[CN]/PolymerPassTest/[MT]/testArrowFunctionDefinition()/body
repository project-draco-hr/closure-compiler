{
  setAcceptedLanguage(LanguageMode.ECMASCRIPT6);
  test(LINE_JOINER.join("var ES6Test = Polymer({","  is: 'x-element',","  sayHi: ()=>42,","});"),LINE_JOINER.join("/** "," * @constructor @extends {PolymerElement} "," * @implements {PolymerES6TestInterface} "," */","var ES6Test = function() {};","","ES6Test = Polymer(/** @lends {ES6Test.prototype} */ {","  is: 'x-element',","  /** @this {ES6Test} */","  sayHi: ()=>42,","});"));
}
