{
  testError(LINE_JOINER.join("Polymer({","  is: 'x-element',","  properties: {","    isHappy: true,","  },","});"),POLYMER_INVALID_PROPERTY);
  testError(LINE_JOINER.join("Polymer({","  is: 'x-element',","  properties: {","    isHappy: {","      value: true,","    },","  },","});"),POLYMER_INVALID_PROPERTY);
}
