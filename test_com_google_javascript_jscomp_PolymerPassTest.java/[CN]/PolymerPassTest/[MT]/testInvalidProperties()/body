{
  testSame(Joiner.on("\n").join("Polymer({","  is: 'x-element',","  properties: {","    isHappy: true,","  },","});"),POLYMER_INVALID_PROPERTY,true);
  testSame(Joiner.on("\n").join("Polymer({","  is: 'x-element',","  properties: {","    isHappy: {","      value: true,","    },","  },","});"),POLYMER_INVALID_PROPERTY,true);
}
