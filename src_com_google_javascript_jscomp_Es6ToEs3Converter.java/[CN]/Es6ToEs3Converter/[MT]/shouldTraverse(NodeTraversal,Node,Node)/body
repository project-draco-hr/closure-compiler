{
switch (n.getType()) {
case Token.FUNCTION:
    if (n.isArrowFunction()) {
      visitArrowFunction(t,n);
    }
  break;
case Token.CLASS:
checkClassSuperReferences(n);
break;
case Token.CLASS_MEMBERS:
checkMemberDefs(n);
break;
case Token.ARRAY_COMP:
case Token.OBJECT_PATTERN:
cannotConvertYet(n,Token.name(n.getType()));
return false;
}
return true;
}
