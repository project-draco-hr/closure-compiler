{
  final SpecializeModule specializeModule=new SpecializeModule(compiler,devirtualizePrototypeMethods,inlineFunctions,removeUnusedPrototypeProperties);
  return new CompilerPass(){
    @Override public void process(    Node externs,    Node root){
      specializeModule.process(externs,root);
      new VarCheck(compiler).process(externs,root);
    }
  }
;
}
