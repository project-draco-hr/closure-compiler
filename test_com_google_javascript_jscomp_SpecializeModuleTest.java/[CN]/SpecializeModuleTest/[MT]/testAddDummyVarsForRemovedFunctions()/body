{
  JSModule[] modules=createModuleStar("var A = function() {alert(B() + C());A()};" + "var B = function() {return 6};" + "var C = function() {return 8};"+ "A();","" + "A();");
  test(modules,new String[]{"var A = function() {alert(6 + 8);A()};" + "A();" + "var B, C;","A = function() {alert(B() + C());A()};" + "B = function() {return 6};" + "C = function() {return 8};"+ "A();"});
}
