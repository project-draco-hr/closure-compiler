{
  setAcceptedLanguage(LanguageMode.ECMASCRIPT6);
  disableTypeCheck();
  testSame(LINE_JOINER.join("class A {","  method1() {}","  method2() {}","}"));
  testError(LINE_JOINER.join("class A {","  method1() {}","  method1() {}","}"),StrictModeCheck.DUPLICATE_CLASS_METHODS);
  testError(LINE_JOINER.join("class A {","  method() {","    for(;;) {","      function a(){}","    }","  }","}"),StrictModeCheck.BAD_FUNCTION_DECLARATION);
  testSame(LINE_JOINER.join("class A {","  method() {this.method.caller}","}"));
  testSame(LINE_JOINER.join("class A {","  method() {this.method.arguments}","}"));
  testSame(LINE_JOINER.join("class A {","  method() {","    var obj = {a : 1, a : 2}","  }","}"),StrictModeCheck.DUPLICATE_OBJECT_KEY);
  testSame(LINE_JOINER.join("class A {","  constructor() {this.a = 1;}","  method() {this.a = b}","}"),StrictModeCheck.UNKNOWN_VARIABLE);
  testSame(LINE_JOINER.join("class A {","  methodA() {}","  methodB() {delete this.methodA}","}"));
  testSame(LINE_JOINER.join("class A {","  constructor() {this.x = 1;}","  method() {","    with (this.x) {}","  }","}"),StrictModeCheck.USE_OF_WITH);
  testSame(LINE_JOINER.join("class A {","  method(eval) {}","}"),StrictModeCheck.EVAL_DECLARATION);
  testSame(LINE_JOINER.join("class A {","  method() {var eval = 1;}","}"),StrictModeCheck.EVAL_DECLARATION);
  testSame(LINE_JOINER.join("class A {","  method() {eval = 1}","}"),StrictModeCheck.EVAL_ASSIGNMENT);
  testSame(LINE_JOINER.join("class A {","  method(arguments) {}","}"),StrictModeCheck.ARGUMENTS_DECLARATION);
  testSame(LINE_JOINER.join("class A {","  method() {var arguments = 1;}","}"),StrictModeCheck.ARGUMENTS_DECLARATION);
  testSame(LINE_JOINER.join("class A {","  method() {arguments = 1}","}"),StrictModeCheck.ARGUMENTS_ASSIGNMENT);
  testSame(LINE_JOINER.join("class A {","  method() {arguments.callee}","}"),StrictModeCheck.ARGUMENTS_CALLEE_FORBIDDEN);
  testSame(LINE_JOINER.join("class A {","  method() {arguments.caller}","}"),StrictModeCheck.ARGUMENTS_CALLER_FORBIDDEN);
}
