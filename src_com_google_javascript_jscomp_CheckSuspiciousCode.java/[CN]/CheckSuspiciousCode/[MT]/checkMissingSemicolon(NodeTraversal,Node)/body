{
switch (n.getType()) {
case IF:
    Node trueCase=n.getSecondChild();
  reportIfWasEmpty(t,trueCase);
Node elseCase=trueCase.getNext();
if (elseCase != null) {
reportIfWasEmpty(t,elseCase);
}
break;
case WHILE:
case FOR:
case FOR_OF:
reportIfWasEmpty(t,NodeUtil.getLoopCodeBlock(n));
break;
default :
break;
}
}
