{
  if (type.isNoResolvedType() || (type.isNamedType() && type.isUnknownType())) {
    return true;
  }
  if (type.isUnionType()) {
    for (    JSType alternate : type.toMaybeUnionType().getAlternates()) {
      if (isUnresolvedType(alternate)) {
        return true;
      }
    }
  }
  return false;
}
