{
  String name=null;
  ScopeManager scopeManager=context.scopeManager;
  if (scopeManager.hasNonLocals() && context.random.nextDouble() < getOwnConfig().get("shadow").getAsDouble()) {
    Symbol symbol=scopeManager.getRandomSymbol(ScopeManager.EXCLUDE_EXTERNS | ScopeManager.EXCLUDE_LOCALS);
    if (symbol != null) {
      name=symbol.name;
    }
  }
  if (name == null) {
    name="x_" + context.snGenerator.getNextNumber();
  }
  scopeManager.addSymbol(new Symbol(name));
  return Node.newString(Token.NAME,name);
}
