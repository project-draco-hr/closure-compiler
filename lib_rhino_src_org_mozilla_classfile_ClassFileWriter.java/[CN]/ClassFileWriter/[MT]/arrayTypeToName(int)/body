{
switch (type) {
case ByteCode.T_BOOLEAN:
    return 'Z';
case ByteCode.T_CHAR:
  return 'C';
case ByteCode.T_FLOAT:
return 'F';
case ByteCode.T_DOUBLE:
return 'D';
case ByteCode.T_BYTE:
return 'B';
case ByteCode.T_SHORT:
return 'S';
case ByteCode.T_INT:
return 'I';
case ByteCode.T_LONG:
return 'J';
default :
throw new IllegalArgumentException("bad operand");
}
}
