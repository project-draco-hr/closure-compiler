{
  checkNotNull(moduleRoots);
  checkNotNull(inputs);
  checkNotNull(pathResolver);
  this.pathResolver=pathResolver;
  this.errorHandler=errorHandler;
  this.moduleRootUris=createRootUris(Iterables.transform(moduleRoots,pathResolver));
  this.moduleUris=resolveUris(Iterables.transform(Iterables.transform(inputs,UNWRAP_DEPENDENCY_INFO),pathResolver),moduleRootUris);
}
