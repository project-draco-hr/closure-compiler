{
  testSame("function f() {var arguments}");
  testError("var f = function arguments() {}",VarCheck.VAR_ARGUMENTS_SHADOWED_ERROR);
  testError("var f = function (arguments) {}",VarCheck.VAR_ARGUMENTS_SHADOWED_ERROR);
  testError("function f() {try {} catch(arguments) {}}",VarCheck.VAR_ARGUMENTS_SHADOWED_ERROR);
}
