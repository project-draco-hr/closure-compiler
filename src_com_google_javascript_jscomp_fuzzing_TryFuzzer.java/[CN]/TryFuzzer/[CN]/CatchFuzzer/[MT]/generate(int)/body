{
  Node catchBlock=new Node(Token.BLOCK);
  if (isEnough(budget)) {
    Node param=new IdentifierFuzzer(random,scopeManager,config,snGenerator).generate(1);
    catchBlock.addChildToBack(new Node(Token.CATCH,param,new BlockFuzzer(random,scopeManager,config,snGenerator).generate(budget - 1)));
    scopeManager.removeSymbol(param.getQualifiedName());
  }
  return catchBlock;
}
