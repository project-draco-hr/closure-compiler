{
  Node catchBlock=new Node(Token.BLOCK);
  if (isEnough(budget)) {
    Node param=new IdentifierFuzzer(context).generate(1);
    catchBlock.addChildToBack(new Node(Token.CATCH,param,new BlockFuzzer(context).generate(budget - 1)));
    context.scopeManager.removeSymbol(param.getQualifiedName());
  }
  return catchBlock;
}
