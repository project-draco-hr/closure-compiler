{
  boolean assigned=false;
  int size=references.size();
  for (int i=0; i < size; i++) {
    Reference ref=references.get(i);
    if (ref.isLvalue() || ref.isInitializingDeclaration()) {
      if (!assigned) {
        assigned=true;
      }
 else {
        return false;
      }
    }
  }
  return assigned;
}
