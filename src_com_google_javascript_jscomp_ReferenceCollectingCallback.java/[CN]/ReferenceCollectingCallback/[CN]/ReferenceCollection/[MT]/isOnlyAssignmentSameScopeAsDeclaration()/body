{
  Reference ref=getOneAndOnlyAssignment();
  Preconditions.checkNotNull(ref);
  for (BasicBlock block=ref.getBasicBlock(); block != null; block=block.getParent()) {
    if (block.isFunction) {
      if (ref.getSymbol().getScope() != ref.scope) {
        return false;
      }
    }
  }
  return true;
}
