{
  BasicBlock currentBlock;
  for (currentBlock=thatBlock; currentBlock != null && currentBlock != this; currentBlock=currentBlock.getParent()) {
  }
  if (currentBlock == this) {
    return true;
  }
  if (isGlobalScopeBlock() && thatBlock.isGlobalScopeBlock()) {
    return true;
  }
  return false;
}
