{
  if (parent != null) {
switch (parent.getType()) {
case Token.DO:
case Token.FOR:
case Token.FOR_OF:
case Token.TRY:
case Token.WHILE:
case Token.WITH:
case Token.CLASS:
      return true;
case Token.AND:
case Token.HOOK:
case Token.IF:
case Token.OR:
    return n != parent.getFirstChild();
}
}
return n.isCase();
}
