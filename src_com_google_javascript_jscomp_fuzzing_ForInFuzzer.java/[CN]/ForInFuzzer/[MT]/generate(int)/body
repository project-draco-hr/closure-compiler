{
  scopeManager.localScope().loopNesting++;
  AbstractFuzzer[] fuzzers={new ForInItemFuzzer(random,scopeManager,config,snGenerator),new ExpressionFuzzer(random,scopeManager,config,snGenerator),new BlockFuzzer(random,scopeManager,config,snGenerator)};
  Node[] components=distribute(budget - 1,fuzzers);
  scopeManager.localScope().loopNesting--;
  return new Node(Token.FOR,components);
}
