{
  fold("if (x) { a,b,c } else { d,c,e }","x ? (a,b,c) : (d,c,e)");
  fold("if (x) { a() && b() } else { c() && d () }","x ? a() && b() : c() && d () ");
  fold("if (x) { (a(),a()) && b() } else { d() }","x ? (a(),a()) && b() : d () ");
  fold("if (x) { (a(),a()) && b() } else { (c(),c()) && d () }","x ? (a(),a()) && b() : (c(),c()) && d () ");
}
