{
  test("a || (b || c); a * (b * c); a | (b | c)","(a || b) || c; (a * b) * c; (a | b) | c");
  testSame("a % (b % c); a / (b / c); a - (b - c);");
  test("a * (b % c);","b % c * a");
  test("(a * b) * (c / d)","c / d * (a * b)");
  test("c / d * (a * b)","c / d * a * b");
  test("(a + b) * (c % d)","c % d * (a + b)");
  testSame("(a / b) * (c % d)");
  testSame("(c = 5) * (c % d)");
}
