{
  fold("if(x)if(y){while(1){}}","if(x&&y){while(1){}}");
  fold("if(x||z)if(y){while(1){}}","if((x||z)&&y){while(1){}}");
  fold("if(x)if(y||z){while(1){}}","if((x)&&(y||z)){while(1){}}");
  foldSame("if(x||z)if(y||z){while(1){}}");
  fold("if(x)if(y){if(z){while(1){}}}","if(x&&y&&z){while(1){}}");
}
