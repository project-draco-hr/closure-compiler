{
  fold("function(){if(x)return 1;else return 2}","function(){return x?1:2}");
  fold("function(){if(x)return 1+x;else return 2-x}","function(){return x?1+x:2-x}");
  fold("function(){if(x)return y += 1;else return y += 2}","function(){return x?(y+=1):(y+=2)}");
  foldSame("function(){if(x)return;else return 2-x}");
  foldSame("function(){if(x)return x;else return}");
}
