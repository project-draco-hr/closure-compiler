{
  fold("while(!!true) foo()","while(1) foo()");
  fold("while(!!x) foo()","while(x) foo()");
  fold("while(!(!x&&!y)) foo()","while(x||y) foo()");
  fold("while(x||!!y) foo()","while(x||y) foo()");
  fold("while(!(!!x&&y)) foo()","while(!(x&&y)) foo()");
  fold("while(x&&!0) foo()","while(x) foo()");
  fold("while(x||!1) foo()","while(x) foo()");
}
