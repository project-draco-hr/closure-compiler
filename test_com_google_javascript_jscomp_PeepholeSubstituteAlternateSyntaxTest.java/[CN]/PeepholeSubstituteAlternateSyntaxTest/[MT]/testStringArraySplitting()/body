{
  testSame("var x=['1','2','3','4']");
  testSame("var x=['1','2','3','4','5']");
  test("var x=['1','2','3','4','5','6']","var x='1,2,3,4,5,6'.split(',')");
  test("var x=['1','2','3','4','5','6','7']","var x='1,2,3,4,5,6,7'.split(',')");
  test("var x=[',',',',',',',',',',',']","var x=', , , , , ,'.split(' ')");
  test("var x=[',',' ',',',',',',',',']","var x=',; ;,;,;,;,'.split(';')");
  test("var x=[',',' ',',',',',',',',']","var x=',; ;,;,;,;,'.split(';')");
}
