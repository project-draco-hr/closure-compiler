{
  Node nameNode=null;
  if (node.isFunction()) {
    if (parent.isName()) {
      nameNode=parent;
    }
 else     if (parent.isAssign()) {
      nameNode=parent.getFirstChild();
    }
 else {
      nameNode=node.getFirstChild();
    }
  }
 else   if (node.isCall() || node.isNew()) {
    nameNode=node.getFirstChild();
  }
  if (nameNode != null) {
    JSType type=node.getJSType();
    typeInformation.add(Joiner.on("").join(Token.name(node.getType())," ",getNameString(nameNode)," = ",(type != null) ? type.toString() : "null"));
  }
  if (node.isGetProp()) {
    Node child=node.getFirstChild();
    if (child.isName() && child.getString().endsWith("$self")) {
      JSType type=child.getJSType();
      typeInformation.add(Joiner.on("").join(Token.name(child.getType())," ",child.getString()," = ",(type != null) ? type.toString() : "null"));
    }
  }
}
