{
  test("var ACONST = 4;var b = ACONST;","var ACONST$$constant = 4; var b = ACONST$$constant;");
  test("var a, ACONST = 4;var b = ACONST;","var a; var ACONST$$constant = 4; var b = ACONST$$constant;");
  test("var ACONST; ACONST = 4; var b = ACONST;","var ACONST$$constant; ACONST$$constant = 4;" + "var b = ACONST$$constant;");
  test("var ACONST = new Foo(); var b = ACONST;","var ACONST$$constant = new Foo(); var b = ACONST$$constant;");
  test("/** @const */var aa; aa=1;","var aa$$constant;aa$$constant=1");
}
