{
  test("x |= 1;","x = x | 1;");
  test("x ^= 1;","x = x ^ 1;");
  test("x &= 1;","x = x & 1;");
  test("x <<= 1;","x = x << 1;");
  test("x >>= 1;","x = x >> 1;");
  test("x >>>= 1;","x = x >>> 1;");
  test("x += 1;","x = x + 1;");
  test("x -= 1;","x = x - 1;");
  test("x *= 1;","x = x * 1;");
  test("x /= 1;","x = x / 1;");
  test("x %= 1;","x = x % 1;");
  test("/** @suppress {const} */ x += 1;","/** @suppress {const} */ x = x + 1;");
}
