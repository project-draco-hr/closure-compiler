{
  List<GraphvizEdge> edgeList=Lists.newArrayList();
  for (  LinkedUndirectedGraphNode<N,E> node : nodes.values()) {
    for (    UndiGraphEdge<N,E> edge : node.getNeighborEdges()) {
      if (edge.getNodeA() == node) {
        edgeList.add((GraphvizEdge)edge);
      }
    }
  }
  return edgeList;
}
