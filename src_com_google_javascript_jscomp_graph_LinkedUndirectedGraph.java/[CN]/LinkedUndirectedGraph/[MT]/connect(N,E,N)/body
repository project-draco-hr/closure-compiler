{
  UndiGraphNode<N,E> node=getUndirectedGraphNode(srcValue);
  if (node == null) {
    throw new IllegalArgumentException(srcValue + " does not exist in graph");
  }
  LinkedUndirectedGraphNode<N,E> src=(LinkedUndirectedGraphNode<N,E>)node;
  node=getUndirectedGraphNode(destValue);
  if (node == null) {
    throw new IllegalArgumentException(destValue + " does not exist in graph");
  }
  LinkedUndirectedGraphNode<N,E> dest=(LinkedUndirectedGraphNode<N,E>)node;
  LinkedUndirectedGraphEdge<N,E> edge=new LinkedUndirectedGraphEdge<N,E>(src,edgeValue,dest);
  src.getNeighborEdges().add(edge);
  dest.getNeighborEdges().add(edge);
  return edge;
}
