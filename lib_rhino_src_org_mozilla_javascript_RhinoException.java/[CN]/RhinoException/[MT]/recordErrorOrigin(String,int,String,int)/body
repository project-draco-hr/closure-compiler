{
  if (lineNumber == -1) {
    lineNumber=0;
  }
  if (sourceName != null) {
    initSourceName(sourceName);
  }
  if (lineNumber != 0) {
    initLineNumber(lineNumber);
  }
  if (lineSource != null) {
    initLineSource(lineSource);
  }
  if (columnNumber != 0) {
    initColumnNumber(columnNumber);
  }
}
