{
  Node parent=getParentNode();
  int pType=parent.getType();
  if (pType == Token.FUNCTION) {
    return parent;
  }
 else   if (pType == Token.ASSIGN) {
    return parent.getLastChild();
  }
 else   if (pType == Token.VAR) {
    return nameNode.getFirstChild();
  }
 else {
    return null;
  }
}
