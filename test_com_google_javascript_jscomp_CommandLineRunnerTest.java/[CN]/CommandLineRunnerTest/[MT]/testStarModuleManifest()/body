{
  useModules=ModulePattern.STAR;
  testSame(new String[]{"var x = 3;","var y = 5;","var z = 7;","var a = 9;"});
  StringBuilder builder=new StringBuilder();
  lastCommandLineRunner.printModuleGraphManifestTo(lastCompiler.getModuleGraph(),builder);
  assertEquals("{m0}\n" + "i0\n" + "\n"+ "{m1:m0}\n"+ "i1\n"+ "\n"+ "{m2:m0}\n"+ "i2\n"+ "\n"+ "{m3:m0}\n"+ "i3\n",builder.toString());
}
