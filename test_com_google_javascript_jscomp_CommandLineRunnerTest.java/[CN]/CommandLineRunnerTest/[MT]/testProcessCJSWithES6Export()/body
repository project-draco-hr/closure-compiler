{
  args.add("--process_common_js_modules");
  args.add("--entry_point=app");
  args.add("--dependency_mode=STRICT");
  args.add("--language_in=ECMASCRIPT6");
  setFilename(0,"foo.js");
  setFilename(1,"app.js");
  test(new String[]{"export default class Foo {" + "  bar() { console.log('bar'); }" + "}","var FooBar = require('./foo');" + "var baz = new FooBar.default();" + "console.log(baz.bar());"},new String[]{"var module$foo={}," + "Foo$$module$foo=function(){};" + "Foo$$module$foo.prototype.bar=function(){console.log(\"bar\")};"+ "module$foo.default=Foo$$module$foo;","var module$app={}," + "FooBar$$module$app=module$foo," + "baz$$module$app=new FooBar$$module$app.default();"+ "console.log(baz$$module$app.bar());"});
}
