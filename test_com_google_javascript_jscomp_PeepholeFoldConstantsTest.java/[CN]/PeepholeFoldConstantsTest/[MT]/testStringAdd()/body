{
  fold("x = 'a' + \"bc\"","x = \"abc\"");
  fold("x = 'a' + 5","x = \"a5\"");
  fold("x = 5 + 'a'","x = \"5a\"");
  fold("x = 'a' + ''","x = \"a\"");
  fold("x = \"a\" + foo()","x = \"a\"+foo()");
  fold("x = foo() + 'a' + 'b'","x = foo()+\"ab\"");
  fold("x = (foo() + 'a') + 'b'","x = foo()+\"ab\"");
  fold("x = foo() + 'a' + 'b' + 'cd' + bar()","x = foo()+\"abcd\"+bar()");
  fold("x = foo() + 2 + 'b'","x = foo()+2+\"b\"");
  fold("x = foo() + 'a' + 2","x = foo()+\"a2\"");
  fold("x = '' + null","x = \"null\"");
  fold("x = true + '' + false","x = \"truefalse\"");
  fold("x = '' + []","x = \"\"+[]");
}
