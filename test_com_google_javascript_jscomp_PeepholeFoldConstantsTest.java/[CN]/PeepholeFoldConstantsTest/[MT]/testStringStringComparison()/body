{
  fold("'a' < 'b'","true");
  fold("'a' <= 'b'","true");
  fold("'a' > 'b'","false");
  fold("'a' >= 'b'","false");
  fold("+'a' < +'b'","false");
  foldSame("typeof a < 'a'");
  foldSame("'a' >= typeof a");
  fold("typeof a < typeof a","false");
  fold("typeof a >= typeof a","true");
  fold("typeof 3 > typeof 4","false");
  fold("typeof function() {} < typeof function() {}","false");
  fold("'a' == 'a'","true");
  fold("'b' != 'a'","true");
  foldSame("'undefined' == typeof a");
  foldSame("typeof a != 'number'");
  foldSame("'undefined' == typeof a");
  foldSame("'undefined' == typeof a");
  fold("typeof a == typeof a","true");
  fold("'a' === 'a'","true");
  fold("'b' !== 'a'","true");
  fold("typeof a === typeof a","true");
  fold("typeof a !== typeof a","false");
  foldSame("'' + x <= '' + y");
  foldSame("'' + x != '' + y");
  foldSame("'' + x === '' + y");
  foldSame("'' + x <= '' + x");
  foldSame("'' + x != '' + x");
  foldSame("'' + x === '' + x");
}
