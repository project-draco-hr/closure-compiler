{
  fold("x * Infinity * 2","x * Infinity");
  foldSame("x - Infinity - 2");
  foldSame("x - 1 + Infinity");
  foldSame("x - 2 + 1");
  foldSame("x - 2 + 3");
  foldSame("1 + x - 2 + 1");
  foldSame("1 + x - 2 + 3");
  foldSame("1 + x - 2 + 3 - 1");
  foldSame("f(x)-0");
  foldSame("x-0-0");
  foldSame("x+2-2+2");
  foldSame("x+2-2+2-2");
  foldSame("x-2+2");
  foldSame("x-2+2-2");
  foldSame("x-2+2-2+2");
  foldSame("1+x-0-NaN");
  foldSame("1+f(x)-0-NaN");
  foldSame("1+x-0+NaN");
  foldSame("1+f(x)-0+NaN");
  foldSame("1+x+NaN");
  foldSame("x+2-2");
  foldSame("x+2");
  foldSame("x-2");
}
