{
  fold("x = y & 1 & 1","x = y & 1");
  fold("x = y & 1 & 2","x = y & 0");
  fold("x = y & 3 & 1","x = y & 1");
  fold("x = 3 & y & 1","x = y & 1");
  fold("x = y & 3 & 3","x = y & 3");
  fold("x = 3 & y & 3","x = y & 3");
  fold("x = y | 1 | 1","x = y | 1");
  fold("x = y | 1 | 2","x = y | 3");
  fold("x = y | 3 | 1","x = y | 3");
  fold("x = 3 | y | 1","x = y | 3");
  fold("x = y | 3 | 3","x = y | 3");
  fold("x = 3 | y | 3","x = y | 3");
}
