{
  doTest("myScript_js","myScript.js");
  doTest("foo","foo");
  doTest("c","");
  doTest("_1","1");
  doTest("_","_");
  doTest("unnamed_script",null);
  doTest("some_dir_some_foo_js","some/dir/some/foo.js");
  doTest("some_dir_some_foo_js","some\\dir\\some\\foo.js");
  doTest("_12_foo_34_js","12 foo 34.js");
}
