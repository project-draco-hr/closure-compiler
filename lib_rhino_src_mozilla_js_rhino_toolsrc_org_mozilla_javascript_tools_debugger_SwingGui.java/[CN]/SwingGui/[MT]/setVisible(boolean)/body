{
  super.setVisible(b);
  if (b) {
    console.consoleTextArea.requestFocus();
    context.split.setDividerLocation(0.5);
    try {
      console.setMaximum(true);
      console.setSelected(true);
      console.show();
      console.consoleTextArea.requestFocus();
    }
 catch (    Exception exc) {
    }
  }
}
