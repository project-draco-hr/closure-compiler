{
  testSame("function A() { /** @type {?} JSDoc comment. */ this.foo; }");
  testSame("function A() { /** @private */ this.foo; }");
  testSame("/** @param {string} x */ SomeInterface.prototype.foo;");
  testWarning("function A() { /* Normal comment */ this.foo; }","function A() { /* Normal comment */ JSCOMPILER_PRESERVE(this.foo); }",e);
  testWarning("function A() { /** JSDoc with no declaration. */ this.foo; }","function A() { JSCOMPILER_PRESERVE(/** JSDoc with no declaration. */ this.foo); }",e);
}
