{
  fold("while(!false) foo()","while(1) foo()");
  fold("while(202) foo()","while(1) foo()");
  fold("while(Infinity) foo()","while(1) foo()");
  fold("while('text') foo()","while(1) foo()");
  fold("while([]) foo()","while(1) foo()");
  fold("while({}) foo()","while(1) foo()");
  fold("while(/./) foo()","while(1) foo()");
}
