{
  test("var ABC=2; var x = ABC;","var x=2");
  test("var AA = 'aa'; AA;","'aa'");
  test("var A_A=10; A_A + A_A;","10+10");
  testSame("var AA; AA=1");
  testSame("var x = AA;");
  testSame("var AA = '1234567890'; foo(AA); foo(AA); foo(AA);");
  test("var AA = '123456789012345';AA;","'123456789012345'");
}
