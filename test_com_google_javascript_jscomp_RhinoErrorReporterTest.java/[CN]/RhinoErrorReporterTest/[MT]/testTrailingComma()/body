{
  String message="Parse error. Internet Explorer has a non-standard " + "intepretation of trailing commas. Arrays will have the wrong " + "length and objects will not parse at all.";
  assertError("var x = [1,];",RhinoErrorReporter.TRAILING_COMMA,message);
  assertError("var x = {1: 2,};",RhinoErrorReporter.TRAILING_COMMA,message);
}
