{
  testConversion("!x");
  testConversion("!null");
  testConversion("!3.14");
  testConversion("!true");
  testConversion("~x");
  testConversion("~null");
  testConversion("~3.14");
  testConversion("~true");
  testConversion("+x");
  testConversion("+null");
  testConversion("+3.14");
  testConversion("+true");
  testConversion("-x");
  testConversion("-null");
  testConversion("-true");
  testConversion("!~+-z");
  testConversion("void x");
  testConversion("void null");
  testConversion("void void !x");
  testConversion("void (x + 1)");
}
