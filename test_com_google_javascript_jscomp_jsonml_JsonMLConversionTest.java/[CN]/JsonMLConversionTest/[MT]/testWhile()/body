{
  testConversion("while (true) {;}");
  testConversion("while (true) {f()}");
  testConversion("while (f(x, y)) {break;}");
  testConversion("while (f(f(f(x, y)))) {}");
  testConversion("while ((f(f(f(x, y))))()) {}");
  testConversion("while (x--) {2 + 3; q = 2 + 3; var v = y * z; " + "g = function(a) {true; var b = a + 1; return a * a}}");
}
