{
  testConversion("do {} while (true)");
  testConversion("do {;} while (true)");
  testConversion("do {} while (f(x, y))");
  testConversion("do {} while (f(f(f(x, y))))");
  testConversion("do {} while ((f(f(f(x, y))))())");
  testConversion("do {2 + 3; q = 2 + 3; var v = y * z;" + "g = function(a) {true; var b = a + 1; return a * a}} while (--x)");
}
