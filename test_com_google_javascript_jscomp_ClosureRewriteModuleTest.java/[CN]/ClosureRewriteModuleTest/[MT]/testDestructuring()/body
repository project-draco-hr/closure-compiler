{
  testEs6(LINE_JOINER.join("goog.module('ns.a');","var {foo, bar} = goog.require('ns.b');"),LINE_JOINER.join("goog.provide('ns.a');","goog.require('ns.b');","goog.scope(function(){","  var {foo, bar} = ns.b;","});"));
  testEs6(LINE_JOINER.join("goog.module('ns.a');","const {foo, bar} = goog.require('ns.b');"),LINE_JOINER.join("goog.provide('ns.a');","goog.require('ns.b');","goog.scope(function(){","  const {foo, bar} = ns.b;","});"));
}
