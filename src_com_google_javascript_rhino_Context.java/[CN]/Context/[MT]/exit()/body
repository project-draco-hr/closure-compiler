{
  boolean released=false;
  Context cx=getCurrentContext();
  if (cx == null) {
    throw new RuntimeException("Calling Context.exit without previous Context.enter");
  }
  if (cx.enterCount < 1)   Kit.codeBug();
  --cx.enterCount;
  if (cx.enterCount == 0) {
    released=true;
    setThreadContext(null);
  }
}
