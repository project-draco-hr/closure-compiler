{
  out.append("[");
  for (int j=0; j <= length; ++j) {
    if (j > 0) {
      out.append(",");
    }
    LineCharMapping current=charToMap.get(j);
    if (current == null) {
      out.append("-1");
    }
 else {
      out.append(String.valueOf(current.basisMapping.id));
    }
  }
  out.append("]");
}
