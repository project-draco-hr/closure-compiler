{
  out.append("[");
  out.append(escapeString(sourceFile));
  out.append(",");
  out.append(originalPosition.getLineNumber() + "");
  out.append(",");
  out.append(originalPosition.getCharacterIndex() + "");
  if (originalName != null) {
    out.append(",");
    out.append(escapeString(originalName));
  }
  out.append("]");
}
