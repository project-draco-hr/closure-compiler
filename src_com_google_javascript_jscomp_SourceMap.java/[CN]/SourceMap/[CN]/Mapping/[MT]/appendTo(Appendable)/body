{
  out.append("[");
  out.append("\"");
  out.append(sourceFile);
  out.append("\"");
  out.append(",");
  out.append(originalPosition.getLineNumber() + "");
  out.append(",");
  out.append(originalPosition.getCharacterIndex() + "");
  if (originalName != null) {
    out.append(",");
    out.append("\"");
    out.append(originalName);
    out.append("\"");
  }
  out.append("]");
}
