{
  int id=(m != null) ? m.id : UNMAPPED;
  if (lastId != id) {
    lastIdString=(id == UNMAPPED) ? UNMAPPED_STRING : String.valueOf(id);
    lastId=id;
  }
  String idString=lastIdString;
  for (int i=line; i <= nextLine; i++) {
    if (i == nextLine) {
      for (int j=col; j < nextCol; j++) {
        addCharEntry(idString);
      }
      break;
    }
    closeLine();
    openLine();
    col=0;
  }
}
