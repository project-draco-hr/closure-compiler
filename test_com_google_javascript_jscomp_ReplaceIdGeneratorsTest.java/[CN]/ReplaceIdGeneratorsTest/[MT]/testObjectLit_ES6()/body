{
  setAcceptedLanguage(LanguageMode.ECMASCRIPT6);
  testError(LINE_JOINER.join("/** @idGenerator */","goog.xid = function() {};","things = goog.xid({fooX() {}})"),ReplaceIdGenerators.SHORTHAND_FUNCTION_NOT_SUPPORTED_IN_ID_GEN);
  testError(LINE_JOINER.join("/** @idGenerator */ ","goog.xid = function() {};","things = goog.xid({shorthand})"),ReplaceIdGenerators.SHORTHAND_ASSIGNMENT_NOT_SUPPORTED_IN_ID_GEN);
  testError(LINE_JOINER.join("/** @idGenerator */","goog.xid = function() {};","things = goog.xid({['fooX']: 'test'})"),ReplaceIdGenerators.COMPUTED_PROP_NOT_SUPPORTED_IN_ID_GEN);
}
