{
  Node parent=n.getParent();
  Node left=n.getFirstChild();
  Node right=left.getNext();
  left=trySimpilifyUnusedResult(left);
  if (left == null || !mayHaveSideEffects(left)) {
    n.removeChild(right);
    parent.replaceChild(n,right);
    reportCodeChange();
    return right;
  }
  return n;
}
