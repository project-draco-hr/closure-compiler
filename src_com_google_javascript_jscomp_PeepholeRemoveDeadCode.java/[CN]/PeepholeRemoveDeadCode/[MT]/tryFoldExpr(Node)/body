{
  Node result=trySimplifyUnusedResult(subtree.getFirstChild());
  if (result == null) {
    Node parent=subtree.getParent();
    if (parent.isLabel()) {
      Node replacement=IR.block().srcref(subtree);
      parent.replaceChild(subtree,replacement);
      subtree=replacement;
    }
 else {
      subtree.detachFromParent();
      subtree=null;
    }
  }
  return subtree;
}
