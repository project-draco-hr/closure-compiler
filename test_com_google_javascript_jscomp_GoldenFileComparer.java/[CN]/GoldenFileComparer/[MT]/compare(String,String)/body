{
  String[] compiledLines=compiledSource.split("\n");
  String[] goldenLines=goldenSource.split("\n");
  assert_().withFailureMessage("Num lines of compiled code do not match that of expected").that(compiledLines.length).isEqualTo(goldenLines.length);
  for (int i=0; i < compiledLines.length; i++) {
    assert_().withFailureMessage("Instrumented code does not match expected at line no: " + (i + 1) + "\nExpected:\n"+ goldenLines[i]+ "\nFound:\n"+ compiledLines[i]+ "\n").that(compiledLines[i]).isEqualTo(goldenLines[i]);
  }
  return true;
}
