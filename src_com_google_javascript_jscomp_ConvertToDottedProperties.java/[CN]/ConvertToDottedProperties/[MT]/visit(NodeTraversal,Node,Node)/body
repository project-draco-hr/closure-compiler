{
switch (n.getType()) {
case Token.GETTER_DEF:
case Token.SETTER_DEF:
case Token.STRING:
    if (NodeUtil.isObjectLitKey(n,parent) && NodeUtil.isValidPropertyName(n.getString())) {
      n.putBooleanProp(Node.QUOTED_PROP,false);
    }
  break;
case Token.GETELEM:
Node left=n.getFirstChild();
Node right=left.getNext();
if (right.isString() && NodeUtil.isValidPropertyName(right.getString())) {
n.removeChild(left);
n.removeChild(right);
parent.replaceChild(n,IR.getprop(left,right));
compiler.reportCodeChange();
}
break;
}
}
