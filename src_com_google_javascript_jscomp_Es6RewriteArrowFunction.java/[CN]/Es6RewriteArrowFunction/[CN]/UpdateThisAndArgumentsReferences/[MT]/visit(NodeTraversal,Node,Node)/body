{
  if (n.isThis()) {
    Node name=IR.name(THIS_VAR).srcref(n);
    parent.replaceChild(n,name);
    changedThis=true;
  }
 else   if (n.isName() && n.getString().equals("arguments")) {
    Node name=IR.name(ARGUMENTS_VAR).srcref(n);
    parent.replaceChild(n,name);
    changedArguments=true;
  }
}
