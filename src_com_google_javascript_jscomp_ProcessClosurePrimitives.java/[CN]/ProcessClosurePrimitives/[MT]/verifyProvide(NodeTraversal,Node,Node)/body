{
  if (!verifyLastArgumentIsString(t,methodName,arg)) {
    return false;
  }
  for (  String part : Splitter.on('.').split(arg.getString())) {
    if (!NodeUtil.isValidPropertyName(part)) {
      compiler.report(t.makeError(arg,INVALID_PROVIDE_ERROR,part));
      return false;
    }
  }
  return true;
}
