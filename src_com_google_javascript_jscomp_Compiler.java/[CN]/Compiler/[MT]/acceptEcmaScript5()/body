{
switch (options.getLanguageIn()) {
case ECMASCRIPT5:
case ECMASCRIPT5_STRICT:
case ECMASCRIPT6:
case ECMASCRIPT6_STRICT:
case ECMASCRIPT6_TYPED:
    return true;
case ECMASCRIPT3:
  return false;
default :
throw new IllegalStateException("unexpected language mode: " + options.getLanguageIn());
}
}
