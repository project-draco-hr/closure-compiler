{
  CompilerInput input=getInput(id);
  if (input == null) {
    return;
  }
  Preconditions.checkState(input.isExtern(),"Not an extern input: %s",input.getName());
  inputsById.remove(id);
  externs.remove(input);
  Node root=input.getAstRoot(this);
  if (root != null) {
    root.detachFromParent();
  }
}
