{
  Preconditions.checkArgument(budget >= 1);
  Node node=new Node(Token.BREAK);
  Scope localScope=scopeManager.localScope();
  if (localScope.loopLabels.size() + localScope.otherLabels.size() > 0 && random.nextInt(2) == 0) {
    node.addChildToBack(Node.newString(Token.LABEL_NAME,localScope.randomLabelForBreak(random)));
  }
  return node;
}
