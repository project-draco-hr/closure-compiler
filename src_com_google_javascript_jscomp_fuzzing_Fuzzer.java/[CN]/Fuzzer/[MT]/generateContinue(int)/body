{
  Preconditions.checkArgument(budget >= 1);
  Node node=new Node(Token.CONTINUE);
  Scope localScope=scopeManager.localScope();
  if (localScope.loopLabels.size() > 0 && random.nextInt(2) == 0) {
    node.addChildToBack(Node.newString(Token.LABEL_NAME,localScope.randomLabelForContinue(random)));
  }
  return node;
}
