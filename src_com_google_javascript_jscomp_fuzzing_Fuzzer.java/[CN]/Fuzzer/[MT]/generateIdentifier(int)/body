{
  Preconditions.checkArgument(budget >= 1);
  String name=null;
  if (scopeManager.hasNonLocals() && random.nextInt(10) < 1) {
    name=scopeManager.getRandomSymbol(true);
  }
  if (name == null) {
    name="x_" + nextNumber();
  }
  scopeManager.addSymbol(name);
  return Node.newString(Token.NAME,name);
}
