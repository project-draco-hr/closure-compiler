{
  String[] js=new String[]{LINE_JOINER.join("var goog = {};","/** @constructor */","goog.Foo = function() {};","goog.Foo.bar = function(){","  var first = new goog.Forgot();","  var second = new goog.Forgot();","};")};
  String warning="'goog.Forgot' used but not goog.require'd";
  test(js,js,null,MISSING_REQUIRE_WARNING,warning);
}
