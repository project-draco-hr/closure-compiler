{
  Node rhs=node.getNext();
  Node last=parent;
  for (  Node ancestor : assignAncestors) {
    if (ancestor.isExprResult()) {
      lastAncestor.removeChild(ancestor);
    }
 else {
      rhs.detachFromParent();
      ancestor.replaceChild(last,rhs);
    }
    last=ancestor;
  }
  compiler.reportCodeChange();
}
