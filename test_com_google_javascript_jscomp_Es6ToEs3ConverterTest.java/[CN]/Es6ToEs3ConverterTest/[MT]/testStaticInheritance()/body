{
  compareJsDoc=false;
  test(Joiner.on('\n').join("class D {","  static f() {}","}","class C extends D { constructor() {} }","C.f();"),Joiner.on('\n').join("/** @constructor @struct */","var D = function() {};","D.f = function () {};","/** @constructor @struct @extends{D} */","var C = function() {};","$jscomp.copyProperties(C, D);","$jscomp.inherits(C, D);","C.f();"));
  test(Joiner.on('\n').join("class D {","  static f() {}","}","class C extends D {","  constructor() {}","  f() {}","}","C.f();"),Joiner.on('\n').join("/** @constructor @struct */","var D = function() {};","D.f = function() {};","/** @constructor @struct @extends{D} */","var C = function() { };","$jscomp.copyProperties(C, D);","$jscomp.inherits(C, D);","C.prototype.f = function() {};","C.f();"));
  test(Joiner.on('\n').join("class D {","  static f() {}","}","class C extends D {","  constructor() {}","  static f() {}","  g() {}","}"),Joiner.on('\n').join("/** @constructor @struct */","var D = function() {};","D.f = function() {};","/** @constructor @struct @extends{D} */","var C = function() { };","$jscomp.copyProperties(C, D);","$jscomp.inherits(C, D);","C.f = function() {};","C.prototype.g = function() {};"));
}
