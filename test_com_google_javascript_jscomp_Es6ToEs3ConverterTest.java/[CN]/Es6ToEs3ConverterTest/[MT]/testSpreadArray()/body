{
  test("var arr = [1, 2, ...mid, 4, 5];","var arr = [1, 2].concat(mid, [4, 5]);");
  test("var arr = [1, 2, ...mid(), 4, 5];","var arr = [1, 2].concat(mid(), [4, 5]);");
  test("var arr = [1, 2, ...mid, ...mid2(), 4, 5];","var arr = [1, 2].concat(mid, mid2(), [4, 5]);");
  test("var arr = [...mid()];","var arr = mid().slice(0);");
  test("f(1, [2, ...mid, 4], 5);","f(1, [2].concat(mid, [4]), 5);");
}
