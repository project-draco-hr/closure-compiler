{
  test("var arr = [1, 2, ...mid, 4, 5];","var arr = [].concat([1, 2], mid, [4, 5]);");
  test("var arr = [1, 2, ...mid(), 4, 5];","var arr = [].concat([1, 2], mid(), [4, 5]);");
  test("var arr = [1, 2, ...mid, ...mid2(), 4, 5];","var arr = [].concat([1, 2], mid, mid2(), [4, 5]);");
  test("var arr = [...mid()];","var arr = [].concat(mid());");
  test("f(1, [2, ...mid, 4], 5);","f(1, [].concat([2], mid, [4]), 5);");
  test("function f() { return [...arguments]; };","function f() { return [].concat(arguments); };");
  test("function f() { return [...arguments, 2]; };","function f() { return [].concat(arguments, [2]); };");
}
