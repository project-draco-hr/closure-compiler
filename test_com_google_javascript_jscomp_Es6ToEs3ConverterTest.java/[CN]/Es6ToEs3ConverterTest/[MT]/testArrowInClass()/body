{
  test(LINE_JOINER.join("class C {","  constructor() {","    this.counter = 0;","  }","","  init() {","    document.onclick = () => this.logClick();","  }","","  logClick() {","     this.counter++;","  }","}"),LINE_JOINER.join("/**"," * @constructor"," * @struct"," */","var C = function() { this.counter = 0; };","","C.prototype.init = function() {","  /** @const */ var $jscomp$this = this;","  document.onclick = function() { return $jscomp$this.logClick(); }","};","","C.prototype.logClick = function() {","  this.counter++;","}"));
