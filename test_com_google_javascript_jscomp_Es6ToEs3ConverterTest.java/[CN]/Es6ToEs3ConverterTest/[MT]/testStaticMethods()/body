{
  test(Joiner.on('\n').join("class C {","  static foo() {}","}"),Joiner.on('\n').join("/** @constructor @struct */","var $jscomp$unique$class$C = function() {};","/** @constructor @struct */","var C = $jscomp$unique$class$C;","$jscomp$unique$class$C.foo = function() {};"));
  test(Joiner.on('\n').join("class C {","  static foo() {}","","  foo() {}","}"),Joiner.on('\n').join("/** @constructor @struct */","var $jscomp$unique$class$C = function() {};","/** @constructor @struct */","var C = $jscomp$unique$class$C;","","$jscomp$unique$class$C.foo = function() {};","","$jscomp$unique$class$C.prototype.foo = function() {};"));
  test(Joiner.on('\n').join("class C {","  static foo() {}","","  bar() { C.foo(); }","}"),Joiner.on('\n').join("/** @constructor @struct */","var $jscomp$unique$class$C = function() {};","/** @constructor @struct */","var C = $jscomp$unique$class$C;","","$jscomp$unique$class$C.foo = function() {};","","$jscomp$unique$class$C.prototype.bar = function() { C.foo(); };"));
}
