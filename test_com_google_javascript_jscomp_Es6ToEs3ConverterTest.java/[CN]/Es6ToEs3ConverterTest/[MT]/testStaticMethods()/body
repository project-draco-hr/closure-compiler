{
  test(Joiner.on('\n').join("class C {","  static foo() {}","}"),Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","C.foo = function() {};"));
  test(Joiner.on('\n').join("class C {","  static foo() {}","","  foo() {}","}"),Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","","C.foo = function() {};","","C.prototype.foo = function() {};"));
  test(Joiner.on('\n').join("class C {","  static foo() {}","","  bar() { C.foo(); }","}"),Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","","C.foo = function() {};","","C.prototype.bar = function() { C.foo(); };"));
}
