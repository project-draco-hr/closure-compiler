{
  test("var C = class { }","/** @constructor @struct */ var C = function() {}");
  test("var C = class { foo() {} }",LINE_JOINER.join("/** @constructor @struct */ var C = function() {}","","C.prototype.foo = function() {}"));
  test("var C = class C { }","/** @constructor @struct */ var C = function() {}");
  test("var C = class C { foo() {} }",LINE_JOINER.join("/** @constructor @struct */ var C = function() {}","","C.prototype.foo = function() {};"));
}
