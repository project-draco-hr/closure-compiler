{
  test(Joiner.on('\n').join("if (foo) {","  class C { }","}"),Joiner.on('\n').join("if (foo) {","  /** @constructor @struct */","  var $jscomp$unique$class$C = function() {}","  /** @constructor @struct */","  var C = $jscomp$unique$class$C;","}"));
  test(Joiner.on('\n').join("if (foo)","  class C { }"),Joiner.on('\n').join("if (foo) {","  /** @constructor @struct */","  var $jscomp$unique$class$C = function() {}","  /** @constructor @struct */","  var C = $jscomp$unique$class$C;","}"));
}
