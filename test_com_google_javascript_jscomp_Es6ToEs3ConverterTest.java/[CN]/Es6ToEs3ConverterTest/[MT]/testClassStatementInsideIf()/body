{
  test(Joiner.on('\n').join("if (foo) {","  class C { }","}"),Joiner.on('\n').join("if (foo) {","  /** @constructor @struct */","  var C = function() {};","}"));
  test(Joiner.on('\n').join("if (foo)","  class C { }"),Joiner.on('\n').join("if (foo) {","  /** @constructor @struct */","  var C = function() {};","}"));
}
