{
  test(Joiner.on('\n').join("if (foo) {","  class C { }","}"),Joiner.on('\n').join("if (foo) {","  /** @constructor @struct */","  function C() { }","}"));
  test(Joiner.on('\n').join("if (foo)","  class C { }"),Joiner.on('\n').join("if (foo)","  /** @constructor @struct */","  function C() { }"));
}
