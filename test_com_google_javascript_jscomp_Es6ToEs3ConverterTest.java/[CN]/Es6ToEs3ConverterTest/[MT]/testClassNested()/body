{
  test("class C { f() { class D {} } }",Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct */","  var D = function() {}","};"));
  compareJsDoc=false;
  test("class C { f() { class D extends C {} } }",Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct @extends{C} */","  var D = function(var_args) { C.apply(this, arguments); };","  $jscomp.copyProperties(D, C);","  $jscomp.inherits(D, C);","};"));
}
