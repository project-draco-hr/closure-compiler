{
  test("class C { f() { class D {} } }",Joiner.on('\n').join("/** @constructor @struct */","function C() {}","C.prototype.f = function() {","  /** @constructor @struct */ function D() {}","};"));
  test("class C { f() { class D extends C {} } }",Joiner.on('\n').join("/** @constructor @struct */","function C() {}","C.prototype.f = function() {","  /** @constructor @struct @extends{C} */ function D() {}","  goog.inherits(D, C);","};"));
}
