{
  test("class C { f() { class D {} } }",Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct */","  var D = function() {}","};"));
  test("class C { f() { class D extends C {} } }",Joiner.on('\n').join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct @extends{C} */","  var D = function() {}","  $jscomp$inherits(D, C);","  $jscomp$copy$properties(D, C);","};"));
}
