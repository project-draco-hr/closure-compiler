{
  test("class C { f() { class D {} } }",LINE_JOINER.join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct */","  var D = function() {}","};"));
  test("class C { f() { class D extends C {} } }",LINE_JOINER.join("/** @constructor @struct */","var C = function() {};","C.prototype.f = function() {","  /** @constructor @struct @extends{C} */","  var D = function(var_args) { C.apply(this, arguments); };","  $jscomp.inherits(D, C);","};"));
}
