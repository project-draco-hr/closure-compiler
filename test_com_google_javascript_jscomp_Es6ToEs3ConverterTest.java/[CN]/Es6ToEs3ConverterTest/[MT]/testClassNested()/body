{
  test("class C { f() { class D {} } }",Joiner.on('\n').join("/** @constructor @struct */","var $jscomp$unique$class$C = function() {};","/** @constructor @struct */","var C = $jscomp$unique$class$C;","$jscomp$unique$class$C.prototype.f = function() {","  /** @constructor @struct */","  var D = function D() {}","};"));
  test("class C { f() { class D extends C {} } }",Joiner.on('\n').join("/** @constructor @struct */","var $jscomp$unique$class$C = function() {};","/** @constructor @struct */","var C = $jscomp$unique$class$C;","$jscomp$unique$class$C.prototype.f = function() {","  /** @constructor @struct @extends{C} */","  var D = function D() {}","  $jscomp$inherits(D, C);","  $jscomp$copy$properties(D, C);","};"));
}
