{
  test(Joiner.on('\n').join("class D {}","class C extends D {","  constructor(args) {","    super(...args)","  }","}"),Joiner.on('\n').join("/** @constructor @struct */","var D = function(){};","/** @constructor @struct @extends {D} */","var C=function(args) {","  C.base.apply(C, [].concat([this, 'constructor'], args))","};","$jscomp.copyProperties(C,D);","$jscomp.inherits(C,D);"));
}
