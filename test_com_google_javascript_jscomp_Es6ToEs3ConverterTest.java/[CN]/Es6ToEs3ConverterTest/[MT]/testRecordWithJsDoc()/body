{
  test(LINE_JOINER.join("/**"," * @record"," */","class Converter {","  /**","   * @param {X} x","   * @return {Y}","   */","  convert(x) {}","}"),LINE_JOINER.join("/**"," * @struct @record"," */","var Converter = function() { };","","/**"," * @param {X} x"," * @return {Y}"," */","Converter.prototype.convert = function(x) {};"));
}
