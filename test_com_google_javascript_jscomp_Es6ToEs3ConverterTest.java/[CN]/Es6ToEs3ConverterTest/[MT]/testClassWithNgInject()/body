{
  test(Joiner.on('\n').join("class A {","  /** @ngInject */","  constructor($scope) {}","}"),Joiner.on('\n').join("/** @constructor @struct @ngInject */","var A = function($scope) {}"));
  test(Joiner.on('\n').join("/** @ngInject */","class A {","  constructor($scope) {}","}"),Joiner.on('\n').join("/** @constructor @struct @ngInject */","var A = function($scope) {}"));
}
