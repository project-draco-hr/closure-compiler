{
  test("function f(...zero) {}",Joiner.on('\n').join("function f(zero) {","  zero = [].slice.apply(arguments, 0);","}"));
  test("function f(zero, ...one) {}",Joiner.on('\n').join("function f(zero, one) {","  one = [].slice.apply(arguments, 1);","}"));
  test("function f(zero, one, ...two) {}",Joiner.on('\n').join("function f(zero, one, two) {","  two = [].slice.apply(arguments, 2);","}"));
}
