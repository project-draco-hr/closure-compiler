{
  test(Joiner.on('\n').join("/**"," * Converts Xs to Ys."," * @interface"," */","class Converter {","  /**","   * @param {X} x","   * @return {Y}","   */","  convert(x) {}","}"),Joiner.on('\n').join("/**"," * Converts Xs to Ys."," * @interface"," */","function Converter() {}","","/**"," * @param {X} x"," * @return {Y}"," */","Converter.prototype.convert = function(x) {};"));
}
