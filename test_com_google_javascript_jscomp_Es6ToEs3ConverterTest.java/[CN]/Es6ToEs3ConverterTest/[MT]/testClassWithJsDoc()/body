{
  test("class C { }",Joiner.on('\n').join("/**"," * @constructor"," * @struct"," */","function C() {}"));
  test("/** @deprecated */ class C { }",Joiner.on('\n').join("/**"," * @deprecated"," * @constructor"," * @struct"," */","function C() {}"));
  test("/** @dict */ class C { }",Joiner.on('\n').join("/**"," * @constructor"," * @dict"," */","function C() {}"));
}
