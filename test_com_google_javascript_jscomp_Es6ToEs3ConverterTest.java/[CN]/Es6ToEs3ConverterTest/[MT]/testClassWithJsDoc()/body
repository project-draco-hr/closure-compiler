{
  test("class C { }",Joiner.on('\n').join("var $jscomp$unique$class$C = function() {}","/** @constructor @struct */","var C = $jscomp$unique$class$C;"));
  test("/** @deprecated */ class C { }",Joiner.on('\n').join("var $jscomp$unique$class$C = function() {}","/** @constructor @struct @deprecated */","var C = $jscomp$unique$class$C;"));
  test("/** @dict */ class C { }",Joiner.on('\n').join("var $jscomp$unique$class$C = function() {}","/** @constructor @dict */","var C = $jscomp$unique$class$C;"));
}
