{
  test("class C { }",Joiner.on('\n').join("/** @constructor @struct */","var C = function() { };"));
  test("/** @deprecated */ class C { }",Joiner.on('\n').join("/** @constructor @struct @deprecated */","var C = function() {};"));
  test("/** @dict */ class C { }",Joiner.on('\n').join("/** @constructor @dict */","var C = function() {};"));
}
