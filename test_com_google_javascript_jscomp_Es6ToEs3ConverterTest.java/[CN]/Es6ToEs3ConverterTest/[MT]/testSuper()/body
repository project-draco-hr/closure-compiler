{
  enableAstValidation(false);
  test("class C { constructor() { super(); } }","error, super call not valid w/o class",Es6ToEs3Converter.CANNOT_CONVERT_YET);
  test("class C extends D { constructor() { super(); } }","function C() { D.call(this); }",Es6ToEs3Converter.CANNOT_CONVERT_YET);
  test("class C extends D { constructor(str) { super(str); } }","function C() { D(str); }",Es6ToEs3Converter.CANNOT_CONVERT_YET);
  test(Joiner.on('\n').join("class C extends D {","  foo() { return super.foo(); }","}"),"function C() {} C.prototype.foo = function() {return D.foo.call(this);}",Es6ToEs3Converter.CANNOT_CONVERT_YET);
}
