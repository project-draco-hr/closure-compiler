{
  test("tag``",Joiner.on('\n').join("var $jscomp$templatelit$0 = [''];","$jscomp$templatelit$0['raw'] = [''];","tag($jscomp$templatelit$0);"));
  test("tag`${hello} world`",Joiner.on('\n').join("var $jscomp$templatelit$0 = ['', ' world'];","$jscomp$templatelit$0['raw'] = ['', ' world'];","tag($jscomp$templatelit$0, hello);"));
  test("tag`${hello} ${world}`",Joiner.on('\n').join("var $jscomp$templatelit$0 = ['', ' ', ''];","$jscomp$templatelit$0['raw'] = ['', ' ', ''];","tag($jscomp$templatelit$0, hello, world);"));
  test("tag`\"`",Joiner.on('\n').join("var $jscomp$templatelit$0 = ['\\\"'];","$jscomp$templatelit$0['raw'] = ['\\\"'];","tag($jscomp$templatelit$0);"));
  test("tag`a\tb`",Joiner.on('\n').join("var $jscomp$templatelit$0 = ['a\tb'];","$jscomp$templatelit$0['raw'] = ['a\\tb'];","tag($jscomp$templatelit$0);"));
}
