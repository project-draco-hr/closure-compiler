{
  FlowScope blind=newScope();
  testBinop(blind,Token.ASSIGN,createVar(blind,"a",createNullableType(OBJECT_TYPE)),createVar(blind,"b",createNullableType(OBJECT_TYPE)),Sets.newHashSet(new Pair<String,JSType>("a",OBJECT_TYPE),new Pair<String,JSType>("b",OBJECT_TYPE)),Sets.newHashSet(new Pair<String,JSType>("a",NULL_TYPE),new Pair<String,JSType>("b",NULL_TYPE)));
}
