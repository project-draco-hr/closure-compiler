{
  for (  int op : Arrays.asList(Token.LT,Token.GT,Token.LE,Token.GE)) {
    FlowScope blind=newScope();
    testBinop(blind,op,createUntypedNumber(8),createVar(blind,"a",createUnionType(STRING_TYPE,VOID_TYPE)),Sets.newHashSet(new TypedName("a",STRING_TYPE)),Sets.newHashSet(new TypedName("a",createUnionType(STRING_TYPE,VOID_TYPE))));
  }
}
