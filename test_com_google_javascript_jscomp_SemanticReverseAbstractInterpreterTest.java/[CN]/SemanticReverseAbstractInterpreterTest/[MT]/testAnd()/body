{
  FlowScope blind=newScope();
  testBinop(blind,Token.AND,createVar(blind,"b",createUnionType(STRING_TYPE,NULL_TYPE)),createVar(blind,"a",createUnionType(NUMBER_TYPE,VOID_TYPE)),Sets.newHashSet(new Pair<String,JSType>("a",NUMBER_TYPE),new Pair<String,JSType>("b",STRING_TYPE)),Sets.newHashSet(new Pair<String,JSType>("a",createUnionType(NUMBER_TYPE,VOID_TYPE)),new Pair<String,JSType>("b",createUnionType(STRING_TYPE,NULL_TYPE))));
}
