{
  if (!formatted) {
    int firstDigit=chars[0] == '-' ? 1 : 0;
    int decPoint=point - firstDigit;
    if (decPoint < -5 || decPoint > 21) {
      toExponentialFormat(firstDigit,decPoint);
    }
 else {
      toFixedFormat(firstDigit,decPoint);
    }
    formatted=true;
  }
  return new String(chars,0,end);
}
