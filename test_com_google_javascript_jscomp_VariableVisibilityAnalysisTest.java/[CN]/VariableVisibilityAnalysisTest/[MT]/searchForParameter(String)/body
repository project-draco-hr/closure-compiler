{
  Preconditions.checkArgument(parameterName != null);
  final Node[] foundNode=new Node[1];
  AbstractPostOrderCallback findParameter=new AbstractPostOrderCallback(){
    @Override public void visit(    NodeTraversal t,    Node n,    Node parent){
      if (n.getParent().getType() == Token.LP && parameterName.equals(n.getString())) {
        foundNode[0]=n;
      }
    }
  }
;
  new NodeTraversal(lastCompiler,findParameter).traverse(lastCompiler.jsRoot);
  return foundNode[0];
}
