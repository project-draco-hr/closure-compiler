{
  if (NodeUtil.isName(n) && var.getName().equals(n.getString()) && parent != null) {
    if (parent.getType() == Token.LP) {
      return true;
    }
 else     if (NodeUtil.isVar(parent)) {
      return n.hasChildren();
    }
    return false;
  }
 else {
    Node name=n.getFirstChild();
    return name != null && NodeUtil.isName(name) && var.getName().equals(name.getString()) && NodeUtil.isAssignmentOp(n);
  }
}
