{
  List<SourceFile> inputs=Lists.newArrayList(SourceFile.fromCode("in1",input));
  List<SourceFile> externs=Lists.newArrayList(SourceFile.fromCode("externs1",EXTERNS));
  Compiler compiler=new Compiler();
  BlackHoleErrorManager.silence(compiler);
  compiler.compile(externs,inputs,options);
  return assertSymbolTableValid(compiler.buildKnownSymbolTable());
}
