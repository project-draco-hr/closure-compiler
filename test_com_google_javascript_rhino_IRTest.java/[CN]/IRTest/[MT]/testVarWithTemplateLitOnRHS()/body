{
  testIR(IR.var(IR.name("x"),new Node(Token.TEMPLATELIT,IR.string(""))),LINE_JOINER.join("VAR","    NAME x","        TEMPLATELIT","            STRING ",""));
  testIR(IR.var(IR.name("x"),new Node(Token.TAGGED_TEMPLATELIT,IR.name("y"),IR.string(""))),LINE_JOINER.join("VAR","    NAME x","        TAGGED_TEMPLATELIT","            NAME y","            STRING ",""));
}
