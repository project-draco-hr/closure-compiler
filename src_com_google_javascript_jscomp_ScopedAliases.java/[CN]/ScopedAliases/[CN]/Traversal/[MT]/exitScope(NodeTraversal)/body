{
  if (t.getScopeDepth() > scopeFunctionDepth) {
    findNamespaceShadows(t);
  }
  if (t.getScopeDepth() == scopeFunctionDepth) {
    renameNamespaceShadows(t);
    injectedDecls.clear();
    aliases.clear();
    forbiddenLocals.clear();
    transformation=null;
    hasNamespaceShadows=false;
  }
}
