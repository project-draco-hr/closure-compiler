{
  testSameEs6(LINE_JOINER.join("someCall(function() {","  var a = arguments","}.bind(this))"));
  testSameEs6(LINE_JOINER.join("someCall(function() {","  var a = arguments[0]","}.bind(this))"));
  testSameEs6(LINE_JOINER.join("someCall(function() {","  var a = arguments.callee","}.bind(this))"));
  testSameEs6(LINE_JOINER.join("someCall(function() {","  var a = arguments.length","}.bind(this))"));
}
