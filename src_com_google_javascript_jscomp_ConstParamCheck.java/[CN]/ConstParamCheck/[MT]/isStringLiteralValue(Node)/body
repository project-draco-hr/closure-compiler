{
  if (node == null) {
    return false;
  }
  if (node.isString()) {
    return true;
  }
 else   if (node.getToken() == Token.ADD) {
    Preconditions.checkState(node.getChildCount() == 2);
    Node left=node.getFirstChild();
    Node right=node.getLastChild();
    return isStringLiteralValue(left) && isStringLiteralValue(right);
  }
  return false;
}
