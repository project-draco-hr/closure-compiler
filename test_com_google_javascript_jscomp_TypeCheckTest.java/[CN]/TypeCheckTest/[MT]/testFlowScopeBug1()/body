{
  Node n=parseAndTypeCheck("/** @param {number} a \n" + "* @param {number} b */\n" + "function f(a, b) {\n"+ "/** @type number */"+ "var i = 0;"+ "for (; (i + a) < b; ++i) {}}");
  assertEquals(registry.getNativeType(JSTypeNative.NUMBER_TYPE),n.getFirstChild().getLastChild().getLastChild().getFirstChild().getNext().getFirstChild().getJSType());
}
