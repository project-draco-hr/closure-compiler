{
  testTypes(LINE_JOINER.join("/** @record */ function Rec() {}","/** @type {string} */ Rec.prototype.str;","/** @type {?} */ Rec.prototype.unknown;","","/** @constructor */ function Foo() {}","Foo.prototype.str = 'foo';","","var /** !Rec */ x = new Foo;"),LINE_JOINER.join("initializing variable","found   : Foo","required: Rec"));
}
