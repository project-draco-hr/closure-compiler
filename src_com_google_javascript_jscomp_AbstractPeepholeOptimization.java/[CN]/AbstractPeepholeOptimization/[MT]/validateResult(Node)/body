{
  done: {
    if (n != null && n.getType() != Token.SCRIPT && (n.getType() != Token.BLOCK || !n.isSyntheticBlock())) {
      for (      Node parent : n.getAncestors()) {
        if (parent.getType() == Token.SCRIPT) {
          break done;
        }
      }
      Preconditions.checkState(false);
    }
  }
  return n;
}
